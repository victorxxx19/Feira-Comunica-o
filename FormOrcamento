    <div class="header-container">
        <h4>
            <i class="material-icons">request_quote</i>
            Gerador de Orçamentos
        </h4>
        <img src="data:image/png;base64," alt="Logo" class="header-logo">
    </div>

    <main>
        <div class="section-card">
            <h5 class="section-title"><i class="material-icons">search</i>Buscar Produto</h5>
            <div class="row" style="margin-bottom: 0;">
                <div class="input-field col s12 m6">
                    <input id="cod_produto_busca" type="text" placeholder="Ex: FC-0001">
                    <label for="cod_produto_busca">Cód. do Produto</label>
                </div>
                <div class="col s12 m6" style="padding-top: 15px;">
                    <button id="btnBuscarProduto" class="btn waves-effect waves-light">
                        <i class="material-icons left">find_in_page</i>Buscar
                    </button>
                </div>
            </div>
            <div id="busca-erro" class="red-text" style="display:none; margin-top: 10px;"></div>
        </div>

        <div id="success-message" class="card-panel teal lighten-4" style="display:none; margin-top: 20px; text-align: center;">
            <h5 class="green-text text-darken-3">Orçamento Gerado com Sucesso!</h5>
            <a id="pdf-link" href="#" target="_blank" class="btn waves-effect waves-light">
                <i class="material-icons left">download</i>
                Baixar / Visualizar PDF
            </a>
        </div>

        <div id="dados-orcamento" style="display: none;">

            <div class="section-card produto-info">
                <div class="row" style="margin-bottom: 0;">
                    <div class="col s12 m6">
                        <span class="label">Descrição</span>
                        <p id="descricao_display" class="value">-</p>
                    </div>
                    <div class="col s12 m3">
                        <span class="label">Faca</span>
                        <p id="faca_display" class="value">-</p>
                    </div>
                    <div class="col s12 m3">
                        <span class="label">Substrato</span>
                        <p id="substrato_display" class="value">-</p>
                    </div>
                </div>
            </div>

            <div class="section-card">
                <h5 class="section-title"><i class="material-icons">person</i>Dados da Proposta</h5>
                <div class="row">
                    <div class="input-field col s12 m4">
                        <select id="vendedora">
                            <option value="" disabled selected>Carregando...</option>
                        </select>
                        <label>Vendedora</label>
                    </div>
                    <div class="input-field col s12 m5">
                        <input id="cliente_nome" type="text" disabled>
                        <label for="cliente_nome">Nome do Cliente</label>
                    </div>
                    <div class="input-field col s12 m3">
                        <input id="cliente_cnpj" type="text" disabled>
                        <label for="cliente_cnpj">CNPJ</label>
                    </div>
                </div>
            </div>

            <div class="section-card">
                <h5 class="section-title"><i class="material-icons">attach_money</i>Valores e Condições</h5>
                <div class="row">
                    <div class="input-field col s12 m3">
                        <input id="item_qtd" type="number" min="1">
                        <label for="item_qtd">Quantidade</label>
                    </div>
                    <div class="input-field col s12 m3">
                        <input id="item_preco_unit" type="text" placeholder="Ex: 0,55">
                        <label for="item_preco_unit">Preço Unitário (R$)</label>
                    </div>
                    <div class="input-field col s12 m3">
                        <input id="item_preco_total" type="text" disabled>
                        <label for="item_preco_total" class="active">Preço Total (R$)</label>
                    </div>
                    <div class="input-field col s12 m3">
                        <input id="preco_final_total" type="text" placeholder="Preço final (se diferente)">
                        <label for="preco_final_total">Valor Total Final (R$)</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12 m3">
                        <select id="cond_pagamento">
                            <option value="" disabled selected>Selecione</option>
                            <option value="À Vista">À Vista</option>
                            <option value="7 dias">7 dias</option>
                            <option value="15 dias">15 dias</option>
