                        <input id="prazo_entrega" type="text" value="15 dias úteis">
                        <label for="prazo_entrega">Prazo de Produção</label>
                    </div>
                    <div class="input-field col s12 m3">
                        <input id="validade" type="text" value="7 dias">
                        <label for="validade">Validade da Proposta</label>
                    </div>
                </div>
                <div class="row">
