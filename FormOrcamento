    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

    <script>
        // --- INICIALIZAÇÃO ---
        document.addEventListener("DOMContentLoaded", function() {
            M.FormSelect.init(document.querySelectorAll('select'));
            M.updateTextFields();

            google.script.run
                .withSuccessHandler(onVendedorasSuccess)
                .withFailureHandler(onFailure)
                .getListaDeAba("Vendedoras", 1); 

            document.getElementById('btnBuscarProduto').addEventListener('click', buscarProduto);
            document.getElementById('btnGerarOrcamento').addEventListener('click', gerarOrcamento);
            document.getElementById('item_qtd').addEventListener('input', calcularTotal);
            document.getElementById('item_preco_unit').addEventListener('input', calcularTotal);
        });

        function onVendedorasSuccess(lista) {
            const select = document.getElementById('vendedora');
            select.innerHTML = '<option value="" disabled selected>Selecione</option>'; 
            lista.forEach(nome => {
                select.innerHTML += `<option value="${nome}">${nome}</option>`;
            });
            M.FormSelect.init(select);
        }

        // --- BUSCA DO PRODUTO ---
        function buscarProduto() {
            const codigo = document.getElementById('cod_produto_busca').value;
            if (!codigo) {
                showBuscaErro("Digite um código de produto.");
                return;
            }
            showLoader(true);
            showBuscaErro(null);
            
            google.script.run
                .withSuccessHandler(onBuscarSucesso)
                .withFailureHandler(onFailure)
                .buscarProdutoPeloCodigo(codigo); 
        }

        function onBuscarSucesso(produto) {
            showLoader(false);
            if (!produto) {
                showBuscaErro("Produto não encontrado.");
                document.getElementById('dados-orcamento').style.display = 'none';
                return;
            }
            if (produto.status && (produto.status === "Bloqueado" || produto.status === "Cancelado")) {
                 showBuscaErro(`Produto ${produto.status}. Motivo: ${produto.motivo}`);
                document.getElementById('dados-orcamento').style.display = 'none';
                return;
            }

            document.getElementById('descricao_display').textContent = produto.descricao;
            document.getElementById('faca_display').textContent = produto.faca;
            document.getElementById('substrato_display').textContent = produto.substrato;
            document.getElementById('cliente_nome').value = produto.cliente;
            document.getElementById('cliente_cnpj').value = produto.clienteCNPJ || "N/A";
            document.getElementById('cod_produto_hidden').value = produto.codigo;
            document.getElementById('descricao_hidden').value = produto.descricao;
            document.getElementById('faca_hidden').value = produto.faca;
            document.getElementById('substrato_hidden').value = produto.substrato;
            document.getElementById('dados-orcamento').style.display = 'block';
            document.getElementById('btnGerarOrcamento').disabled = false;
            document.getElementById('success-message').style.display = 'none';
            M.updateTextFields();
        }
        
        // --- CÁLCULO DE TOTAL ---
        function calcularTotal() {
            const qtd = parseFloat(document.getElementById('item_qtd').value) || 0;
            const precoUnitStr = document.getElementById('item_preco_unit').value.replace('R$', '').replace('.', '').replace(',', '.').trim();
            const precoUnit = parseFloat(precoUnitStr) || 0;
            
            const total = (qtd * precoUnit);
            
            const totalFormatado = total.toLocaleString('pt-BR', { minimumFractionDigits: 2, maximumFractionDigits: 2 });
            
            const totalField = document.getElementById('item_preco_total');
            totalField.value = totalFormatado;
            M.updateTextFields(); 
        }

        // --- GERAÇÃO DO ORÇAMENTO ---
        function gerarOrcamento() {
            if (!validarCampos()) return;
            
            showLoader(true);
            document.getElementById('btnGerarOrcamento').disabled = true;
            document.getElementById('success-message').style.display = 'none';
            
            const precoTotalAuto = document.getElementById('item_preco_total').value;
            const precoFinalManual = document.getElementById('preco_final_total').value;

            const dadosOrcamento = {
                vendedora: document.getElementById('vendedora').value,
                clienteNome: document.getElementById('cliente_nome').value,
                clienteCNPJ: document.getElementById('cliente_cnpj').value, 
                itemQtd: document.getElementById('item_qtd').value,
                itemPrecoUnit: document.getElementById('item_preco_unit').value,
                itemPrecoTotal: precoTotalAuto,
                itemCod: document.getElementById('cod_produto_hidden').value,
                itemDesc: document.getElementById('descricao_hidden').value,
                itemFaca: document.getElementById('faca_hidden').value,
                itemSubstrato: document.getElementById('substrato_hidden').value,
                precoFinalTotal: (precoFinalManual && precoFinalManual.trim() !== "") ? precoFinalManual : precoTotalAuto,
                condPagamento: document.getElementById('cond_pagamento').value,
                tipoFrete: document.getElementById('tipo_frete').value,
                prazoEntrega: document.getElementById('prazo_entrega').value,
                validade: document.getElementById('validade').value,
                observacoes: document.getElementById('observacoes').value,
            };
            
            google.script.run
                .withSuccessHandler(onGerarSuccess)
                .withFailureHandler(onFailure)
                .gerarOrcamentoPDF(dadosOrcamento);
        }
        
        function onGerarSuccess(resposta) {
            showLoader(false);
            if (resposta.sucesso) {
                document.getElementById('pdf-link').href = resposta.pdfLink;
                document.getElementById('success-message').style.display = 'block'; // <-- Agora vai funcionar
                resetarFormulario();
            } else {
                onFailure({ message: resposta.mensagem });
            }
        }

        // --- FUNÇÕES HELPER ---
        function validarCampos() {
            if (!document.getElementById('vendedora').value) {
                M.toast({html: 'Selecione uma vendedora.', classes: 'red'});
                return false;
            }
            if (!document.getElementById('item_qtd').value || parseFloat(document.getElementById('item_qtd').value) <= 0) {
                M.toast({html: 'A quantidade é obrigatória.', classes: 'red'});
                return false;
            }
             if (!document.getElementById('item_preco_unit').value) {
                M.toast({html: 'O preço unitário é obrigatório.', classes: 'red'});
                return false;
            }
            if (!document.getElementById('cond_pagamento').value) {
                M.toast({html: 'Selecione a condição de pagamento.', classes: 'red'});
                return false;
            }
            return true;
        }

        function resetarFormulario() {
            document.getElementById('cod_produto_busca').value = "";
            document.getElementById('dados-orcamento').style.display = 'none'; // <-- Isto escondia a msg
            
            document.getElementById('cliente_nome').value = "";
            document.getElementById('cliente_cnpj').value = "";
            document.getElementById('item_qtd').value = "";
            document.getElementById('item_preco_unit').value = "";
            document.getElementById('item_preco_total').value = "";
            document.getElementById('preco_final_total').value = "";
            document.getElementById('observacoes').value = "";
            
            document.getElementById('vendedora').selectedIndex = 0;
            document.getElementById('cond_pagamento').selectedIndex = 0;
            document.getElementById('tipo_frete').selectedIndex = 0;
            
            M.FormSelect.init(document.querySelectorAll('select'));
            M.updateTextFields();
        }

        function showLoader(isLoading) {
            document.getElementById('loader').style.display = isLoading ? 'block' : 'none';
            document.getElementById('btnBuscarProduto').disabled = isLoading;
            document.getElementById('btnGerarOrcamento').disabled = isLoading;
        }

        function showBuscaErro(mensagem) {
            const el = document.getElementById('busca-erro');
            el.textContent = mensagem;
            el.style.display = mensagem ? 'block' : 'none';
        }
        
        function onFailure(erro) {
            showLoader(false);
            M.toast({html: 'Erro: ' + erro.message, classes: 'red darken-2'});
            document.getElementById('btnGerarOrcamento').disabled = false;
            document.getElementById('btnBuscarProduto').disabled = false;
        }

    </script>
</body>
</html>
