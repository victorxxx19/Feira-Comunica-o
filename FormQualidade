<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

<script>
// ===================================================
// JAVASCRIPT CORRIGIDO E REFOR√áADO (FormQualidade)
// ===================================================

// Vari√°veis Globais
let todosPedidos = [];
let listaUnidadesCQ = [];
let listaInspetores = [];
let mudancas = {}; // Objeto que rastreia as mudan√ßas para salvar (APENAS para o bot√£o "Salvar Altera√ß√µes")
let modalAtrasoInstance;
let modalConfirmInstance;
let modalDetalhesInstance;
let currentConfirmCallback = null;
let pedidosParaConclusao = []; // Usado pelo modal de atraso

const ORDEM_UNIDADES_CQ = ["Mesa-01", "Mesa-02", "Vemax-01", "Vemax-02"];

// --- Auxiliares de UI/UX ---
function showPageLoader(show) {
  const loader = document.getElementById('page-loader');
  if (loader) loader.style.display = show ? 'block' : 'none';
  document.getElementById('divObjetos').style.opacity = show ? 0.5 : 1;
  document.getElementById('footer-bar').style.display = show ? 'none' : 'flex';
}
function normalizarStatus(str) {
  if (!str) return "";
  return str.toLowerCase()
    .replace(/[√°√†√¢√£]/g, 'a')
    .replace(/[√©√®√™]/g, 'e')
    .replace(/[√≠√¨√Æ]/g, 'i')
    .replace(/[√≥√≤√¥√µ]/g, 'o')
    .replace(/[√∫√π√ª√º]/g, 'u')
    .replace(/√ß/g, 'c')
    .trim()
    .replace(/\s/g, ''); // Remove todos os espa√ßos
}

// --- Fun√ß√µes de Inicializa√ß√£o ---
document.addEventListener('DOMContentLoaded', function() {
  modalAtrasoInstance = M.Modal.init(document.getElementById('motivo-atraso-modal'), { dismissible: false });
  modalConfirmInstance = M.Modal.init(document.getElementById('confirm-modal'), { dismissible: true });
  modalDetalhesInstance = M.Modal.init(document.getElementById('detalhes-pedido-modal'), { dismissible: true });
  
  showPageLoader(true);
  google.script.run
    .withSuccessHandler(onDadosCarregados)
    .withFailureHandler(onCarregarErro)
    .buscarPedidosParaQualidade();

  setupListeners(); 
});

// --- Callbacks do Backend ---
function onDadosCarregados(dadosDoBackend) {
  showPageLoader(false);
  
  if (dadosDoBackend && dadosDoBackend.pedidos) {
    todosPedidos = dadosDoBackend.pedidos || [];
    listaUnidadesCQ = ORDEM_UNIDADES_CQ;
    listaInspetores = dadosDoBackend.inspetores || []; 
    
    renderizarTelaCompleta();
  } else {
    onCarregarErro({ message: "O backend n√£o retornou dados v√°lidos." });
  }
}
function onCarregarErro(err) {
  showPageLoader(false);
  showErrorToast("Falha ao carregar dados: " + (err.message || "Erro desconhecido."));
  renderizarTelaCompleta(); 
}

// --- Renderiza√ß√£o Principal ---
function renderizarTelaCompleta() {
  const controlarCorpo = document.getElementById('tabela-para-controlar-corpo');
  const quadrosContainer = document.getElementById('quadros-cq-container');
  const faturamentoCorpo = document.getElementById('tabela-faturamento-corpo');
  const msgSemPedidos = document.getElementById('msg-sem-pedidos');
  const msgSemCQ = document.getElementById('msg-sem-cq');
  const msgSemFaturamento = document.getElementById('msg-sem-faturamento');
  
  controlarCorpo.innerHTML = "";
  quadrosContainer.innerHTML = "";
  faturamentoCorpo.innerHTML = "";
  
  let pedidosParaControlar = [];
  let pedidosEmQuadrosCQ = {};
  let pedidosNaFaturamento = [];
  
  todosPedidos.forEach(pedido => {
      const statusNormal = normalizarStatus(pedido.status);
      const temUnidadeCQ = pedido.unidadeCQ && pedido.unidadeCQ !== '';

      if (statusNormal === 'naexpedicao') {
          pedidosNaFaturamento.push(pedido);
      } else if (statusNormal === 'controlando' || (temUnidadeCQ && statusNormal !== 'concluido' && statusNormal !== 'naexpedicao')) {
          // Se j√° est√° controlando, OU se tem unidade E n√£o est√° conclu√≠do/expedido
          const unidade = pedido.unidadeCQ || "Sem Unidade";
          if (!pedidosEmQuadrosCQ[unidade]) {
              pedidosEmQuadrosCQ[unidade] = [];
          }
          pedidosEmQuadrosCQ[unidade].push(pedido);
      } else if (statusNormal === 'emproducao' || statusNormal === 'producaofinalizada') {
          pedidosParaControlar.push(pedido);
      }
  });

  // Renderiza Tabela 1
  if (pedidosParaControlar.length === 0) {
      msgSemPedidos.style.display = 'block';
  } else {
      msgSemPedidos.style.display = 'none';
      pedidosParaControlar.sort((a, b) => parseDate(a.dataEntrega) - parseDate(b.dataEntrega));
      pedidosParaControlar.forEach(pedido => {
          controlarCorpo.appendChild(criarLinhaParaControlar(pedido));
      });
  }
  
  // Renderiza Fila de Qualidade (Kanban)
  const unidadesComPedidos = Object.keys(pedidosEmQuadrosCQ);
  // Garante a ordem dos quadros + "Sem Unidade"
  const unidadesOrdenadas = ORDEM_UNIDADES_CQ.filter(u => unidadesComPedidos.includes(u));
  if (pedidosEmQuadrosCQ["Sem Unidade"] && pedidosEmQuadrosCQ["Sem Unidade"].length > 0) {
      unidadesOrdenadas.push("Sem Unidade"); // Adiciona "Sem Unidade" ao final
  }

  if (unidadesOrdenadas.length === 0) {
      msgSemCQ.style.display = 'block';
  } else {
      msgSemCQ.style.display = 'none';
      unidadesOrdenadas.forEach(nomeUnidade => {
          if (pedidosEmQuadrosCQ[nomeUnidade]) {
              const pedidosOrdenados = pedidosEmQuadrosCQ[nomeUnidade].sort((a, b) => {
                  const filaA = parseInt(a.fila) || 999;
                  const filaB = parseInt(b.fila) || 999;
                  if (filaA !== 999 && filaB !== 999) return filaA - filaB;
                  return parseDate(a.dataEntrega) - parseDate(b.dataEntrega);
              });
              quadrosContainer.appendChild(criarQuadroCQComTabela(nomeUnidade, pedidosOrdenados));
          }
      });
  }

  // Renderiza Aguardando Faturamento
  if (pedidosNaFaturamento.length === 0) {
      msgSemFaturamento.style.display = 'block';
  } else {
      msgSemFaturamento.style.display = 'none';
      pedidosNaFaturamento.sort((a, b) => parseDate(a.dataEntrega) - parseDate(b.dataEntrega));
      pedidosNaFaturamento.forEach(pedido => {
          faturamentoCorpo.appendChild(criarLinhaFaturamento(pedido));
      });
  }
  
  // Inicializa os selects do Materialize
  const selects = document.querySelectorAll('select');
  if (selects.length > 0) {
      M.FormSelect.init(selects, {
          dropdownOptions: { container: document.body }
      });
  }
}

// --- Cria√ß√£o de Linhas/Quadros ---
function criarLinhaParaControlar(pedido) {
  const tr = document.createElement('tr');
  tr.dataset.linhaPlanilha = pedido.linhaPlanilha;
  const dataStatus = checkDataStatus(pedido.dataEntrega);
  tr.innerHTML = `
    <td class="num-pedido-clicavel">${pedido.numPedido}</td>
    <td class="${dataStatus.classe}">${pedido.dataEntrega} ${dataStatus.icone}</td>
    <td class="col-cliente">${pedido.cliente}</td>
    <td class="col-descricao">${pedido.descricao}</td>
    <td>${formatarNumero(pedido.quantidade)}</td>
    <td>${pedido.maquinaProd || ''}</td>
    <td>${pedido.impressor || ''}</td>
    <td class="col-obs-cq">${pedido.obsCQ || ''}</td>
    <td class="${getStatusColorClass(pedido.status)}">${pedido.status}</td>
    <td class="col-acao-cq">
      <div class="input-field">
        <select data-tipo-change="unidadeCQ"> ${gerarOptions(listaUnidadesCQ, "", true)}
        </select>
      </div>
    </td>
  `;
  return tr;
}

function criarQuadroCQComTabela(nomeUnidade, pedidos) {
  const divQuadro = document.createElement('div');
  divQuadro.className = 'maquina-quadro';
  const iconeUnidade = (nomeUnidade === "Sem Unidade") ? "work_outline" : "assignment_ind";
  divQuadro.innerHTML = `
    <h5 class="maquina-titulo">
      <i class="material-icons">${iconeUnidade}</i>
      ${nomeUnidade}
    </h5>
    <div class="quadro-corpo">
      <table class="tabela-cq highlight">
        <thead>
          <tr>
            <th>Fila</th>
            <th>N¬∞ Pedido</th>
            <th>Data Entrega</th>
            <th>Cliente</th>
            <th>Descri√ß√£o</th>
            <th>Qtd. Pedido</th>
            <th>Qtd. Aprovada</th>
            <th>Unidade CQ</th>
            <th>Inspetor</th>
            <th>Obs. CQ</th>
            <th>Status</th>
            <th>Concluir Controle</th>
          </tr>
        </thead>
        <tbody></tbody>
      </table>
    </div>
  `;
  const tbody = divQuadro.querySelector('tbody');
  const totalPedidos = pedidos.length;
  pedidos.forEach((pedido, index) => {
    const tr = document.createElement('tr');
    tr.dataset.linhaPlanilha = pedido.linhaPlanilha;
    tr.id = 'linha-' + pedido.linhaPlanilha; // ID √∫nico para a linha
    
    // Controles de Fila
    let tdFilaContent = `<span class="fila-numero">${index + 1}</span>`;
    if (nomeUnidade !== "Sem Unidade") {
      const isPrimeiro = index === 0;
      const isUltimo = index === totalPedidos - 1;
      tdFilaContent = `
        <div class="fila-controles">
          <button class="btn-fila" data-action="UP" data-linha="${pedido.linhaPlanilha}" ${isPrimeiro ? 'disabled' : ''}>‚ñ≤</button>
          <span class="fila-numero">${index + 1}</span>
          <button class="btn-fila" data-action="DOWN" data-linha="${pedido.linhaPlanilha}" ${isUltimo ? 'disabled' : ''}>‚ñº</button>
        </div>
        <input type="hidden" data-tipo-change="fila" value="${index + 1}">
      `;
    }

    const dataStatus = checkDataStatus(pedido.dataEntrega);
    const qtdAprovadaVal = pedido.qtdAprovada || '';

    tr.innerHTML = `
      <td class="col-fila-mq">${tdFilaContent}</td>
      <td class="col-pedido-mq num-pedido-clicavel">${pedido.numPedido}</td>
      <td class="col-data-mq ${dataStatus.classe}">${pedido.dataEntrega} ${dataStatus.icone}</td>
      <td class="col-cliente-mq">${pedido.cliente}</td>
      <td class="col-descricao-mq">${pedido.descricao}</td>
      <td>${formatarNumero(pedido.quantidade)}</td>
      <td class="col-qtd-aprovada-cq">
        <div class="input-field">
          <input type="number" step="1"
                 id="qtdAprovada-${pedido.linhaPlanilha}" 
                 data-tipo-change="qtdAprovada" 
                 value="${qtdAprovadaVal}">
        </div>
      </td>
      <td class="col-unidade-cq-mq">
        <div class="input-field">
          <select id="unidadeCQ-${pedido.linhaPlanilha}" data-tipo-change="unidadeCQ">
            ${gerarOptions(listaUnidadesCQ, pedido.unidadeCQ)}
          </select>
        </div>
      </td>
      <td class="col-impressor-mq">
        <div class="input-field">
          <select id="inspetor-${pedido.linhaPlanilha}" data-tipo-change="inspetor">
            ${gerarOptions(listaInspetores, pedido.inspetor, true)}
          </select>
        </div>
      </td>
      <td class="col-obs-cq-mq">
        <div class="input-field">
            <input type="text"
                   id="obsCQ-${pedido.linhaPlanilha}"
                   data-tipo-change="obsCQ"
                   value="${pedido.obsCQ || ''}">
        </div>
      </td>
      <td class="col-status-mq ${getStatusColorClass(pedido.status)}">
        ${pedido.status}
      </td>
      <td class="col-acao-cq">
        <button class="btn waves-effect waves-light btn-concluir-cq" data-linha="${pedido.linhaPlanilha}">
          Finalizar Controle
        </button>
      </td>
    `;
    tbody.appendChild(tr);
  });
  return divQuadro;
}

function criarLinhaFaturamento(pedido) {
  const tr = document.createElement('tr');
  tr.dataset.linhaPlanilha = pedido.linhaPlanilha;
  tr.id = 'linha-' + pedido.linhaPlanilha; // ID √∫nico para a linha
  const dataStatus = checkDataStatus(pedido.dataEntrega);
  tr.innerHTML = `
    <td class="num-pedido-clicavel">${pedido.numPedido}</td>
    <td class="${dataStatus.classe}">${pedido.dataEntrega} ${dataStatus.icone}</td>
    <td class="col-cliente">${pedido.cliente}</td>
    <td class="col-descricao">${pedido.descricao}</td>
    <td>${formatarNumero(pedido.quantidade)}</td>
    <td class="${getStatusColorClass(pedido.status)}">${pedido.status}</td>
    <td class="col-acao-cq">
      <button class="btn waves-effect waves-light btn-finalizar-pedido" data-linha="${pedido.linhaPlanilha}">
        Finalizar Pedido
      </button>
    </td>
  `;
  return tr;
}

// --- Listeners de Eventos ---
function setupListeners() {
  // Eventos de mudan√ßa para Tabela 1 e Fila de Qualidade (Selects)
  document.getElementById('tabela-para-controlar-corpo').addEventListener('change', handleEventoTabela1);
  document.getElementById('quadros-cq-container').addEventListener('change', handleEvento);
  
  // Eventos de digita√ß√£o para Fila de Qualidade (Input Qtd/Obs)
  document.getElementById('quadros-cq-container').addEventListener('input', handleEvento);
  
  // Eventos de clique para bot√µes UP/DOWN e Finalizar Controle
  document.getElementById('quadros-cq-container').addEventListener('click', function(event) {
    const btnFila = event.target.closest('.btn-fila');
    if (btnFila) {
      const linha = btnFila.dataset.linha;
      const acao = btnFila.dataset.action;
      if (linha && acao) reordenarPedidos(linha, acao);
      return;
    }
    const btnConcluir = event.target.closest('.btn-concluir-cq');
    if (btnConcluir) {
      handleConcluirControleClick(btnConcluir.dataset.linha);
      return;
    }
  }); 
  
  // Evento de clique para Finalizar Pedido
  document.getElementById('tabela-faturamento-corpo').addEventListener('click', function(event) {
    const btnFinalizar = event.target.closest('.btn-finalizar-pedido');
    if (btnFinalizar) {
      handleFinalizarPedidoClick(btnFinalizar.dataset.linha);
      return;
    }
  });
  
  // Listener de clique para o modal de detalhes (N√∫meros de Pedido)
  document.getElementById('divObjetos').addEventListener('click', function(event) {
    const pedidoCell = event.target.closest('.num-pedido-clicavel');
    if (pedidoCell) {
      const tr = pedidoCell.closest('tr[data-linha-planilha]');
      if (tr) {
        handleNumeroPedidoClick(tr.dataset.linhaPlanilha);
        return;
      }
    }
  });
  
  // Bot√£o Salvar
  document.getElementById('btnSalvar').addEventListener('click', onSalvarClick);
  
  // Confirma√ß√£o de Modal
  document.getElementById('confirm-modal-confirmar').addEventListener('click', onConfirmModalConfirm);
  document.getElementById('confirm-modal-cancelar').addEventListener('click', function() {
    currentConfirmCallback = null;
    modalConfirmInstance.close();
  });
  
  // Modal de Atraso
  document.getElementById('motivoAtrasoConfirmar').addEventListener('click', onConfirmarAtrasoClick);
  document.getElementById('motivoAtrasoInput').addEventListener('input', checkMotivoAtrasoInput);
}

// ===================================================
// --- NOVAS FUN√á√ïES DE EVENTO E WORKFLOW ---
// ===================================================

/**
 * Pega os dados do formul√°rio de UMA linha espec√≠fica.
 * @param {string} linhaPlanilha O ID da linha (ex: '5')
 * @returns {object} Um objeto com os dados atuais da linha.
 */
function getDadosDaLinha(linhaPlanilha) {
    const pedido = todosPedidos.find(p => p.linhaPlanilha == linhaPlanilha);
    if (!pedido) return null;

    // Pega os valores *atuais* do DOM, se a linha existir (ex: na Fila de Qualidade)
    const elLinha = document.getElementById('linha-' + linhaPlanilha);
    
    let dados = {
        linhaPlanilha: linhaPlanilha,
        numPedido: pedido.numPedido,
        dataEntrega: pedido.dataEntrega,
        semExcedente: pedido.semExcedente,
        quantidade: pedido.quantidade,
        // Pega valores padr√£o do objeto de cache
        qtdAprovada: pedido.qtdAprovada,
        inspetor: pedido.inspetor,
        unidadeCQ: pedido.unidadeCQ,
        obsCQ: pedido.obsCQ,
        status: pedido.status
    };

    if (elLinha) {
        // Se a linha est√° na tela, sobrescreve com os valores do DOM
        const inputQtd = elLinha.querySelector(`input[data-tipo-change="qtdAprovada"]`);
        const selectInspetor = elLinha.querySelector(`select[data-tipo-change="inspetor"]`);
        const selectUnidade = elLinha.querySelector(`select[data-tipo-change="unidadeCQ"]`);
        const inputObs = elLinha.querySelector(`input[data-tipo-change="obsCQ"]`);
        
        if (inputQtd) dados.qtdAprovada = inputQtd.value;
        if (selectInspetor) dados.inspetor = selectInspetor.value;
        if (selectUnidade) dados.unidadeCQ = selectUnidade.value;
        if (inputObs) dados.obsCQ = inputObs.value;
    }
    
    return dados;
}

/**
 * Valida uma linha de dados antes de salvar (para Na Expedi√ß√£o ou Conclu√≠do).
 * @param {object} dadosDaLinha - O objeto retornado por getDadosDaLinha
 * @returns {object} - {valido: boolean, mensagem: string}
 */
function validarLinha(dadosDaLinha) {
    // 1. Valida√ß√£o de Inspetor
    if (!dadosDaLinha.inspetor || dadosDaLinha.inspetor.trim() === '') {
        return { valido: false, mensagem: `Pedido ${dadosDaLinha.numPedido}: N√£o pode ser finalizado sem um Inspetor selecionado.` };
    }

    // 2. Valida√ß√£o de Qtd. Aprovada
    const qtdNum = parseFloat(dadosDaLinha.qtdAprovada) || 0;
    if (qtdNum <= 0) {
        return { valido: false, mensagem: `Pedido ${dadosDaLinha.numPedido}: A "Qtd. Aprovada" deve ser informada (maior que 0).` };
    }

    // 3. Valida√ß√£o de Regra de Excedente
    const validacaoExcedente = getRegrasExcedente(dadosDaLinha, qtdNum);
    if (!validacaoExcedente.valida) {
        let mensagem = `A Qtd. Aprovada (${formatarNumero(qtdNum)}) deve ser `;
        if (validacaoExcedente.semExcedente) {
            mensagem += `EXATAMENTE a Qtd. do Pedido (${formatarNumero(validacaoExcedente.limiteMax)}).`;
        } else {
            mensagem += `entre ${formatarNumero(validacaoExcedente.limiteMin)} e ${formatarNumero(validacaoExcedente.limiteMax)} (Regra +/- 10%).`;
        }
        return { valido: false, mensagem: mensagem };
    }

    return { valido: true, mensagem: "OK" };
}

/**
 * (FLUXO NOVO - Tabela 1)
 * Lida com a sele√ß√£o de Unidade CQ na Tabela 1.
 * Move o item para "Controlando" e salva IMEDIATAMENTE.
 */
function handleEventoTabela1(event) {
    const elemento = event.target;
    const tipoChange = elemento.dataset.tipoChange;
    if (tipoChange !== 'unidadeCQ') return;

    const container = elemento.closest('[data-linha-planilha]');
    if (!container) return;

    const linhaPlanilha = container.dataset.linhaPlanilha;
    const valor = elemento.value;

    if (valor && valor !== "") {
        const pedido = getDadosDaLinha(linhaPlanilha);
        
        // Define as mudan√ßas de status e unidade
        pedido.status = "Controlando";
        pedido.unidadeCQ = valor;
        
        // Remove campos desnecess√°rios para o backend
        delete pedido.numPedido;
        delete pedido.dataEntrega;
        delete pedido.semExcedente;
        delete pedido.quantidade;

        showConfirmModal(
            'Iniciar Controle?',
            `Mover o pedido <strong>${pedido.numPedido}</strong> para a fila <strong>${valor}</strong>?`,
            function() {
                showPageLoader(true);
                // Salva imediatamente
                salvarPedidos([pedido]); 
            },
            'blue'
        );
    }
}

/**
 * (FLUXO NOVO - Tabela 2)
 * Lida com cliques no bot√£o "Finalizar Controle".
 * Valida e salva IMEDIATAMENTE.
 */
function handleConcluirControleClick(linhaPlanilha) {
    const dadosLinha = getDadosDaLinha(linhaPlanilha);
    if (!dadosLinha) return;

    // 1. Valida√ß√£o (Inspetor, Qtd. Aprovada, Regra Excedente)
    const validacao = validarLinha(dadosLinha);
    if (!validacao.valido) {
        showErrorToast(validacao.mensagem);
        return;
    }

    // 2. Confirma√ß√£o
    showConfirmModal(
      'Finalizar Controle?',
      `Mover o pedido <strong>${dadosLinha.numPedido}</strong> para "Aguardando Faturamento"?`,
      function() {
          // 3. Prepara os dados para salvar
          dadosLinha.status = "Na Expedi√ß√£o";
          
          // Limpa dados desnecess√°rios para o backend
          delete dadosLinha.numPedido;
          delete dadosLinha.dataEntrega;
          delete dadosLinha.semExcedente;
          delete dadosLinha.quantidade;
          
          // 4. Salva
          showPageLoader(true);
          salvarPedidos([dadosLinha]);
      },
      'cyan'
    );
}

/**
 * (FLUXO NOVO - Tabela 3)
 * Lida com cliques no bot√£o "Finalizar Pedido".
 * Valida e salva IMEDIATAMENTE.
 */
function handleFinalizarPedidoClick(linhaPlanilha) {
    const dadosLinha = getDadosDaLinha(linhaPlanilha);
    if (!dadosLinha) return;

    // 1. Valida√ß√£o (Inspetor, Qtd. Aprovada, Regra Excedente)
    // A valida√ß√£o j√° foi feita no passo anterior, mas validamos de novo por seguran√ßa
    const validacao = validarLinha(dadosLinha);
    if (!validacao.valido) {
        showErrorToast(validacao.mensagem);
        return;
    }
  
    // 2. Confirma√ß√£o
    showConfirmModal(
      'Finalizar Pedido?',
      '<strong>Esta a√ß√£o n√£o pode ser desfeita.</strong><br>Tem certeza que deseja CONCLUIR o pedido <strong>' + dadosLinha.numPedido + '</strong>?',
      function() {
          // 3. Prepara os dados para salvar
          dadosLinha.status = "Conclu√≠do";
          
          // Limpa dados desnecess√°rios
          delete dadosLinha.numPedido;
          // delete dadosLinha.dataEntrega; // Data de entrega √© usada para checar atraso
          delete dadosLinha.semExcedente;
          delete dadosLinha.quantidade;

          // 4. Verifica Atraso
          const dataStatus = checkDataStatus(dadosLinha.dataEntrega);
          if (dataStatus.classe === 'data-atrasada') {
              pedidosParaConclusao = [dadosLinha]; // Salva o pedido para o modal de atraso
              
              modalAtrasoInstance.open();
              const motivoInput = document.getElementById('motivoAtrasoInput');
              if(motivoInput) {
                   motivoInput.value = "";
                   M.textareaAutoResize(motivoInput);
              }
              checkMotivoAtrasoInput();
          } else {
              // 5. Salva (sem atraso)
              showPageLoader(true);
              salvarPedidos([dadosLinha]);
          }
      },
      'green'
    );
}

/**
 * (FLUXO NOVO - Bot√£o Salvar Altera√ß√µes)
 * Lida com mudan√ßas de campos que N√ÉO s√£o de status (fila, obs, etc)
 */
function handleEvento(event) {
  const elemento = event.target;
  const tipoChange = elemento.dataset.tipoChange;
  if (!tipoChange) return;

  const container = elemento.closest('[data-linha-planilha]');
  if (!container) return;
  
  const linhaPlanilha = container.dataset.linhaPlanilha;
  const valor = elemento.value;
  
  const pedido = todosPedidos.find(p => p.linhaPlanilha == linhaPlanilha);
  if (!pedido) return;

  // Se o usu√°rio mudar a Unidade CQ de um pedido j√° em controle
  if (tipoChange === 'unidadeCQ') {
      pedido.unidadeCQ = valor;
      if (!mudancas[linhaPlanilha]) mudancas[linhaPlanilha] = {};
      mudancas[linhaPlanilha][tipoChange] = valor;
      document.getElementById('btnSalvar').disabled = false;
      // Re-renderiza para mover entre os quadros
      renderizarTelaCompleta();
      return;
  }

  // 2. L√≥gica normal de salvamento (para todos os outros campos: fila, inspetor, qtd, obs)
  pedido[tipoChange] = valor; 
  if (!mudancas[linhaPlanilha]) mudancas[linhaPlanilha] = {};
  mudancas[linhaPlanilha][tipoChange] = valor;
  document.getElementById('btnSalvar').disabled = false;

  // Re-renderiza se for mudan√ßa de fila para atualizar os bot√µes UP/DOWN
  if (tipoChange === 'fila') {
    renderizarTelaCompleta();
  }
}

/**
 * (FLUXO NOVO - Bot√£o Salvar Altera√ß√µes)
 * Salva apenas as mudan√ßas no objeto 'mudancas' (fila, obs, inspetor, etc)
 */
function onSalvarClick() {
  const btn = document.getElementById('btnSalvar');
  btn.disabled = true;
  showPageLoader(true);
  
  const pedidosComMudancas = [];
  
  try {
    Object.keys(mudancas).forEach(function(linhaPlanilha) {
      const mudanca = mudancas[linhaPlanilha];
      mudanca.linhaPlanilha = linhaPlanilha; // Garante que a linha est√° no objeto
      pedidosComMudancas.push(mudanca);
    });
  } catch (e) {
     showErrorToast("Erro ao processar mudan√ßas: " + e.message);
     return; 
  }
  
  if (pedidosComMudancas.length === 0) {
    showSuccessToast("Nenhuma mudan√ßa para salvar."); // Mudado de erro para sucesso
    btn.disabled = true;
    showPageLoader(false);
    return;
  }
  
  // Salva no backend
  salvarPedidos(pedidosComMudancas);
}

/**
 * (FLUXO NOVO)
 * Lida com a confirma√ß√£o do modal de atraso.
 */
function onConfirmarAtrasoClick() {
  const motivo = document.getElementById('motivoAtrasoInput').value.trim();
  if (!motivo) {
    showErrorToast("O motivo do atraso √© obrigat√≥rio!");
    return;
  }
  
  // Adiciona o motivo de atraso a todos os pedidos pendentes
  pedidosParaConclusao.forEach(function(mudanca) {
      mudanca.motivoAtraso = motivo;
  });
  
  modalAtrasoInstance.close();
  showPageLoader(true); 
  salvarPedidos(pedidosParaConclusao);
  pedidosParaConclusao = []; // Limpa o array
}

// --- Fun√ß√µes de Comunica√ß√£o com Google Apps Script ---
function salvarPedidos(pedidos) {
  if (!pedidos || pedidos.length === 0) {
    showErrorToast("Nenhum dado para salvar.");
    showPageLoader(false);
    document.getElementById('btnSalvar').disabled = false;
    return;
  }

  showSuccessToast("Salvando dados...");
  
  google.script.run
    .withSuccessHandler(onSalvarSucesso)
    .withFailureHandler(onSalvarErro)
    .salvarMudancasQualidade(pedidos);
}
function onSalvarSucesso(msg) {
  showSuccessToast(msg);
  mudancas = {}; // Limpa as mudan√ßas pendentes
  document.getElementById('btnSalvar').disabled = true;
  
  // Recarrega todos os dados
  google.script.run
    .withSuccessHandler(onDadosCarregados) 
    .withFailureHandler(onCarregarErro) 
    .buscarPedidosParaQualidade();
}
function onSalvarErro(err) {
  showPageLoader(false); 
  showErrorToast(err.message || err);
  document.getElementById('btnSalvar').disabled = false;
  
  // Recarrega os dados para reverter visualmente
  google.script.run
    .withSuccessHandler(onDadosCarregados) 
    .withFailureHandler(onCarregarErro) 
    .buscarPedidosParaQualidade();
}

// --- Fun√ß√µes Auxiliares (Toasts, Formata√ß√£o, etc.) ---
function showConfirmModal(title, message, callback, colorClass) {
  colorClass = colorClass || 'green';
  const titleEl = document.getElementById('confirm-modal-title');
  const msgEl = document.getElementById('confirm-modal-message');
  const btnConfirmar = document.getElementById('confirm-modal-confirmar');
  
  if(titleEl) titleEl.innerText = title;
  if(msgEl) msgEl.innerHTML = message;
  if(btnConfirmar) btnConfirmar.className = 'waves-effect btn ' + colorClass;
  
  currentConfirmCallback = callback;
  modalConfirmInstance.open();
}
function onConfirmModalConfirm() {
  modalConfirmInstance.close();
  if (typeof currentConfirmCallback === 'function') {
    currentConfirmCallback();
  }
  currentConfirmCallback = null;
}
function checkMotivoAtrasoInput() {
  const input = document.getElementById('motivoAtrasoInput');
  const btn = document.getElementById('motivoAtrasoConfirmar');
  if(input && btn) {
    btn.disabled = input.value.trim().length === 0;
  }
}
function reordenarPedidos(linhaPlanilha, direcao) {
  const pedidoAtual = todosPedidos.find(function(p) { return p.linhaPlanilha == linhaPlanilha; });
  if (!pedidoAtual) return;
  const nomeUnidade = pedidoAtual.unidadeCQ;
  
  // 1. Filtra e ordena a lista APENAS para a unidade
  const pedidosNaUnidade = todosPedidos
    .filter(function(p) { return p.unidadeCQ === nomeUnidade; })
    .sort(function(a, b) { return (parseInt(a.fila) || 999) - (parseInt(b.fila) || 999); });
    
  const indiceAtual = pedidosNaUnidade.findIndex(function(p) { return p.linhaPlanilha == linhaPlanilha; });
  if (indiceAtual === -1) return;
  
  const indiceDestino = (direcao === 'UP') ? indiceAtual - 1 : indiceAtual + 1;
  if (indiceDestino < 0 || indiceDestino >= pedidosNaUnidade.length) return;
  
  const pedidoDestino = pedidosNaUnidade[indiceDestino];
  
  // 2. Troca os valores de Fila
  const novaFilaAtual = (indiceDestino + 1).toString();
  const novaFilaDestino = (indiceAtual + 1).toString();
  
  // Atualiza o objeto local e rastreia a mudan√ßa
  pedidoAtual.fila = novaFilaAtual;
  if (!mudancas[pedidoAtual.linhaPlanilha]) mudancas[pedidoAtual.linhaPlanilha] = {};
  mudancas[pedidoAtual.linhaPlanilha].fila = novaFilaAtual;
  
  pedidoDestino.fila = novaFilaDestino;
  if (!mudancas[pedidoDestino.linhaPlanilha]) mudancas[pedidoDestino.linhaPlanilha] = {};
  mudancas[pedidoDestino.linhaPlanilha].fila = novaFilaDestino;
  
  document.getElementById('btnSalvar').disabled = false;
  renderizarTelaCompleta(); // Re-renderiza para que a nova ordem apare√ßa
}

// Fun√ß√µes de formata√ß√£o (mantidas)
function getStatusColorClass(status) {
  const statusLower = normalizarStatus(status);
  switch (statusLower) {
    case "emproducao": return "status-em-producao"; 
    case "producaofinalizada": return "status-em-producao"; 
    case "controlando": return "status-controlando";
    case "naexpedicao": return "status-na-expedicao";
    case "concluido": return "status-concluido";
    default: return "";
  }
}
function showSuccessToast(message) { 
  M.toast({ 
    html: '<i class="material-icons left">check_circle</i> ' + message, 
    classes: 'green darken-1', 
    displayLength: 3000, 
    container: '#toast-custom-container' 
  }); 
}
function showErrorToast(error) { 
  const msg = (typeof error === 'object' && error.message) ? error.message : error;
  M.toast({ 
    html: '<i class="material-icons left">error</i> ' + msg, 
    classes: 'red darken-2', 
    displayLength: 5000, 
    container: '#toast-custom-container' 
  }); 
}
function formatarNumero(valor) { 
  if (valor === null || valor === undefined || valor === "") return '0';
  let numeroLimpo = String(valor).replace(/\./g, "").replace(',', '.'); 
  let num = parseFloat(numeroLimpo); 
  if (isNaN(num)) return valor; 
  return num.toLocaleString('pt-BR', { 
    maximumFractionDigits: 0, 
    minimumFractionDigits: 0 
  }); 
}
function gerarOptions(lista, valorSelecionado, comVazio) {
  if (!lista) lista = [];
  comVazio = comVazio || false;
  let html = comVazio ? '<option value="" ' + (!valorSelecionado ? 'selected' : '') + '>Selecione</option>' : '';
  for (var i = 0; i < lista.length; i++) {
    var item = lista[i];
    var selected = item === valorSelecionado ? 'selected' : '';
    html += '<option value="' + item + '" ' + selected + '>' + item + '</option>';
  }
  return html;
}
function parseDate(dateString) { 
  if (!dateString) return new Date(0);
  const parts = dateString.split('/'); 
  if (parts.length !== 3) return new Date(0);
  return new Date(parts[2], parts[1] - 1, parts[0]); 
}
function checkDataStatus(dateString) { 
  if (!dateString) return { classe: "", icone: "" };
  const hoje = new Date(); 
  hoje.setHours(0, 0, 0, 0);
  const dataEntrega = parseDate(dateString); 
  if (isNaN(dataEntrega.getTime()) || dataEntrega.getTime() === 0) return { classe: "", icone: "" }; 
  const diffTime = dataEntrega.getTime() - hoje.getTime();
  const diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24)); 
  
  if (diffDays < 0) {
    return { 
      classe: 'data-atrasada', 
      icone: '<i class="material-icons data-status-icon red-text">error_outline</i>' 
    }; 
  }
  if (diffDays <= 2) {
    return { 
      classe: 'data-alerta', 
      icone: '<i class="material-icons data-status-icon orange-text">warning</i>' 
    }; 
  }
  return { classe: "", icone: '' };
}

// Dentro do <script> do FormQualidade.html

/**
 * Retorna as regras de valida√ß√£o de quantidade aprovada (limites).
 */
function getRegrasExcedente(pedido, qtdAprovada) {
    const qtdPedido = parseFloat(String(pedido.quantidade).replace(/\./g, "").replace(',', '.')) || 0;
    const qtdAprovadaNum = parseFloat(String(qtdAprovada).replace(/\./g, "").replace(',', '.')) || 0;
    const semExcedente = pedido.semExcedente === true || pedido.semExcedente === "TRUE" || pedido.semExcedente === "VERDADEIRO";

    let limiteMin = qtdPedido;
    let limiteMax = qtdPedido;

    if (semExcedente) {
        // Se Sem Excedente: Exige a quantidade EXATA
        limiteMin = qtdPedido;
        limiteMax = qtdPedido;
    } else {
        // Se Com Excedente: Permite +/- 10%
        limiteMin = Math.round(qtdPedido * 0.90);
        limiteMax = Math.round(qtdPedido * 1.10);
    }
    
    // Valida√ß√£o
    const valida = (qtdAprovadaNum >= limiteMin && qtdAprovadaNum <= limiteMax);

    return {
        valida: valida,
        limiteMin: limiteMin,
        limiteMax: limiteMax,
        semExcedente: semExcedente
    };
}

// Fun√ß√µes do Modal de Detalhes (mantidas)
function handleNumeroPedidoClick(linhaPlanilha) {
  const loading = document.getElementById('detalhes-loading');
  const content = document.getElementById('detalhes-conteudo');
  loading.style.display = 'block';
  content.style.display = 'none';
  
  // Limpa campos
  document.getElementById('detalhe-num-pedido').innerText = '---';
  document.getElementById('detalhe-cliente').innerText = '---';
  document.getElementById('detalhe-vendedora').innerText = '---';
  document.getElementById('detalhe-tipo-expedicao').innerText = '---';
  document.getElementById('detalhe-descricao').innerText = '---';
  document.getElementById('detalhe-obs-pcp').innerText = 'Buscando...';
  document.getElementById('detalhe-obs-cq').innerText = 'Buscando...';
  
  modalDetalhesInstance.open();
  
  google.script.run
    .withSuccessHandler(onGetDetalhesSucesso)
    .withFailureHandler(onGetDetalhesErro)
    .getDetalhesCompletosDoPedido(linhaPlanilha);
}

function onGetDetalhesSucesso(detalhes) {
  const loading = document.getElementById('detalhes-loading');
  const content = document.getElementById('detalhes-conteudo');
  
  // Fun√ß√£o auxiliar para preencher campo
  const preencher = (id, valor, sufixo = '') => {
    const el = document.getElementById(id);
    if (el) {
      el.innerText = (valor || '---') + (valor ? sufixo : '');
    }
  };
  
  // Fun√ß√£o auxiliar para preencher observa√ß√µes
  const preencherObs = (id, valor, textoVazio = 'Nenhuma observa√ß√£o.') => {
    const el = document.getElementById(id);
    if (el) {
      if (valor && valor.trim()) {
        el.innerText = valor;
        el.className = 'detalhes-pedido-obs';
        // Define a cor com base no tipo de observa√ß√£o
        if (id === 'detalhe-problemas-producao' || id === 'detalhe-motivo-cancelamento' || id === 'detalhe-motivo-atraso') {
          el.style.borderColor = '#D32F2F'; // Vermelho para problemas
          el.style.backgroundColor = '#FFEBEE';
          el.style.color = '#B71C1C';
        } else {
          el.style.borderColor = '#ffc107'; // Amarelo padr√£o
          el.style.backgroundColor = '#fff3cd';
          el.style.color = '#856404';
        }
      } else {
        el.innerText = textoVazio;
        el.className = 'detalhes-pedido-obs vazio';
        // Limpa estilos customizados se estiver vazio
        el.style.borderColor = '';
        el.style.backgroundColor = '';
        el.style.color = '';
      }
    }
  };

  // Preenche todos os 43 campos
  preencher('detalhe-num-pedido', detalhes.numPedido);
  preencher('detalhe-data-pedido', detalhes.dataPedido);
  preencher('detalhe-tipo-pedido', detalhes.tipo);
  preencher('detalhe-quantidade', formatarNumero(detalhes.quantidade));
  preencher('detalhe-vendedora', detalhes.vendedora);
  preencher('detalhe-tipo-expedicao', detalhes.tipoExpedicao);
  preencher('detalhe-data-entrega', detalhes.dataEntrega);
  preencher('detalhe-data-conclusao', detalhes.dataConclusao);
  preencher('detalhe-status', detalhes.status);
  preencher('detalhe-cod-produto', detalhes.codProduto);
  preencher('detalhe-cliente', detalhes.cliente);
  preencher('detalhe-descricao', detalhes.descricao);
  preencher('detalhe-faca', detalhes.faca);
  preencher('detalhe-substrato', detalhes.substrato);
  preencher('detalhe-tipo-cola', detalhes.tipoCola);
  preencher('detalhe-largura', detalhes.largura, ' mm');
  preencher('detalhe-altura', detalhes.altura, ' mm');
  preencher('detalhe-puxada', detalhes.puxada, ' mm');
  preencher('detalhe-qtd-cores', detalhes.qtdCores);
  preencher('detalhe-carreiras', detalhes.carreiras);
  preencher('detalhe-tipo-acabamento', detalhes.tipoAcabamento);
  preencher('detalhe-cold', detalhes.cold);
  preencher('detalhe-metragem', detalhes.metragem, ' m');
  preencher('detalhe-sem-excedente', (detalhes.semExcedente === 'true' || detalhes.semExcedente === 'VERDADEIRO') ? 'SIM' : 'N√ÉO');

  // Produ√ß√£o
  preencher('detalhe-maquina', detalhes.maquina);
  preencher('detalhe-fila', detalhes.fila);
  preencher('detalhe-impressor', detalhes.impressor);
  preencher('detalhe-data-inicio-prod', detalhes.dataInicioProd);
  preencher('detalhe-data-fim-prod', detalhes.dataFimProd);
  preencher('detalhe-fornecedor-substrato', detalhes.fornecedorSubstrato);
  preencher('detalhe-lote-substrato', detalhes.loteSubstrato);
  preencher('detalhe-metragem-real', detalhes.metragemReal, ' m');
  preencher('detalhe-refugo-metragem', detalhes.refugoMetragem, ' m');

  // CQ
  preencher('detalhe-unidade-cq', detalhes.unidadeCQ);
  preencher('detalhe-inspetor', detalhes.inspetor);
  preencher('detalhe-qtd-aprovada-cq', formatarNumero(detalhes.qtdAprovadaCQ));

  // Observa√ß√µes
  preencherObs('detalhe-obs-pcp', detalhes.obsPCP);
  preencherObs('detalhe-problemas-producao', detalhes.problemasProducao, 'Nenhum problema relatado.');
  preencherObs('detalhe-obs-cq', detalhes.obsCQ);
  preencherObs('detalhe-motivo-mais-metragem', detalhes.motivoMaisMetragem, 'Nenhum motivo informado.');
  preencherObs('detalhe-motivo-atraso', detalhes.motivoAtraso, 'Nenhum atraso ou motivo informado.');
  preencherObs('detalhe-motivo-cancelamento', detalhes.motivoCancelamento, 'N√£o cancelado.');

  // Exibe o conte√∫do
  loading.style.display = 'none';
  content.style.display = 'block';
}

function onGetDetalhesErro(err) {
  showErrorToast('Erro ao buscar detalhes: ' + err.message);
  modalDetalhesInstance.close();
}
</script>
</body>
</html>
