<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

<script>
// VARIAVEIS E HELPERS
let todosPedidos = [];
let todosLotes = {};
let listaInspetores = [];
let listaUnidadesCQ = ["Mesa-01", "Mesa-02", "Mesa-03", "Vemax", "Revisora One"];
let loteEmEdicaoId = null;
const selectOptions = { dropdownOptions: { container: document.body } };

function gerarOptions(lista, texto) {
    return `<option value="" disabled selected>${texto}</option>` + lista.map(x=>`<option>${x}</option>`).join('');
}
function formatNum(v) { return v ? parseFloat(String(v).replace(/\./g,'').replace(',','.')).toLocaleString('pt-BR') : '0'; }
function normalizar(s) { return s ? s.toLowerCase().normalize('NFD').replace(/[\u0300-\u036f]/g,"").replace(/ç/g,'c').replace(/\s/g,'') : ''; }
function showPageLoader(s) { document.getElementById('page-loader').style.display=s?'flex':'none'; }
function onCarregarErro(e) { showPageLoader(false); M.toast({html:'Erro: '+e.message,classes:'red'}); }
function populaSelect(id,l) { 
    const el = document.getElementById(id);
    if(el) el.innerHTML = gerarOptions(l, "Selecione");
}

document.addEventListener('DOMContentLoaded', () => {
    M.AutoInit();
    carregarDados();
});

function carregarDados() {
    showPageLoader(true);
    google.script.run.withSuccessHandler(data => {
        todosPedidos = data.pedidos || [];
        todosLotes = data.lotesInspecao || {};
        listaInspetores = data.inspetores || [];
        populaSelect('lote-unidade-cq', listaUnidadesCQ);
        populaSelect('lote-inspetor', listaInspetores);
        M.FormSelect.init(document.querySelectorAll('select'), selectOptions);
        renderizar();
        showPageLoader(false);
    }).buscarPedidosParaQualidade_v2();
}

function renderizar() {
    const corpo1 = document.getElementById('tabela-para-controlar-corpo');
    const corpo2 = document.getElementById('tabela-em-controle-corpo');
    corpo1.innerHTML = ""; corpo2.innerHTML = "";

    const paraIniciar = todosPedidos.filter(p => ['producaofinalizada','emproducao'].includes(normalizar(p.status)));
    const emAndamento = todosPedidos.filter(p => normalizar(p.status) === 'controlando');

    if(paraIniciar.length === 0) document.getElementById('msg-sem-pedidos').style.display = 'block';
    else {
        document.getElementById('msg-sem-pedidos').style.display = 'none';
        paraIniciar.forEach(p => {
            corpo1.innerHTML += `
            <tr>
                <td>${p.numPedido}</td><td>${p.dataEntrega}</td><td>${p.cliente}</td>
                <td style="font-size:0.8rem">${p.descricao}</td><td>${formatNum(p.quantidade)}</td>
                <td>${p.status}</td>
                <td><button class="btn-small btn-iniciar-cq waves-effect tooltipped" data-tooltip="Iniciar" onclick="iniciar('${p.linhaPlanilha}')"><i class="material-icons">play_arrow</i></button></td>
            </tr>`;
        });
    }

    if(emAndamento.length === 0) document.getElementById('msg-sem-cq').style.display = 'block';
    else {
        document.getElementById('msg-sem-cq').style.display = 'none';
        emAndamento.forEach(p => {
            const dados = todosLotes[p.numPedido] || {totalAprovado:0, totalPesoRefugo:0, count:0, pesoReferencia:0};
            
            let textoPerda = "-";
            let classePerda = "";
            if (dados.totalPesoRefugo > 0) {
                const qtdPorRolo = parseFloat(p.qtdPorRolo) || 1000;
                const pesoRef = parseFloat(dados.pesoReferencia) || 0;
                const qtdPedido = parseFloat(String(p.quantidade).replace(/\./g, '').replace(',', '.')) || 0;
                let percStr = "?%"; let qtdEstStr = "?";
                if (pesoRef > 0 && qtdPedido > 0) {
                    const pesoTotalTeorico = (qtdPedido / qtdPorRolo) * pesoRef;
                    const perc = (dados.totalPesoRefugo / pesoTotalTeorico) * 100;
                    percStr = perc.toFixed(2).replace('.', ',') + "%";
                    const qtdEst = Math.floor((dados.totalPesoRefugo / pesoRef) * qtdPorRolo);
                    qtdEstStr = qtdEst.toLocaleString('pt-BR');
                    if(perc > 10) classePerda = "perda-alta";
                }
                const pesoFmt = dados.totalPesoRefugo.toFixed(3).replace('.', ',');
                textoPerda = `${pesoFmt} kg (${percStr}) : ${qtdEstStr}`;
            }

            corpo2.innerHTML += `
            <tr data-id="${p.numPedido}">
                <td>${p.numPedido}</td><td>${p.dataEntrega}</td><td>${p.cliente}</td>
                <td>${formatNum(p.quantidade)}</td>
                <td style="font-weight:bold; color:#2E7D32;">${formatNum(dados.totalAprovado)}</td>
                <td class="${classePerda}" style="font-size:0.8rem; font-weight:bold;">${textoPerda}</td>
                <td><span class="new badge grey" data-badge-caption="">${dados.count}</span></td>
                <td style="white-space:nowrap;">
                    <button class="btn-small btn-lancar-lote blue tooltipped" data-tooltip="Lançar" onclick="abrirLancar('${p.numPedido}')"><i class="material-icons">add_task</i></button>
                    <button class="btn-small cyan darken-2 waves-effect tooltipped" data-tooltip="Gerenciar" onclick="abrirGerenciador('${p.numPedido}')"><i class="material-icons">edit_note</i></button>
                    <button class="btn-small btn-sobra orange tooltipped" data-tooltip="Sobra" onclick="abrirSobra('${p.numPedido}')"><i class="material-icons">inventory_2</i></button>
                    <button class="btn-small btn-finalizar-cq green tooltipped" data-tooltip="Finalizar" onclick="abrirFinalizar('${p.numPedido}')"><i class="material-icons">check</i></button>
                </td>
            </tr>`;
        });
        M.Tooltip.init(document.querySelectorAll('.tooltipped'));
    }
}

// --- GERENCIADOR ---
function abrirGerenciador(num) {
    document.getElementById('gerenciar-num-pedido').innerText = num;
    // Busca dados para o header
    const p = todosPedidos.find(x => String(x.numPedido) === String(num));
    if(p) {
        const qtdRolo = parseFloat(String(p.qtdPorRolo).replace(/\./g,'').replace(',','.')) || 1000;
        const qtdTotal = parseFloat(String(p.quantidade).replace(/\./g,'').replace(',','.')) || 0;
        document.getElementById('gerenciar-qtd-rolo').innerText = formatNum(qtdRolo);
        document.getElementById('gerenciar-qtd-rolo').dataset.val = qtdRolo;
        document.getElementById('gerenciar-qtd-total').innerText = formatNum(qtdTotal);
        document.getElementById('gerenciar-qtd-total').dataset.val = qtdTotal;
    }
    
    cancelarEdicao();
    const selUn = document.getElementById('edit-unidade');
    const selInsp = document.getElementById('edit-inspetor');
    if(selUn.options.length <= 1) populaSelect('edit-unidade', listaUnidadesCQ);
    if(selInsp.options.length <= 1) populaSelect('edit-inspetor', listaInspetores);
    
    M.FormSelect.init(document.querySelectorAll('#area-editar-lote select'), selectOptions);
    M.Modal.getInstance(document.getElementById('modal-gerenciar')).open();
    carregarListaLotes(num);
}
function carregarListaLotes(num) {
    const tbody = document.getElementById('tbody-gerenciar');
    const loader = document.getElementById('loader-gerenciar');
    tbody.innerHTML = ""; loader.style.display = 'block';
    google.script.run.withSuccessHandler(lotes => {
        loader.style.display = 'none';
        if(lotes.length === 0) { tbody.innerHTML = "<tr><td colspan='7'>Vazio.</td></tr>"; return; }
        lotes.forEach(l => {
            const json = JSON.stringify(l).replace(/"/g, '&quot;');
            tbody.innerHTML += `
            <tr id="tr-${l.idLog}">
                <td>${l.data}</td><td>${l.unidade}</td><td>${l.inspetor}</td>
                <td style="font-weight:bold; color:#2E7D32;">${formatNum(l.qtdAprovada)}</td>
                <td style="color:#C62828; font-weight:bold;">${l.strReprova}</td>
                <td style="max-width:150px; font-size:0.8rem; overflow:hidden;">${l.motivos}</td>
                <td>
                    <button class="btn-small blue icon-btn" onclick="prepararEdicao(${json})"><i class="material-icons">edit</i></button>
                    <button class="btn-small red icon-btn" onclick="excluirLote('${l.idLog}','${num}')"><i class="material-icons">delete</i></button>
                </td>
            </tr>`;
        });
    }).getLogCompletoPedido(num);
}
function prepararEdicao(l) {
    loteEmEdicaoId = l.idLog;
    document.getElementById('edit-id-log').innerText = l.idLog;
    document.getElementById('edit-unidade').value = l.unidade;
    document.getElementById('edit-inspetor').value = l.inspetor;
    document.getElementById('edit-qtd-aprovada').value = l.qtdAprovada;
    document.getElementById('edit-peso-refugo').value = l.pesoRefugo;
    document.getElementById('edit-motivos').value = l.motivos;
    
    // Tenta preencher peso rolo
    const num = document.getElementById('gerenciar-num-pedido').innerText;
    const p = todosPedidos.find(x => String(x.numPedido) === String(num));
    document.getElementById('edit-peso-rolo').value = (p && p.pesoReferencia) ? p.pesoReferencia : "";
    
    M.FormSelect.init(document.querySelectorAll('#area-editar-lote select'), selectOptions);
    M.updateTextFields();
    calcularPerdaEdicao();
    document.getElementById('area-editar-lote').style.display = 'block';
}
function calcularPerdaEdicao() {
    const qtdAprov = parseFloat(document.getElementById('edit-qtd-aprovada').value)||0;
    const pesoRefugo = parseFloat(document.getElementById('edit-peso-refugo').value)||0;
    const pesoRolo = parseFloat(document.getElementById('edit-peso-rolo').value)||0;
    const qtdRolo = parseFloat(document.getElementById('gerenciar-qtd-rolo').dataset.val)||1000;
    const qtdTotal = parseFloat(document.getElementById('gerenciar-qtd-total').dataset.val)||0;
    const display = document.getElementById('edit-display-perda');
    
    if(qtdTotal>0 && qtdRolo>0 && pesoRolo>0) {
        const pesoTotal = (qtdTotal/qtdRolo)*pesoRolo;
        const perda = (pesoRefugo/pesoTotal)*100;
        display.innerText = perda.toFixed(2)+"%";
    } else display.innerText = "---";
}
function salvarEdicao() {
    if(!loteEmEdicaoId) return;
    const btn = document.getElementById('btn-salvar-edicao');
    const orig = btn.innerHTML;
    const pesoRef = parseFloat(document.getElementById('edit-peso-refugo').value)||0;
    const percStr = document.getElementById('edit-display-perda').innerText;
    const strFinal = `${pesoRef.toFixed(3).replace('.',',')} kg (${percStr})`;
    
    const dados = {
        idLog: loteEmEdicaoId,
        unidade: document.getElementById('edit-unidade').value,
        inspetor: document.getElementById('edit-inspetor').value,
        qtdAprovada: document.getElementById('edit-qtd-aprovada').value,
        motivos: document.getElementById('edit-motivos').value,
        strReprovaFinal: strFinal
    };
    const num = document.getElementById('gerenciar-num-pedido').innerText;
    
    btn.disabled=true; btn.innerHTML='Salvando...'; showPageLoader(true);
    google.script.run.withSuccessHandler(res => {
        showPageLoader(false); btn.disabled=false; btn.innerHTML=orig;
        if(res.sucesso) {
            M.toast({html:'Atualizado!',classes:'green'}); cancelarEdicao(); carregarListaLotes(num); carregarDados();
        } else M.toast({html:'Erro: '+res.erro,classes:'red'});
    }).editarLoteCQ(dados);
}
function cancelarEdicao() { document.getElementById('area-editar-lote').style.display='none'; loteEmEdicaoId=null; }
function excluirLote(id, num) {
    if(!confirm("Excluir?")) return;
    showPageLoader(true);
    google.script.run.withSuccessHandler(res => {
        showPageLoader(false);
        if(res.sucesso) { M.toast({html:'Excluído.',classes:'green'}); carregarListaLotes(num); carregarDados(); }
        else M.toast({html:'Erro: '+res.erro,classes:'red'});
    }).excluirLoteCQ(id);
}

// --- LANÇAR LOTE ---
function abrirLancar(id) {
    const p = todosPedidos.find(x => String(x.numPedido) === String(id));
    if(!p) return;
    let qtdRolo = 1000;
    if(p.qtdPorRolo) qtdRolo = parseFloat(String(p.qtdPorRolo).replace(/\./g,'').replace(',','.')) || 1000;
    
    document.getElementById('lote-num-pedido-label').innerText=id;
    document.getElementById('lote-num-pedido-hidden').value=id;
    document.getElementById('lote-qtd-rolo-label').innerText=formatNum(qtdRolo);
    document.getElementById('lote-qtd-rolo-hidden').value=qtdRolo;
    document.getElementById('lote-qtd-total-pedido').value=p.quantidade;
    
    document.getElementById('lote-qtd-aprovada').value="";
    document.getElementById('lote-peso-refugo').value="";
    document.getElementById('display-perc-perda').innerText="0.00%";
    document.getElementById('lote-peso-rolo').value=p.pesoReferencia||"";
    
    const sel = document.getElementById('lote-motivos'); sel.selectedIndex = -1;
    M.FormSelect.init(sel, selectOptions);
    M.Modal.getInstance(document.getElementById('modal-lancar-lote')).open();
    M.updateTextFields();
}
function calcularPerda() {
    const aprov = parseFloat(document.getElementById('lote-qtd-aprovada').value)||0;
    const qtdRolo = parseFloat(document.getElementById('lote-qtd-rolo-hidden').value)||1000;
    const pesoRolo = parseFloat(document.getElementById('lote-peso-rolo').value)||0;
    const pesoRef = parseFloat(document.getElementById('lote-peso-refugo').value)||0;
    const totalPed = parseFloat(document.getElementById('lote-qtd-total-pedido').value)||0;

    if(totalPed>0 && qtdRolo>0 && pesoRolo>0) {
        const pesoTotal = (totalPed/qtdRolo)*pesoRolo;
        const perda = (pesoRef/pesoTotal)*100;
        document.getElementById('display-perc-perda').innerText = perda.toFixed(2)+"%";
    } else document.getElementById('display-perc-perda').innerText = "0.00%";
}
function handleSalvarLote() {
    const modal = document.getElementById('modal-lancar-lote');
    const sel = document.getElementById('lote-motivos');
    let motivos = "";
    if(sel) {
        const inst = M.FormSelect.getInstance(sel);
        if(inst) motivos = inst.getSelectedValues().join(', ');
        else if(sel.selectedOptions) motivos = Array.from(sel.selectedOptions).map(o=>o.value).join(', ');
    }
    
    const dados = {
        numPedido: document.getElementById('lote-num-pedido-hidden').value,
        unidadeCQ: document.getElementById('lote-unidade-cq').value,
        inspetor: document.getElementById('lote-inspetor').value,
        qtdAprovada: document.getElementById('lote-qtd-aprovada').value,
        pesoRoloReferencia: document.getElementById('lote-peso-rolo').value,
        pesoRefugo: document.getElementById('lote-peso-refugo').value||0,
        percPerda: document.getElementById('display-perc-perda').innerText,
        motivosSelecionados: motivos
    };
    if(!dados.unidadeCQ || !dados.inspetor || !dados.qtdAprovada) { M.toast({html:'Preencha obrigatórios',classes:'red'}); return; }
    if(parseFloat(dados.pesoRefugo)>0 && !dados.motivosSelecionados) { M.toast({html:'Informe motivo',classes:'red'}); return; }
    
    M.Modal.getInstance(modal).close();
    showPageLoader(true);
    google.script.run.withSuccessHandler(()=>{
        showPageLoader(false); M.toast({html:'Lote salvo!',classes:'green'}); carregarDados();
    }).withFailureHandler(onCarregarErro).salvarNovoLoteCQ(dados);
}

// --- SOBRA ---
function abrirSobra(id) {
    document.getElementById('sobra-num-pedido-label').innerText=id;
    document.getElementById('sobra-num-pedido-hidden').value=id;
    document.getElementById('input-sobra-manual').value="";
    M.Modal.getInstance(document.getElementById('modal-sobra')).open();
}
function confirmarSobra() {
    const qtd = document.getElementById('input-sobra-manual').value;
    const id = document.getElementById('sobra-num-pedido-hidden').value;
    const btn = document.getElementById('btn-confirmar-sobra');
    const canc = document.getElementById('btn-cancelar-sobra');
    const load = document.getElementById('loader-sobra');
    if(!qtd || qtd<=0) { M.toast({html:'Inválido',classes:'red'}); return; }
    
    btn.disabled=true; canc.disabled=true; load.style.display='block';
    google.script.run.withSuccessHandler(()=>{
        M.toast({html:'Registrado!',classes:'green'});
        btn.disabled=false; canc.disabled=false; load.style.display='none';
        M.Modal.getInstance(document.getElementById('modal-sobra')).close();
    }).withFailureHandler(e=>{
        M.toast({html:'Erro: '+e.message,classes:'red'});
        btn.disabled=false; canc.disabled=false; load.style.display='none';
    })._adicionarSobraEstoquePublica(id, qtd);
}

// --- FINALIZAR ---
function abrirFinalizar(id) {
    const p = todosPedidos.find(x => String(x.numPedido) === String(id));
    const lotes = todosLotes[id] || {totalAprovado:0};
    const total = lotes.totalAprovado;
    const meta = parseFloat(String(p.quantidade).replace(/\./g,'').replace(',','.'));
    const semExc = (String(p.semExcedente).toUpperCase() === 'TRUE');
    let msg="", cor="", pode=false;
    if(semExc) {
        if(total===meta) { msg="OK! Exato."; cor="green-text"; pode=true; }
        else if(total>meta) { msg=`ERRO: Excesso.`; cor="red-text"; }
        else { msg=`ERRO: Faltam.`; cor="red-text"; }
    } else {
        const min=Math.round(meta*0.9), max=Math.round(meta*1.1);
        if(total>=min && total<=max) { msg="OK!"; cor="green-text"; pode=true; }
        else if(total>max) { msg="ERRO: >110%."; cor="red-text"; }
        else { msg="ERRO: <90%."; cor="red-text"; }
    }
    document.getElementById('finalizar-num').value=id;
    document.getElementById('finalizar-linha').value=p.linhaPlanilha;
    document.getElementById('btn-conf-finalizar').disabled=!pode;
    document.getElementById('finalizar-resumo').innerHTML = `<p>Pedido: <b>${formatNum(meta)}</b> | Aprovado: <b>${formatNum(total)}</b></p><div class="card-panel center-align ${cor} lighten-5"><strong>${msg}</strong></div>`;
    M.Modal.getInstance(document.getElementById('modal-finalizar')).open();
}
document.getElementById('btn-conf-finalizar').onclick = () => {
    const id = document.getElementById('finalizar-num').value;
    const lin = document.getElementById('finalizar-linha').value;
    showPageLoader(true);
    google.script.run.withSuccessHandler(()=>{
        M.Modal.getInstance(document.getElementById('modal-finalizar')).close();
        M.toast({html:'Finalizado!',classes:'green'}); carregarDados();
    }).withFailureHandler(onCarregarErro).finalizarControleQualidade_v2(id,lin,"");
};
function iniciar(lin) { showPageLoader(true); google.script.run.withSuccessHandler(carregarDados).salvarInicioCQ(lin); }

</script>
</body>
</html>
