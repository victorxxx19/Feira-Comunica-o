/**
 * Busca todas as Bobinas-M√ÉE dispon√≠veis para o "Corte em Lote".
 * VERS√ÉO OTIMIZADA para evitar timeouts.
 */
function buscarMaesDisponiveis(substrato, fornecedor, tipoDeCola, larguraCorteTotal) {
  try {
    const abaMae = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAMAE);

    if (!abaMae) {
      throw new Error("Aba 'Almox_BobinasMae' n√£o foi encontrada. Verifique o nome da planilha.");
    }

    if (abaMae.getLastRow() < 2) {
      return { sucesso: true, bobinas: [] };
    }

    // --- OTIMIZA√á√ÉO: LER APENAS O DATARANGE ---
    // Em vez de ler A2:J[ULTIMA_LINHA], lemos apenas os dados que existem.
    const dados = abaMae.getDataRange().getValues();
    // ------------------------------------------

    const substratoBusca = String(substrato).trim().toUpperCase();
    const fornecedorBusca = String(fornecedor).trim().toUpperCase();
    const tipoDeColaBusca = String(tipoDeCola).trim().toUpperCase();

    const bobinasDisponiveis = [];

    // Pula o cabe√ßalho (linha 0)
    for (let i = 1; i < dados.length; i++) {
      const linha = dados[i];

      // --- VERIFICA√á√ÉO DE SEGURAN√áA ---
      // Garante que a linha tem pelo menos 10 colunas (A-J)
      if (linha.length < 10) continue; 
      // --------------------------------

      const status = linha[9].toString().trim(); // Col J (√≠ndice 9)

      if (status !== "Finalizada") {
        const forn = linha[2].toString().trim().toUpperCase(); // Col C (√≠ndice 2)
        const subs = linha[3].toString().trim().toUpperCase(); // Col D (√≠ndice 3)
        const cola = linha[4].toString().trim().toUpperCase(); // Col E (√≠ndice 4)

        // Filtra por Substrato, Fornecedor e Cola
        if (subs === substratoBusca && forn === fornecedorBusca && cola === tipoDeColaBusca) {
          
          const larguraDisp = parseFloat(String(linha[7]).replace(",", ".")) || 0; // Col H (√≠ndice 7)
          const temLargura = (larguraDisp >= larguraCorteTotal); // Poka Yoke

          const comprimento = parseFloat(String(linha[6]).replace(",", ".")) || 0; // Col G (√≠ndice 6)
          
          bobinasDisponiveis.push({
            idBobinaMae: linha[0].toString().trim(), // Col A (√≠ndice 0)
            larguraDisponivel: larguraDisp,
            comprimento: comprimento,
            substrato: linha[3].toString().trim(), 
            lote: linha[1].toString().trim(), // Col B (√≠ndice 1)
            podeCortar: temLargura 
          });
        }
      }
    }
    
    bobinasDisponiveis.sort((a, b) => a.idBobinaMae.localeCompare(b.idBobinaMae));
    return { sucesso: true, bobinas: bobinasDisponiveis };

  } catch (e) {
    Logger.log(e);
    return { sucesso: false, mensagem: e.message, stack: e.stack }; // Retorna o stack para debug
  }
}

