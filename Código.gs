function registrarConsumoPorPedido(dadosConsumo) {
  const lock = LockService.getScriptLock();
  if (!lock.tryLock(15000)) {
    return { sucesso: false, mensagem: "O sistema est√° ocupado. Tente novamente." };
  }
  try {
    const abaFilha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);
    const abaLog = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_LOG_CONSUMO);
    
    const numPedido = dadosConsumo.pedidoConsumido;
    const metragemTotalSolicitada = parseFloat(String(dadosConsumo.metragemConsumida).replace(",", ".")) || 0;
    let metragemRestanteParaConsumir = metragemTotalSolicitada;
    const impressor = dadosConsumo.impressor || "N√£o informado";
    const dataAtual = new Date();
    const motivoExcesso = dadosConsumo.motivoMetragemExcessiva || ""; 

    if (metragemTotalSolicitada <= 0) {
      throw new Error("A metragem consumida deve ser um n√∫mero positivo.");
    }

    // --- OTIMIZA√á√ÉO (PROBLEMA 1): Pega o mapa de consumo UMA VEZ ---
    const consumptionMap = getConsumptionMap(); 
    
    // 2. Encontrar as bobinas-filhas alocadas, calculando o saldo real
    const dadosFilhas = abaFilha.getRange("A2:J" + abaFilha.getLastRow()).getValues();
    const bobinasAlocadas = [];

    for (let i = 0; i < dadosFilhas.length; i++) {
      const linha = dadosFilhas[i];
      const idFilha = linha[0].toString();        // Col A (ID)
      const compInicial = parseFloat(linha[6]);   // Col G (Comp_Inicial)
      const status = linha[8].toString();       // Col I (Status)
      const pedidoAlocado = linha[9].toString();  // Col J (Pedido_Alocado)
      const linhaReal = i + 2;

      // --- OTIMIZA√á√ÉO (PROBLEMA 1): Usa o mapa em vez de chamar calcularSaldoBobinaFilha() ---
      const consumoPrevio = consumptionMap[idFilha] || 0;
      const compDisponivel = compInicial - consumoPrevio;
      
      if (pedidoAlocado === numPedido && (status === "Dispon√≠vel" || status === "Em Uso") && compDisponivel > 0) {
        bobinasAlocadas.push({
          id: idFilha,
          compDisponivel: compDisponivel,
          linhaPlanilha: linhaReal
          // compInicial n√£o √© mais necess√°rio aqui
        });
      }
    }

    // Esta verifica√ß√£o j√° foi feita em 'validarConsumoMetragem', mas mantemos como seguran√ßa
    if (bobinasAlocadas.length === 0) {
      throw new Error(`Nenhuma bobina-filha dispon√≠vel (com saldo > 0) foi encontrada para o pedido ${numPedido}.`);
    }
    
    // 3. Loop de Consumo (Simula√ß√£o)
    const linhasLogParaSalvar = [];
    const bobinasAfetadas = []; // Armazena as bobinas que sofreram altera√ß√£o
    let metragemConsumidaTotalReal = 0;

    for (const bobina of bobinasAlocadas) {
      if (metragemRestanteParaConsumir <= 0) break;
      const consumoNestaBobina = Math.min(bobina.compDisponivel, metragemRestanteParaConsumir);
      
      metragemRestanteParaConsumir -= consumoNestaBobina;
      metragemConsumidaTotalReal += consumoNestaBobina;

      linhasLogParaSalvar.push([
        dataAtual, bobina.id, numPedido, consumoNestaBobina, impressor
      ]);
      
      // Adiciona a bobina afetada e seu *novo* saldo
      bobina.novoSaldo = bobina.compDisponivel - consumoNestaBobina;
      bobinasAfetadas.push(bobina);
    }
    
    // 4. VERIFICA√á√ÉO DE ESTOQUE FINAL
    if (metragemRestanteParaConsumir > 0) {
      throw new Error(`Consumo (${metragemTotalSolicitada}m) √© MAIOR que o estoque alocado (${metragemConsumidaTotalReal.toFixed(0)}m).`);
    }
    
    // 5. SALVAR O LOG
    if (linhasLogParaSalvar.length > 0) {
      abaLog.getRange(abaLog.getLastRow() + 1, 1, linhasLogParaSalvar.length, linhasLogParaSalvar[0].length)
            .setValues(linhasLogParaSalvar);
    }
    
    // 6. ATUALIZAR STATUS DAS BOBINAS (J√Å CALCULADO, S√ì ESCREVER)
    // --- OTIMIZA√á√ÉO (PROBLEMA 1) ---
    // N√£o chama mais calcularSaldoBobinaFilha() daqui
    for (const bobina of bobinasAfetadas) {
      const novoStatus = (bobina.novoSaldo < 1) ? "Finalizada" : "Em Uso";
      abaFilha.getRange(bobina.linhaPlanilha, 8).setValue(bobina.novoSaldo); // Col H (Saldo)
      abaFilha.getRange(bobina.linhaPlanilha, 9).setValue(novoStatus);  // Col I (Status)
    }
    
    // 7. ATUALIZA A METRAGEM TOTAL E REFUGO
    atualizarMetragemTotalConsumida(numPedido, motivoExcesso); 

    return { 
      sucesso: true, 
      mensagem: `Consumo de ${metragemConsumidaTotalReal.toFixed(0)}m registrado para o pedido ${numPedido}.`
    };

  } catch (e) {
    Logger.log(`Erro em registrarConsumoPorPedido: ${e.message} ${e.stack}`);
    return { sucesso: false, mensagem: e.message };
  } finally {
    lock.releaseLock();
  }
}

/**
 * NOVO HELPER: L√™ o log de consumo UMA VEZ e retorna um mapa de saldos.
 * Isso evita a lentid√£o (Problema 1).
 */
function getConsumptionMap() {
  const abaLog = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_LOG_CONSUMO);
  const consumoPorBobina = {};
  
  if (abaLog && abaLog.getLastRow() > 1) {
    // L√™ Col B (ID_Filha), Col C (Pedido), Col D (Consumo)
    const dadosLog = abaLog.getRange(2, 2, abaLog.getLastRow() - 1, 3).getValues(); 
    
    for (const linha of dadosLog) {
      const idFilha = linha[0].toString().trim(); // Col B
      const consumo = parseFloat(String(linha[2]).replace(",", ".")) || 0; // Col D
      if (idFilha) {
        consumoPorBobina[idFilha] = (consumoPorBobina[idFilha] || 0) + consumo;
      }
    }
  }
  return consumoPorBobina;
}

function atualizarMetragemTotalConsumida(numPedido, motivoExcesso = "") { // Corre√ß√£o do argumento
  try {
    const abaLog = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_LOG_CONSUMO);
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);

    // 1. SOMA O CONSUMO TOTAL DO LOG
    let consumoTotalAcumulado = 0;
    if (abaLog.getLastRow() > 1) {
      const dadosLog = abaLog.getRange("C2:D" + abaLog.getLastRow()).getValues(); 
      for (const linhaLog of dadosLog) {
        const pedidoNoLog = linhaLog[0].toString().trim(); 
        const consumo = parseFloat(String(linhaLog[1]).replace(",", ".")) || 0; 
        if (pedidoNoLog === numPedido && consumo > 0) {
          consumoTotalAcumulado += consumo;
        }
      }
    }

    if (consumoTotalAcumulado === 0) return; 

    // 2. ENCONTRA A LINHA DO PEDIDO NA ABA "Pedidos"
    // Lemos at√© a coluna COL_PEDIDO_METRAGEM (AC) para obter a metragem calculada original
    const dadosPedidos = abaPedidos.getRange(2, COL_PEDIDO_NUMERO, abaPedidos.getLastRow() - 1, COL_PEDIDO_METRAGEM).getValues();
    let linhaParaAtualizar = -1;
    let metragemCalculada = 0;

    for (let i = 0; i < dadosPedidos.length; i++) {
      if (dadosPedidos[i][COL_PEDIDO_NUMERO - 1].toString().trim() === numPedido) {
        linhaParaAtualizar = i + 2; 
        metragemCalculada = parseFloat(String(dadosPedidos[i][COL_PEDIDO_METRAGEM - 1]).replace(",", ".")) || 0;
        break;
      }
    }

    // 3. CALCULA O REFUGO E SALVA NA ABA PEDIDOS
    if (linhaParaAtualizar !== -1) {
      const refugoMetragem = consumoTotalAcumulado - metragemCalculada;
      
      // Salva na COL_PEDIDO_METRAGEM_REAL (AN / 40)
      abaPedidos.getRange(linhaParaAtualizar, COL_PEDIDO_METRAGEM_REAL).setValue(consumoTotalAcumulado);
      
      // Salva o Motivo de Excesso (AP / 42)
      if (motivoExcesso && motivoExcesso.trim() !== "") {
          abaPedidos.getRange(linhaParaAtualizar, COL_PEDIDO_MOTIVO_MAIS_METRAGEM).setValue(motivoExcesso);
      }
      
      // Salva o Refugo (AQ / 43)
      abaPedidos.getRange(linhaParaAtualizar, COL_PEDIDO_REFUGO_METRAGEM).setValue(refugoMetragem);
      
      Logger.log(`Metragem real (${consumoTotalAcumulado}m), Motivo Excesso: ${motivoExcesso || 'Nenhum'} e Refugo (${refugoMetragem}m) atualizados.`);
    } else {
      Logger.log(`ERRO: N√£o foi poss√≠vel encontrar a linha do pedido ${numPedido} na aba Pedidos.`);
    }

  } catch (e) {
    Logger.log(`ERRO GRAVE ao atualizar metragem/refugo para o pedido ${numPedido}: ${e.message}`);
  }
}


function limparBobinasSobra(numPedido) {
  try {
    const numPedidoString = String(numPedido).trim();
    Logger.log(`LIMPANDO SOBRAS para o Pedido: "${numPedidoString}"`);
    
    const abaFilha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);
    if (!abaFilha || abaFilha.getLastRow() < 2) return ""; 

    // L√™ as colunas ID_Filha, Status e Pedido_Alocado (Col A, H, I)
    const dados = abaFilha.getRange(2, 1, abaFilha.getLastRow() - 1, 10).getValues();
    
    let bobinasLimpas = 0;
    let linhasParaAtualizar = []; // Coleta as linhas para atualizar de uma vez
    const statusLimpar = "Em Uso"; 
    
    for (let i = 0; i < dados.length; i++) {
      const statusPlanilha = String(dados[i][8]).trim();      // Col H (Status - √≠ndice 7)
      const pedidoAlocadoPlanilha = String(dados[i][9]).trim(); // Col I (Pedido_Alocado - √≠ndice 8)

      // Se a bobina est√° 'Em Uso' e alocada para o pedido que est√° sendo conclu√≠do
      if (pedidoAlocadoPlanilha === numPedidoString && statusPlanilha === statusLimpar) {
        
        const linhaReal = i + 2;
        Logger.log(`Match! Linha ${linhaReal}. Bobina "${statusLimpar}" do pedido ${numPedidoString} ser√° devolvida.`);
        
        // Altera o Status (H) e Pedido_Alocado (I) no array de dados
        dados[i][8] = "Dispon√≠vel"; 
        dados[i][9] = ""; // Limpa a aloca√ß√£o
        
        bobinasLimpas++;
      }
    }
    
    // Se houve altera√ß√µes, reescreve a √°rea afetada.
    if (bobinasLimpas > 0) {
      // Otimiza√ß√£o: A bobina filha tem 9 colunas. 
      abaFilha.getRange(2, 1, abaFilha.getLastRow() - 1, 10).setValues(dados);
      SpreadsheetApp.flush(); // For√ßa a aplica√ß√£o das mudan√ßas
      const msg = `(e ${bobinasLimpas} bobina(s) de sobra foram devolvidas ao estoque.)`;
      Logger.log(`SUCESSO: ${msg}`);
      return msg;
    } else {
      Logger.log(`AVISO: Nenhuma bobina "Em Uso" encontrada para o Pedido ${numPedidoString}.`);
      return "";
    }

  } catch (e) {
    // CAPTURA E RETORNA A MENSAGEM DE ERRO SEM INTERROMPER O SALVAMENTO PRINCIPAL
    Logger.log(`ERRO GRAVE ao limpar sobras do Pedido ${numPedido}: ${e.message} ${e.stack}`);
    return `(Erro ao limpar sobras: ${e.message})`;
  }
}
