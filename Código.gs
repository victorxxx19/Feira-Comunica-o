function _validarAniloxPreenchido(codigoProduto, maquina) {
  try {
    // <<< MUDAN√áA AQUI >>>
    // Se a m√°quina for Digital, ela n√£o usa anilox. A valida√ß√£o passa.
    if (maquina === "Flexo Digital") {
      return true;
    }
    // --- Fim da Mudan√ßa ---

    const abaProdutos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PRODUTOS);
    const mapa = _criarMapaDeColunas(abaProdutos);
    const idxCodigo = mapa["C√≥digo do Produto"];
    const idxFicha = mapa["FichaTecnicaCores"];
    const idxQtdCores = mapa["Quantidade de Cores"];
    
    if (idxFicha === undefined || idxQtdCores === undefined) {
      Logger.log("Aviso: Colunas 'FichaTecnicaCores' ou 'Quantidade de Cores' n√£o encontradas. Pulando valida√ß√£o de Anilox.");
      return true;
    }

    const dados = abaProdutos.getRange(2, 1, abaProdutos.getLastRow() - 1, abaProdutos.getLastColumn()).getValues();

    for (const linha of dados) {
      if (linha[idxCodigo] == codigoProduto) {
        const qtdCores = parseInt(linha[idxQtdCores], 10) || 0;
        
        if (qtdCores <= 1) return true; // 0 ou 1 cor (s√≥ acabamento) n√£o precisa anilox.

        const numEstacoes = qtdCores - 1;
        const fichaJSON = linha[idxFicha] || "[]";
        const ficha = JSON.parse(fichaJSON);
        
        if (ficha.length < numEstacoes) return false; // Ficha n√£o tem esta√ß√µes suficientes

        for (let i = 0; i < numEstacoes; i++) {
          // Se a esta√ß√£o tem uma cor definida (n√£o √© vazia), o anilox √© obrigat√≥rio
          if (ficha[i] && ficha[i].cor && !ficha[i].anilox) {
            return false; // Falha: Cor definida, mas Anilox vazio
          }
        }
        return true; // Sucesso
      }
    }
    return false; // Produto n√£o encontrado
  } catch (e) {
    Logger.log("Erro ao validar Anilox: " + e.message);
    return false; // Assume falha
  }
}
   
function finalizarProducaoPedido(linhaPlanilha, motivoAtraso = "") { 
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const linha = parseInt(linhaPlanilha);
    
    // 1. Leitura dos dados (lendo todas as colunas)
    const dadosLinha = abaPedidos.getRange(linha, 1, 1, TOTAL_COLUNAS_PEDIDOS).getValues()[0];
    const numPedido = dadosLinha[COL_PEDIDO_NUMERO - 1];
    const codProduto = dadosLinha[COL_PEDIDO_PRODUTO_CODIGO - 1]; 
    const faca = dadosLinha[COL_PEDIDO_FACA - 1]; 
    const maquina = dadosLinha[COL_PEDIDO_MAQUINA - 1];
    const cliente = dadosLinha[COL_PEDIDO_CLIENTE - 1];
    const descricao = dadosLinha[COL_PEDIDO_DESCRICAO - 1];
    const statusAtual = dadosLinha[COL_PEDIDO_STATUS - 1];
    
    // *** NOVAS LEITURAS (ETAPA A) ***
    const idUnicoFaca = dadosLinha[COL_PEDIDO_ID_UNICO_FACA - 1]; // Col AV (48)
    const metrosConsumidos = parseFloat(dadosLinha[COL_PEDIDO_METRAGEM_REAL - 1]) || 0; // Col AN (40)

    // 2. Valida√ß√£o de Anilox (agora passa a m√°quina)
    const aniloxOK = _validarAniloxPreenchido(codProduto, maquina); 
    if (!aniloxOK) {
      throw new Error(`N√£o √© poss√≠vel finalizar: A Ficha T√©cnica (Anilox) do Produto ${codProduto} est√° incompleta. Inicie o Setup (‚öôÔ∏è) para preench√™-la.`);
    }

    // 3. Valida√ß√£o de Consumo (seu c√≥digo existente)
    const consumoRegistrado = verificarConsumoRegistrado(numPedido);
    if (consumoRegistrado === false) {
      throw new Error(`N√£o √© poss√≠vel finalizar: Nenhum consumo de material (metragem) foi registrado para o Pedido ${numPedido}.`);
    }

    // 4. Salva Data Fim Produ√ß√£o (Impress√£o) e Motivo Atraso
    abaPedidos.getRange(linha, COL_PEDIDO_DATA_FIM_PROD).setValue(new Date()); // Col AM
    if (motivoAtraso && motivoAtraso.trim() !== "") {
      abaPedidos.getRange(linha, COL_PEDIDO_MOTIVO_ATRASO).setValue(motivoAtraso.trim()); // Col Z
    }
    
    // 5. ROTEIRO (Flexo, Digital, Batida)
    let novoStatus = "";
    let mensagemDeRetorno = "";
    
    if (maquina === "Flexo Digital") {
      novoStatus = "Aguardando Corte (Digital)";
      mensagemDeRetorno = "Impress√£o Digital finalizada e enviada para o Corte.";
    }
    else if (faca && faca.trim().toUpperCase().startsWith("E-")) {
      novoStatus = "Aguardando Batida";
      mensagemDeRetorno = "Produ√ß√£o finalizada e enviada para a Batida.";
    } else {
      novoStatus = (statusAtual === "Controlando") ? "Controlando" : "Produ√ß√£o Finalizada";
      mensagemDeRetorno = (statusAtual === "Controlando") ? "Produ√ß√£o finalizada. Pedido permanece 'Controlando'." : "Produ√ß√£o finalizada e enviada para a Qualidade.";
    }
    
    abaPedidos.getRange(linha, COL_PEDIDO_STATUS).setValue(novoStatus);
    
    // 6. *** NOVA ETAPA A: REGISTRAR USO DA FACA ***
    // (A fun√ß√£o _registrarUsoDeFaca vai ignorar automaticamente se a faca for "Digital")
    _registrarUsoDeFaca(numPedido, idUnicoFaca, metrosConsumidos);
    
    SpreadsheetApp.flush();
    
    // 7. Enviar E-mail (Seu c√≥digo original)
    try {
      const assunto = `Produ√ß√£o Finalizada: Pedido ${numPedido}`;
      const titulo = "Produ√ß√£o Finalizada";
      const corpoHtml = `<p>A etapa de produ√ß√£o do pedido abaixo foi conclu√≠da:</p><p><strong>N¬∫ Pedido:</strong> ${numPedido}</p><p><strong>Cliente:</strong> ${cliente}</p><p><strong>Descri√ß√£o:</strong> ${descricao}</p><p><strong>Status Atual:</strong> ${novoStatus}</p><br><p>O pedido seguir√° para a pr√≥xima etapa.</p>`;
      enviarEmailSistema(assunto, titulo, corpoHtml); 
    } catch (e) {
      Logger.log(`Falha ao ENVIAR e-mail de finaliza√ß√£o de produ√ß√£o: ${e}`);
    }
    
    return mensagemDeRetorno;
    
  } catch (e) {
    Logger.log(`Erro ao finalizar produ√ß√£o (Linha ${linhaPlanilha}): ${e}`);
    throw new Error(e.message);
  }
}
/**
 * ==================================================================
 * FUN√á√ÉO DE SALVAMENTO - FILA DE PRODU√á√ÉO (NOVA)
 * ==================================================================
 * Salva as altera√ß√µes manuais feitas no FormProducao
 * (fila, m√°quina, impressor, fornecedor/lote manual, obsPCP, status)
 */
function salvarMudancasProducao(pedidosAtualizados) {
  if (!pedidosAtualizados || pedidosAtualizados.length === 0) {
    return "Nenhuma altera√ß√£o para salvar.";
  }
  const lock = LockService.getScriptLock();
  if (!lock.tryLock(10000)) {
    throw new Error("O sistema est√° ocupado salvando dados. Tente novamente.");
  }
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const ultimoLinha = abaPedidos.getLastRow();
    if (ultimoLinha < 2) throw new Error("Nenhum dado para atualizar.");

    const ultimaColuna = TOTAL_COLUNAS_PEDIDOS; // 39
    const range = abaPedidos.getRange(2, 1, ultimoLinha - 1, ultimaColuna);
    const dados = range.getValues();

    let alteracoes = 0;

    // 2. Modifica o array 'dados' (em mem√≥ria, muito r√°pido)
    for (const pedido of pedidosAtualizados) {
      const indiceArray = pedido.linhaPlanilha - 2; 

      if (dados[indiceArray]) { // Verifica se o √≠ndice √© v√°lido
        
        // Verifica cada campo enviado pelo frontend e atualiza
        // Usar '!== undefined' permite salvar valores em branco (ex: "Desprogramar")
        
        if (pedido.status !== undefined) {
          dados[indiceArray][COL_PEDIDO_STATUS - 1] = pedido.status;
        }
        if (pedido.maquina !== undefined) {
          dados[indiceArray][COL_PEDIDO_MAQUINA - 1] = pedido.maquina;
        }
        if (pedido.fila !== undefined) {
          dados[indiceArray][COL_PEDIDO_FILA - 1] = pedido.fila;
        }
        if (pedido.obsPCP !== undefined) {
          dados[indiceArray][COL_PEDIDO_OBS_PCP - 1] = pedido.obsPCP;
        }
        if (pedido.fornecedorSubstrato !== undefined) {
          dados[indiceArray][COL_PEDIDO_FORNECEDOR_SUBSTRATO - 1] = pedido.fornecedorSubstrato;
        }
        if (pedido.loteSubstrato !== undefined) {
          dados[indiceArray][COL_PEDIDO_LOTE_SUBSTRATO - 1] = pedido.loteSubstrato;
        }
        if (pedido.impressor !== undefined) {
          dados[indiceArray][COL_PEDIDO_IMPRESSOR - 1] = pedido.impressor;
        }
        
        alteracoes++;
      }
    }

    // 3. Salva TODOS os dados de volta na planilha de uma s√≥ vez
    if (alteracoes > 0) {
      range.setValues(dados);
      SpreadsheetApp.flush();
    }
    
    return `${alteracoes} altera√ß√µes foram salvas com sucesso!`;
  } catch (e) {
    Logger.log(`Erro ao salvar mudan√ßas da Produ√ß√£o: ${e}`);
    throw new Error(`Erro ao salvar: ${e.message}`);
  } finally {
    lock.releaseLock();
  }
}

/**
 * (FUN√á√ÉO FALTANTE)
 * Salva a m√°quina e o status de um pedido e calcula a pr√≥xima fila.
 * (Chamada pelo FormProducao.html)
 */
function salvarProgramacaoMaquina(linhaPlanilha, nomeMaquina) {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const linha = parseInt(linhaPlanilha);
    
    // CASO 1: DESPROGRAMAR (Bot√£o "Selecione" ou vazio)
    if (!nomeMaquina || nomeMaquina === "") {
      abaPedidos.getRange(linha, COL_PEDIDO_MAQUINA).setValue("");
      abaPedidos.getRange(linha, COL_PEDIDO_FILA).setValue("");
      abaPedidos.getRange(linha, COL_PEDIDO_STATUS).setValue("Liberado para produ√ß√£o");
      SpreadsheetApp.flush();
      return { sucesso: true, novoStatus: "Liberado para produ√ß√£o", novaFila: "" };
    }
    
    // CASO 2: PROGRAMAR (Selecionou uma m√°quina)
    
    // 1. Achar a maior fila existente para ESSA m√°quina
    // (L√™ Col W (M√°quina) e X (Fila))
    const dados = abaPedidos.getRange(2, COL_PEDIDO_MAQUINA, abaPedidos.getLastRow() - 1, 2).getValues(); 
    let maxFila = 0;
    
    for(let i=0; i < dados.length; i++) {
        // Ignora a linha atual que estamos editando
        if ((i + 2) !== linha) { 
          if (dados[i][0] === nomeMaquina) { // dados[i][0] √© a Col M√°quina
            const filaAtual = parseInt(dados[i][1]); // dados[i][1] √© a Col Fila
            if (!isNaN(filaAtual) && filaAtual > maxFila) {
              maxFila = filaAtual;
            }
          }
        }
    }
    const novaFila = maxFila + 1; // O pr√≥ximo n√∫mero da fila
    
    // 2. Salvar os tr√™s campos
    abaPedidos.getRange(linha, COL_PEDIDO_MAQUINA).setValue(nomeMaquina);     // Col W (23)
    abaPedidos.getRange(linha, COL_PEDIDO_STATUS).setValue("Programado");     // Col I (9)
    abaPedidos.getRange(linha, COL_PEDIDO_FILA).setValue(novaFila);           // Col X (24)
    
    SpreadsheetApp.flush();
    
    // Retorna a nova fila para o FormProducao atualizar a tela
    return { sucesso: true, novoStatus: "Programado", novaFila: novaFila };
    
  } catch (e) {
    Logger.log(`Erro ao salvar m√°quina (Linha ${linhaPlanilha}): ${e}`);
    throw new Error(`Erro no servidor: ${e.message}`);
  }
}


