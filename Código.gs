 */
function buscarBobinasDisponiveis(substrato, fornecedor, tipoDeCola) { // <-- Aceita Fornecedor e Cola
  try {
    const abaFilha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);
    
    if (!abaFilha) { // Verifica√ß√£o de seguran√ßa
        throw new Error(`Aba "${NOME_ABA_ALMOX_BOBINAFILHA}" n√£o encontrada.`);
    }

    if (abaFilha.getLastRow() < 2) {
      return { sucesso: true, bobinas: [] }; // Retorna vazio, n√£o um erro
    }

    const substratoBusca = String(substrato).trim().toUpperCase();
    const fornecedorBusca = String(fornecedor).trim().toUpperCase();
    const tipoDeColaBusca = String(tipoDeCola).trim().toUpperCase();

    const dados = abaFilha.getRange("A2:J" + abaFilha.getLastRow()).getValues(); // L√™ at√© Col J
    const bobinasDisponiveis = [];

    // Precisamos do mapa de Lote -> Fornecedor
    const loteParaFornecedorMap = {}; // <-- Definido corretamente
    const abaMae = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAMAE);
    
    if (abaMae && abaMae.getLastRow() > 1) { // Verifica se abaMae existe
      const dadosMaeLotes = abaMae.getRange(2, 2, abaMae.getLastRow() - 1, 2).getValues();
      for (const linha of dadosMaeLotes) {
        loteParaFornecedorMap[linha[0].toString().trim()] = linha[1].toString().trim().toUpperCase();
      }
    }

    for (let i = 0; i < dados.length; i++) {
      const linha = dados[i];
      const substratoPlanilha = linha[3].toString().trim().toUpperCase(); // Col D
      const status = linha[8].toString().trim(); // Col I
      const colaPlanilha = linha[4].toString().trim().toUpperCase(); // Col E
      
      // Filtra por Substrato E por Status "Dispon√≠vel" E Tipo de Cola
      if (substratoPlanilha === substratoBusca && status === "Dispon√≠vel" && colaPlanilha === tipoDeColaBusca) {

        const lote = linha[2].toString().trim(); // Col C
        
        // =======================================================
        // --- CORRE√á√ÉO: Erro de digita√ß√£o 'loteParaForneorMap' ---
        const fornPlanilha = (loteParaFornecedorMap[lote] || "N/D");
        // =======================================================

        // Se o fornecedor bate (ou se o usu√°rio n√£o filtrou "Todos")
        if (fornecedorBusca === "" || fornPlanilha === fornecedorBusca) {
          const idPlanilha = linha[0].toString().trim().toUpperCase(); // Col A
          const compInicial = parseFloat(String(linha[6]).replace(",", ".")) || 0; // Col G

          const saldoReal = calcularSaldoBobinaFilha(idPlanilha, compInicial);

          if (saldoReal >= 1) {
            bobinasDisponiveis.push({
              idBobinaFilha: idPlanilha,
              substrato: linha[3].toString().trim(), // Col D
              largura: parseFloat(String(linha[5]).replace(",", ".")).toFixed(3), // Col F
              saldoReal: saldoReal
            });
          }
        }
      }
    }

    bobinasDisponiveis.sort((a, b) => a.idBobinaFilha.localeCompare(b.idBobinaFilha));
    return { sucesso: true, bobinas: bobinasDisponiveis };

  } catch (e) {
    Logger.log(e);
    return { sucesso: false, mensagem: e.message, stack: e.stack };
  }
}


