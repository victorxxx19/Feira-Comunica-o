 * Acabamento e Cold para preencher as colunas antigas (O e P).
 * @param {string} fichaJSON A string JSON da FichaTecnicaCores.
 * @returns {object} {acabamento: string, cold: string}
 */
function _extrairAcabamentoECold(fichaJSON) {
  let acabamento = "";
  let cold = "";
  
  try {
    const ficha = JSON.parse(fichaJSON || "[]");
    
    // Lista de tipos de l√¢mina
    const tiposAcabamento = ["Verniz Brilho", "Verniz Fosco", "Reserva de Verniz", "Lamina√ß√£o Brilho", "Lamina√ß√£o Fosca", "Neutra"];
    const tiposCold = ["Cold Ouro", "Cold Prata"];

    // Encontra o primeiro de cada
    for (const lamina of ficha) {
      const tipo = lamina.cor;
      if (!acabamento && tiposAcabamento.includes(tipo)) {
        acabamento = tipo;
      }
      if (!cold && tiposCold.includes(tipo)) {
        cold = tipo;
      }
    }
    
    // Se nenhum 'Cold' foi encontrado, o padr√£o √© 'Sem Cold'
    if (!cold) {
       cold = "Sem Cold";
    }

  } catch (e) {
    Logger.log("Erro ao extrair acabamento/cold do JSON: " + e.message);
  }
  
  return { acabamento, cold };
}

