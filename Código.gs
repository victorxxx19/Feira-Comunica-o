/**
 * (SUBSTITUIR - V4.2 - L√ìGICA DE PRIORIDADE)
 * Verifica o status de TODAS as l√¢minas de um clich√™ e atualiza o Status Geral
 * do Cliche_Mestre com base na prioridade:
 * Danificada > Em Reposi√ß√£o > Em Fabrica√ß√£o > Dispon√≠vel
 */
function _verificarEAtualizarStatusMestre(idCliche, abaMestre, abaLaminas) {
  Logger.log(`Verificando status mestre para: ${idCliche}`);
  
  // Lista de tipos de l√¢mina a IGNORAR na verifica√ß√£o de status
  const tiposExcluidos = [
      "VERNIZ BRILHO", 
      "VERNIZ FOSCO", 
      "LAMINA√á√ÉO BRILHO", 
      "LAMINA√á√ÉO FOSCA", 
      "NEUTRA"
  ];
  
  const dadosLaminas = abaLaminas.getRange(2, 1, abaLaminas.getLastRow() - 1, 6).getValues();
  
  // Vari√°veis de prioridade
  let temDanificada = false;
  let temReposicao = false;
  let temFabricacao = false;
  let temDisponivel = false;
  let encontrouAlgumaLaminaContavel = false;
  
  for (const lamina of dadosLaminas) {
    // Se a l√¢mina pertence ao clich√™ que estamos verificando
    if (lamina[1] == idCliche) { // Col B (ID_Cliche)
      const tipoLamina = (lamina[3] || "").toUpperCase().trim(); // Col D (Tipo)
      
      // Ignora l√¢minas de acabamento
      if (tiposExcluidos.includes(tipoLamina)) {
        continue;
      }
      
      // Se chegamos aqui, √© uma l√¢mina "cont√°vel"
      encontrouAlgumaLaminaContavel = true;
      const statusLamina = lamina[5]; // Coluna F (Status_Lamina)
      
      // Verifica os status em ordem de prioridade
      if (statusLamina === "Danificada") {
        temDanificada = true;
        break; // Status de prioridade m√°xima, n√£o precisa checar mais nada
      }
      if (statusLamina === "Em Reposi√ß√£o") {
        temReposicao = true;
      }
      if (statusLamina === "Em Fabrica√ß√£o") {
        temFabricacao = true;
      }
      if (statusLamina === "Dispon√≠vel") {
        temDisponivel = true;
      }
    }
  }
  
  // Se n√£o achou nenhuma l√¢mina (ex: clich√™ s√≥ de verniz), n√£o faz nada
  if (!encontrouAlgumaLaminaContavel) {
    Logger.log(`‚Üí ${idCliche} n√£o possui l√¢minas cont√°veis. Status mestre mantido.`);
    return; // Sai da fun√ß√£o
  }

  // Define o novo status mestre baseado na prioridade
  let novoStatusMestre = "";
  if (temDanificada) {
    novoStatusMestre = "Com Problema";
  } else if (temReposicao) {
    novoStatusMestre = "Em Reposi√ß√£o";
  } else if (temFabricacao) {
    novoStatusMestre = "Em Fabrica√ß√£o";
  } else if (temDisponivel) {
    // S√≥ fica "Dispon√≠vel" se TODAS as l√¢minas cont√°veis estiverem dispon√≠veis
    // (ou seja, nenhuma danificada, em reposi√ß√£o ou em fabrica√ß√£o)
    novoStatusMestre = "Dispon√≠vel";
  } else {
    // Caso de seguran√ßa: se n√£o tiver nenhuma l√¢mina ou status estranho
    novoStatusMestre = "Indefinido"; 
  }

  Logger.log(`‚úì Status Mestre para ${idCliche} definido como: ${novoStatusMestre}`);

  // Atualiza o Status Geral do Clich√™ Mestre (Coluna G)
  const dadosMestre = abaMestre.getRange(2, 1, abaMestre.getLastRow() - 1, 7).getValues();
  for (let i = 0; i < dadosMestre.length; i++) {
    if (dadosMestre[i][0] == idCliche) {
      const linhaMestre = i + 2;
      abaMestre.getRange(linhaMestre, 7).setValue(novoStatusMestre); // Coluna G (Status_Geral)
      break;
    }
  }
}

