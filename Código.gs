}


function receberPedidoFaca(dadosRecebimento) {
  const lock = LockService.getScriptLock();
  lock.waitLock(10000);
  
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const abaPedidos = ss.getSheetByName(NOME_ABA_FACAS_PEDIDOS);
    const abaInventario = ss.getSheetByName(NOME_ABA_FACAS_INVENTARIO);
    
    const { idPedidoFaca, idUnicoFaca, dataRecebimento, valorNF, numNF } = dadosRecebimento;
    const dataReceb = new Date(dataRecebimento.split('/').reverse().join('-'));
    let codFacaModelo = "";

    // 1. Atualiza o Facas_Pedidos (Log de Compra)
    const dadosPedidos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, 12).getDisplayValues();
    for (let i = 0; i < dadosPedidos.length; i++) {
      if (dadosPedidos[i][0] == idPedidoFaca) { // Col A (ID_Pedido_Faca)
        const linhaPedido = i + 2;
        const dataPrevista = dadosPedidos[i][6]; // Col G
        const statusReceb = _calcularStatusRecebimento(dataPrevista, dataRecebimento); // Reutiliza a fun√ß√£o do clich√™
        
        // Atualiza Data Receb(H), Status(I), Valor(J), NF(K)
        abaPedidos.getRange(linhaPedido, 8, 1, 4).setValues([
          [ dataReceb, statusReceb, 
            parseFloat(String(valorNF).replace(",", ".")) || 0,
            numNF
          ]
        ]);
        break;
      }
    }

    // 2. Atualiza o Facas_Inventario
    const dadosInv = abaInventario.getRange(2, 1, abaInventario.getLastRow() - 1, 6).getValues(); // A-F
    for (let i = 0; i < dadosInv.length; i++) {
      if (dadosInv[i][0] == idUnicoFaca) { // Col A (ID_Unico_Faca)
        const linhaInv = i + 2;
        codFacaModelo = dadosInv[i][1]; // Salva o modelo para o passo 3
        
        // Atualiza Status (C) para "Dispon√≠vel" e Data_Entrada (E)
        abaInventario.getRange(linhaInv, 3).setValue("Dispon√≠vel");
        abaInventario.getRange(linhaInv, 5).setValue(dataReceb);
        break;
      }
    }
    
    // 3. Atualiza o status agregado do Modelo na aba "Facas"
    if (codFacaModelo) {
      _atualizarStatusAgregadoFaca(codFacaModelo);
    }
    
    SpreadsheetApp.flush();
    
    // 4. [ETAPA B] GATILHO DE ATUALIZA√á√ÉO DE PEDIDOS
    // Dispara a verifica√ß√£o para este MODELO de faca que agora tem um item "Dispon√≠vel"
    if (codFacaModelo) {
      _atualizarPedidosPendentesPorInsumo(null, codFacaModelo);
    }
    
    return buscarDadosIniciaisFacas(); // Retorna os dados atualizados

  } catch (e) {
    Logger.log(`Erro ao Receber Pedido de Faca: ${e.message}`);
    throw new Error(`Erro no servidor: ${e.message}`);
  } finally {
    lock.releaseLock();
  }
}

/**
 * 7. Reporta um problema em uma faca (muda status e loga)
 * (Fun√ß√£o original que estava faltando)
 */
function reportarProblemaFaca(idUnicoFaca, motivoProblema) {
  const lock = LockService.getScriptLock();
  lock.waitLock(10000);

  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const abaInventario = ss.getSheetByName(NOME_ABA_FACAS_INVENTARIO);
    const abaProblemas = ss.getSheetByName(NOME_ABA_FACAS_PROBLEMAS);
    let codFacaModelo = "";
    
    // 1. Atualiza o Facas_Inventario para "Danificada"
    const dadosInv = abaInventario.getRange(2, 1, abaInventario.getLastRow() - 1, 6).getValues(); // A-F
    for (let i = 0; i < dadosInv.length; i++) {
      if (dadosInv[i][0] == idUnicoFaca) { // Col A
        codFacaModelo = dadosInv[i][1]; // Col B (Modelo)
        abaInventario.getRange(i + 2, 3).setValue("Danificada"); // Col C (Status)
        break;
      }
    }
    
    if (!codFacaModelo) {
      throw new Error("Faca n√£o encontrada no invent√°rio.");
    }

    // 2. Salva o registro na aba Facas_Problemas
    const dataHoje = new Date();
    const idProblema = `PR-Faca-${String(abaProblemas.getLastRow() + 1).padStart(4, '0')}`;
    
    abaProblemas.appendRow([
      idProblema,
      idUnicoFaca,
      dataHoje,
      motivoProblema // (MUDAN√áA) Salva o motivo formatado (ex: "Faca Cega: ...")
    ]);

    // 3. Atualiza o status agregado do Modelo na aba "Facas"
    _atualizarStatusAgregadoFaca(codFacaModelo);
    
    SpreadsheetApp.flush();
    return buscarDadosIniciaisFacas(); // Retorna os dados atualizados

  } catch (e) {
    Logger.log(`Erro ao Reportar Problema de Faca: ${e.message}`);
    throw new Error(`Erro no servidor: ${e.message}`);
  } finally {
    lock.releaseLock();
  }
}


/**
