// Fun√ß√£o auxiliar para criar um objeto de estat√≠sticas zerado
function _initClicheStats() {
  return {
    // Contagens
    countTotalPedidos: 0,
    countNovo: 0,
    countErroInterno: 0,
    countErroClicheria: 0,
    countRecebidos: 0,
    countNoPrazo: 0,
    countAtrasado: 0,
    
    // Custos
    costTotal: 0,
    costNovo: 0,
    costDesgaste: 0,
    costAlteracao: 0,
    costErroInterno: 0,
    costErroClicheria: 0,
    
    // Tempo
    totalLeadTimeDias: 0
  };
}

// Fun√ß√£o principal que ser√° chamada pelo getDadosDashboard()
function getDadosDashboard_Cliches() {
  const ss = SpreadsheetApp.getActiveSpreadsheet();
  const abaPedidos = ss.getSheetByName(NOME_ABA_CLICHE_PEDIDOS);
  
  if (!abaPedidos) {
    throw new Error("Aba de Pedidos de Clich√™ n√£o encontrada: " + NOME_ABA_CLICHE_PEDIDOS);
  }

  const dadosPedidos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, 15).getValues();
  
  const statsPorMes = {};
  const statsPorClicheria = {};
  const statsTotal = _initClicheStats();
  const anoAtual = new Date().getFullYear(); // <-- ADICIONADO
  
  const hoje = new Date();
  hoje.setHours(0, 0, 0, 0);

  for (const row of dadosPedidos) {
    try {
      const dataSolicitacao = new Date(row[4]);
      if (isNaN(dataSolicitacao.getTime())) continue; // Pula linha se data for inv√°lida

      // Filtra pelo ano atual
      if (dataSolicitacao.getFullYear() !== anoAtual) continue;

      const mesKey = dataSolicitacao.toISOString().slice(0, 7); // ex: "2025-11"
      const fornecedor = row[3] || "N/A";
      const tipoPedido = row[13] || "N/A";
      const statusReceb = row[8];
      const valor = parseFloat(row[9] || 0);
      const dataRecebimento = row[7] ? new Date(row[7]) : null;
      
      // Inicializa os acumuladores se n√£o existirem
      if (!statsPorMes[mesKey]) {
        statsPorMes[mesKey] = _initClicheStats();
      }
      if (!statsPorClicheria[fornecedor]) {
        statsPorClicheria[fornecedor] = _initClicheStats();
      }
      
      // Acumula nos 3 objetos (Total, M√™s, Clicheria)
      [statsTotal, statsPorMes[mesKey], statsPorClicheria[fornecedor]].forEach(stats => {
        stats.countTotalPedidos++;
        stats.costTotal += valor;

        // Contagem e Custo por Tipo
        switch (tipoPedido) {
          case "Clich√™ Novo":
            stats.countNovo++;
            stats.costNovo += valor;
            break;
          case "Regrava√ß√£o por Desgaste":
            stats.costDesgaste += valor;
            break;
          case "Altera√ß√£o":
            stats.costAlteracao += valor;
            break;
          case "Regrava√ß√£o por Erro": // Erro Interno
          case "Regrava√ß√£o Jogo Inteiro (Erro)":
            stats.countErroInterno++;
            stats.costErroInterno += valor;
            break;
          case "Reposi√ß√£o por Erro": // Erro Clicheria
            stats.countErroClicheria++;
            stats.costErroClicheria += valor;
            break;
        }

        // Status de Recebimento (s√≥ conta se n√£o estiver pendente)
        if (statusReceb === "No Prazo") {
          stats.countNoPrazo++;
        } else if (statusReceb === "Atrasado") {
          stats.countAtrasado++;
        }
        
        // C√°lculo de Lead Time (s√≥ para pedidos j√° recebidos)
        if (dataRecebimento) {
          stats.countRecebidos++;
          const diffTime = dataRecebimento.getTime() - dataSolicitacao.getTime();
          const diffDays = diffTime / (1000 * 60 * 60 * 24);
          stats.totalLeadTimeDias += diffDays;
        }
      });
      
    } catch (e) {
      Logger.log(`Erro ao processar linha de pedido de clich√™: ${e.message} - Linha: ${row}`);
    }
  }
  
  // ===================================
  // 2. FORMATAR DADOS PARA O FRONTEND
  // ===================================
  
  const meses = Object.keys(statsPorMes).sort();
  
  // --- GR√ÅFICOS (N√çVEL TOTAL) ---
  const graficosTotal = {
    custoMensal: [["M√™s", "Custo Total (R$)", { role: 'annotation' }]],
    contagemMensal: [["M√™s", "Clich√™s Novos", { role: 'annotation' }]],
    custoPorTipo: [["Tipo de Custo", "Valor (R$)"]],
    custoPorClicheria: [["Clicheria", "Custo Total (R$)", { role: 'annotation' }]],
    errosPorClicheria: [["Clicheria", "Qtd. Erros", { role: 'annotation' }]],
    errosInternosMensal: [["M√™s", "Erros Internos", { role: 'annotation' }]],
    prazosMensal: [["M√™s", "No Prazo", { role: 'annotation' }, "Atrasado", { role: 'annotation' }]],
    prazosPorClicheria: [["Clicheria", "No Prazo", { role: 'annotation' }, "Atrasado", { role: 'annotation' }]],
    // (NOVO) Gr√°ficos de Custo de Erro
    custoErroInternoMensal: [["M√™s", "Custo Erro Interno (R$)", { role: 'annotation' }]],
    custoErroClicheriaMensal: [["M√™s", "Custo Erro Clicheria (R$)", { role: 'annotation' }]]
  };
  
  // Popula gr√°ficos mensais
  for (const mesKey of meses) {
    const stats = statsPorMes[mesKey];
    const mesLabel = Utilities.formatDate(new Date(mesKey + '-02'), "GMT", "MMM/yy");
    
    graficosTotal.custoMensal.push([mesLabel, stats.costTotal, `R$ ${stats.costTotal.toFixed(0)}`]);
    graficosTotal.contagemMensal.push([mesLabel, stats.countNovo, stats.countNovo]);
    graficosTotal.errosInternosMensal.push([mesLabel, stats.countErroInterno, stats.countErroInterno]);
    graficosTotal.prazosMensal.push([mesLabel, stats.countNoPrazo, stats.countNoPrazo, stats.countAtrasado, stats.countAtrasado]);
    // (NOVO) Popula dados de custo de erro
    graficosTotal.custoErroInternoMensal.push([mesLabel, stats.costErroInterno, `R$ ${stats.costErroInterno.toFixed(0)}`]);
    graficosTotal.custoErroClicheriaMensal.push([mesLabel, stats.costErroClicheria, `R$ ${stats.costErroClicheria.toFixed(0)}`]);
  }
  
  // Popula gr√°ficos de Custo (Total)
  graficosTotal.custoPorTipo.push(["1. Clich√™ Novo", statsTotal.costNovo]);
  graficosTotal.custoPorTipo.push(["2. Desgaste", statsTotal.costDesgaste]);
  graficosTotal.custoPorTipo.push(["3. Altera√ß√£o", statsTotal.costAlteracao]);
  graficosTotal.custoPorTipo.push(["4. Erro Interno", statsTotal.costErroInterno]);
  graficosTotal.custoPorTipo.push(["5. Erro Clicheria", statsTotal.costErroClicheria]);
  
  // Popula gr√°ficos por Clicheria (Total)
  for (const clicheria in statsPorClicheria) {
    const stats = statsPorClicheria[clicheria];
    graficosTotal.custoPorClicheria.push([clicheria, stats.costTotal, `R$ ${stats.costTotal.toFixed(0)}`]);
    graficosTotal.errosPorClicheria.push([clicheria, stats.countErroClicheria, stats.countErroClicheria]);
    graficosTotal.prazosPorClicheria.push([clicheria, stats.countNoPrazo, stats.countNoPrazo, stats.countAtrasado, stats.countAtrasado]);
  }
  // Ordena por valor (descendente)
  const sortDesc = (a, b) => b[1] - a[1];
  graficosTotal.custoPorClicheria.slice(1).sort(sortDesc);
  graficosTotal.errosPorClicheria.slice(1).sort(sortDesc);
  graficosTotal.prazosPorClicheria.slice(1).sort(sortDesc);

  
  // --- KPIs (N√çVEL TOTAL) ---
  const kpisTotal = {
    ano: anoAtual, // <-- ADICIONADO
    costTotal: statsTotal.costTotal,
    avgCostNovo: (statsTotal.countNovo > 0) ? (statsTotal.costNovo / statsTotal.countNovo) : 0,
    avgLeadTime: (statsTotal.countRecebidos > 0) ? (statsTotal.totalLeadTimeDias / statsTotal.countRecebidos) : 0,
    countErroTotal: statsTotal.countErroInterno + statsTotal.countErroClicheria,
    ...statsTotal // Inclui todas as outras contagens e custos
  };

  // --- DADOS MENSAIS (para o filtro) ---
  const dadosMensais = {};
  for (const mesKey in statsPorMes) {
    const stats = statsPorMes[mesKey];
    dadosMensais[mesKey] = {
      kpis: {
        ano: anoAtual,
        costTotal: stats.costTotal,
        avgCostNovo: (stats.countNovo > 0) ? (stats.costNovo / stats.countNovo) : 0,
        avgLeadTime: (stats.countRecebidos > 0) ? (stats.totalLeadTimeDias / stats.countRecebidos) : 0,
        countErroTotal: stats.countErroInterno + stats.countErroClicheria,
        ...stats
      },
      graficos: {
        custoPorClicheria: [["Clicheria", "Custo Total (R$)", { role: 'annotation' }]],
        errosPorClicheria: [["Clicheria", "Qtd. Erros", { role: 'annotation' }]],
        prazosPorClicheria: [["Clicheria", "No Prazo", { role: 'annotation' }, "Atrasado", { role: 'annotation' }]]
      }
    };
    
    // Calcula os gr√°ficos por clicheria *daquele m√™s*
    for (const clicheria in statsPorClicheria) {
      const pedidosDoMes = dadosPedidos.filter(row => {
          try {
            return new Date(row[4]).toISOString().slice(0, 7) === mesKey && row[3] === clicheria;
          } catch(e) { return false; }
      });
      
      const statsMesClicheria = _initClicheStats();
      for(const row of pedidosDoMes) {
         statsMesClicheria.costTotal += parseFloat(row[9] || 0);
         statsMesClicheria.countErroClicheria += (row[13] === "Reposi√ß√£o por Erro") ? 1 : 0;
         statsMesClicheria.countNoPrazo += (row[8] === "No Prazo") ? 1 : 0;
         statsMesClicheria.countAtrasado += (row[8] === "Atrasado") ? 1 : 0;
      }
      
      if (statsMesClicheria.costTotal > 0)
        dadosMensais[mesKey].graficos.custoPorClicheria.push([clicheria, statsMesClicheria.costTotal, `R$ ${statsMesClicheria.costTotal.toFixed(0)}`]);
      if (statsMesClicheria.countErroClicheria > 0)
        dadosMensais[mesKey].graficos.errosPorClicheria.push([clicheria, statsMesClicheria.countErroClicheria, statsMesClicheria.countErroClicheria]);
      if (statsMesClicheria.countNoPrazo > 0 || statsMesClicheria.countAtrasado > 0)
        dadosMensais[mesKey].graficos.prazosPorClicheria.push([clicheria, statsMesClicheria.countNoPrazo, statsMesClicheria.countNoPrazo, statsMesClicheria.countAtrasado, statsMesClicheria.countAtrasado]);
    }
  }

  return {
    kpis: kpisTotal,
    graficos: graficosTotal,
    dadosMensais: dadosMensais
  };
}

function getDadosDashboard() {
  try {
    const anoAtual = new Date().getFullYear(); 
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    if (abaPedidos.getLastRow() < 2) {
      throw new Error("N√£o h√° dados de pedidos para analisar.");
    }

    const GAP_M = 0.003; 
    const dados = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, TOTAL_COLUNAS_PEDIDOS).getValues();

    // --- KPIs ---
    let kpi_refugoProvocadoTotal = 0;
    let kpi_refugoObrigatorioTotal = 0;
    let kpi_metragemBrutaTotal = 0; 
    let kpi_metragemLiquidaTotal = 0;
    let kpi_pedidosNoPrazo = 0;
    let kpi_pedidosAtrasados = 0;
    let kpi_totalDiasAtraso = 0;
    
    // --- Agregadores (M√°quina/Impressor) - ANUAL ---
    const refugoProvocadoPorMaquina = {};
    const refugoObrigatorioPorMaquina = {};
    const refugoProvocadoPorImpressor = {};
    const refugoObrigatorioPorImpressor = {};
    const metragemConsumidaImpressor = {}; 
    const metragemBoaImpressor = {}; 

    // --- Agregadores Anuais de Insumos ---
    const metrosPorFaca = {};
    const rotulosPorProduto = {};
    
    // --- Agregadores Mensais (Tend√™ncia) ---
    const refugoTotalPorMes = {};
    const metragemConsumidaPorMes = {};
    const metragemBoaPorMes = {};
    const pedidosNoPrazoPorMes = {};
    const pedidosAtrasadosPorMes = {};

    // --- Agregadores de Velocidade (Por M√°quina) ---
    const totalMinutosProducaoPorMaquina = {};
    const totalMetragemParaVelocidadePorMaquina = {};
    const minutosProducaoPorMesPorMaquina = {};
    const metragemParaVelocidadePorMesPorMaquina = {};
    
    // --- Agregadores Mensais para FILTROS ---
    const refugoProvocadoPorMaquinaPorMes = {};
    const refugoObrigatorioPorMaquinaPorMes = {};
    const refugoProvocadoPorImpressorPorMes = {};
    const refugoObrigatorioPorImpressorPorMes = {};
    const metragemConsumidaImpressorPorMes = {};
    const metragemBoaImpressorPorMes = {};
    const metrosPorFacaPorMes = {};
    const rotulosPorProdutoPorMes = {};
    // ---------------------------------------------------------------

    // *** INICIALIZAR OS 12 MESES COM 0 ***
    for (let i = 1; i <= 12; i++) {
        const mesKey = `${anoAtual}-${("0" + i).slice(-2)}`;
        refugoTotalPorMes[mesKey] = 0;
        metragemConsumidaPorMes[mesKey] = 0;
        metragemBoaPorMes[mesKey] = 0;
        pedidosNoPrazoPorMes[mesKey] = 0;
        pedidosAtrasadosPorMes[mesKey] = 0;
        minutosProducaoPorMesPorMaquina[mesKey] = {};
        metragemParaVelocidadePorMesPorMaquina[mesKey] = {};
        refugoProvocadoPorMaquinaPorMes[mesKey] = {};
        refugoObrigatorioPorMaquinaPorMes[mesKey] = {};
        refugoProvocadoPorImpressorPorMes[mesKey] = {};
        refugoObrigatorioPorImpressorPorMes[mesKey] = {};
        metragemConsumidaImpressorPorMes[mesKey] = {};
        metragemBoaImpressorPorMes[mesKey] = {};
        metrosPorFacaPorMes[mesKey] = {};
        rotulosPorProdutoPorMes[mesKey] = {};
        
        MAQUINAS_PARA_ANALISE.forEach(maquina => {
          minutosProducaoPorMesPorMaquina[mesKey][maquina] = 0;
          metragemParaVelocidadePorMesPorMaquina[mesKey][maquina] = 0;
        });
    }

    // Inicializa KPIs por m√°quina
    MAQUINAS_PARA_ANALISE.forEach(maquina => {
      totalMinutosProducaoPorMaquina[maquina] = 0;
      totalMetragemParaVelocidadePorMaquina[maquina] = 0;
    });

    for (const linha of dados) {
      const status = linha[COL_PEDIDO_STATUS - 1]; 

      if (status === "Conclu√≠do" || status === "Na Expedi√ß√£o") {
        
        const maquina = linha[COL_PEDIDO_MAQUINA - 1] || "N√£o Identificada"; 
        const impressor = linha[COL_PEDIDO_IMPRESSOR - 1] || "N√£o Identificado"; 
        const faca = linha[COL_PEDIDO_FACA - 1] || "N√£o Identificada";
        const produto = linha[COL_PEDIDO_PRODUTO_CODIGO - 1] || "N√£o Identificado"; 
        const dataConclusaoStr = linha[COL_PEDIDO_DATA_CONCLUSAO - 1]; 
          
          // Filtro de Ano
          if (dataConclusaoStr) {
            const dataConclusaoAno = parseDate(dataConclusaoStr).getFullYear();
            if (dataConclusaoAno !== anoAtual) continue; // Pula dados de anos anteriores
          } else {
            continue; // Pula se n√£o tiver data de conclus√£o
          }
        
        const qtdAprovadaCQ = parseFloat(linha[COL_PEDIDO_QTD_APROVADA_CQ - 1]) || 0; 
        const puxada_mm = parseFloat(String(linha[COL_PEDIDO_PUXADA - 1]).replace(",", ".")) || 0; 
        const carreiras = parseFloat(String(linha[COL_PEDIDO_CARREIRAS - 1]).replace(",", ".")) || 1; 
        const metragemConsumida = parseFloat(linha[COL_PEDIDO_METRAGEM_REAL - 1]) || 0; 
        const metragemBoaReal = (qtdAprovadaCQ / carreiras) * ((puxada_mm / 1000) + GAP_M);
        const metragemCalculada = parseFloat(String(linha[COL_PEDIDO_METRAGEM - 1]).replace(",", ".")) || 0; 
        const quantidadePedido = parseFloat(String(linha[COL_PEDIDO_QUANTIDADE - 1]).replace(/\./g, '').replace(",", ".")) || 0; 
        const metragemPura = (quantidadePedido / carreiras) * ((puxada_mm / 1000) + GAP_M);
        const refugoProvocado = (metragemConsumida > 0 && metragemCalculada > 0) ? (metragemConsumida - metragemCalculada) : 0;
        const refugoObrigatorio = (metragemCalculada > 0 && metragemPura > 0) ? (metragemCalculada - metragemPura) : 0;
        const rp = (refugoProvocado > 0 ? refugoProvocado : 0);
        const ro = (refugoObrigatorio > 0 ? refugoObrigatorio : 0);
        
        // --- Agregadores Anuais ---
        kpi_refugoProvocadoTotal += rp;
        kpi_refugoObrigatorioTotal += ro;
        kpi_metragemBrutaTotal += metragemConsumida; 
        kpi_metragemLiquidaTotal += metragemBoaReal;
        refugoProvocadoPorMaquina[maquina] = (refugoProvocadoPorMaquina[maquina] || 0) + rp;
        refugoObrigatorioPorMaquina[maquina] = (refugoObrigatorioPorMaquina[maquina] || 0) + ro;
        refugoProvocadoPorImpressor[impressor] = (refugoProvocadoPorImpressor[impressor] || 0) + rp;
        refugoObrigatorioPorImpressor[impressor] = (refugoObrigatorioPorImpressor[impressor] || 0) + ro;
        metragemConsumidaImpressor[impressor] = (metragemConsumidaImpressor[impressor] || 0) + metragemConsumida; 
        metragemBoaImpressor[impressor] = (metragemBoaImpressor[impressor] || 0) + metragemBoaReal; 
        metrosPorFaca[faca] = (metrosPorFaca[faca] || 0) + metragemBoaReal;
        rotulosPorProduto[produto] = (rotulosPorProduto[produto] || 0) + qtdAprovadaCQ;

        if (dataConclusaoStr) {
         const dataConclusao = parseDate(dataConclusaoStr); 
         if (dataConclusao.getFullYear() > 1970) {
           const mesKey = dataConclusao.getFullYear() + "-" + ("0" + (dataConclusao.getMonth() + 1)).slice(-2);
           
           if (refugoTotalPorMes.hasOwnProperty(mesKey)) { 
             const refugoTotalDaLinha = rp + ro;
             
             // Agregadores mensais (tend√™ncia)
             refugoTotalPorMes[mesKey] += refugoTotalDaLinha;
             metragemConsumidaPorMes[mesKey] += metragemConsumida;
             metragemBoaPorMes[mesKey] += metragemBoaReal;

             // Popula dados mensais (Refugo)
             if (refugoProvocadoPorMaquinaPorMes.hasOwnProperty(mesKey)) {
               refugoProvocadoPorMaquinaPorMes[mesKey][maquina] = (refugoProvocadoPorMaquinaPorMes[mesKey][maquina] || 0) + rp;
               refugoObrigatorioPorMaquinaPorMes[mesKey][maquina] = (refugoObrigatorioPorMaquinaPorMes[mesKey][maquina] || 0) + ro;
               refugoProvocadoPorImpressorPorMes[mesKey][impressor] = (refugoProvocadoPorImpressorPorMes[mesKey][impressor] || 0) + rp;
               refugoObrigatorioPorImpressorPorMes[mesKey][impressor] = (refugoObrigatorioPorImpressorPorMes[mesKey][impressor] || 0) + ro;
             }
             
             // Popula dados mensais (Produtividade)
             if (metragemConsumidaImpressorPorMes.hasOwnProperty(mesKey)) {
               metragemConsumidaImpressorPorMes[mesKey][impressor] = (metragemConsumidaImpressorPorMes[mesKey][impressor] || 0) + metragemConsumida;
               metragemBoaImpressorPorMes[mesKey][impressor] = (metragemBoaImpressorPorMes[mesKey][impressor] || 0) + metragemBoaReal;
             }

             // NOVO: Popula dados mensais (Insumos)
             if (metrosPorFacaPorMes.hasOwnProperty(mesKey)) {
               metrosPorFacaPorMes[mesKey][faca] = (metrosPorFacaPorMes[mesKey][faca] || 0) + metragemBoaReal;
               rotulosPorProdutoPorMes[mesKey][produto] = (rotulosPorProdutoPorMes[mesKey][produto] || 0) + qtdAprovadaCQ;
             }
           }
         }
       }
        
        // --- IAL ---
        const dataEntregaStr = linha[COL_PEDIDO_DATA_ENTREGA - 1]; 
        if (dataEntregaStr && dataConclusaoStr) {
         const dataEntrega = parseDate(dataEntregaStr);
         const dataConclusao = parseDate(dataConclusaoStr);
         if (dataEntrega.getFullYear() > 1970 && dataConclusao.getFullYear() > 1970) {
           const mesKey = dataConclusao.getFullYear() + "-" + ("0" + (dataConclusao.getMonth() + 1)).slice(-2); 

           if (dataConclusao > dataEntrega) {
             kpi_pedidosAtrasados++;
             const diffTime = Math.abs(dataConclusao - dataEntrega);
             const diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24)); 
             kpi_totalDiasAtraso += diffDays;
             
             if (pedidosAtrasadosPorMes.hasOwnProperty(mesKey)) {
               pedidosAtrasadosPorMes[mesKey]++;
             }
           } else {
             kpi_pedidosNoPrazo++;
             if (pedidosNoPrazoPorMes.hasOwnProperty(mesKey)) {
               pedidosNoPrazoPorMes[mesKey]++;
             }
           }
         }
       }

        // --- C√ÅLCULO DE VELOCIDADE --- (BLOCO MOVIDO PARA C√Å)
        const dataInicioStr = linha[COL_PEDIDO_DATA_INICIO_PROD - 1]; 
        const dataFimStr = linha[COL_PEDIDO_DATA_FIM_PROD - 1]; 
         if (MAQUINAS_PARA_ANALISE.includes(maquina) && dataInicioStr && dataFimStr && metragemConsumida > 0) {
         const dataInicio = new Date(dataInicioStr);
         const dataFim = new Date(dataFimStr);
         if (!isNaN(dataInicio.getTime()) && !isNaN(dataFim.getTime())) {
           const diffMs = dataFim.getTime() - dataInicio.getTime();
           if (diffMs > 0) {
             const minutosProd = (diffMs / (1000 * 60));
             // [CORRE√á√ÉO DE TYPO APLICADA AQUI]
             totalMetragemParaVelocidadePorMaquina[maquina] += metragemConsumida;
             totalMinutosProducaoPorMaquina[maquina] += minutosProd;
             const dataConclusaoMes = parseDate(dataConclusaoStr);
             if (dataConclusaoMes.getFullYear() > 1970) {
               const mesKey = dataConclusaoMes.getFullYear() + "-" + ("0" + (dataConclusaoMes.getMonth() + 1)).slice(-2);
               if(minutosProducaoPorMesPorMaquina.hasOwnProperty(mesKey)) {
                 minutosProducaoPorMesPorMaquina[mesKey][maquina] += minutosProd;
                 metragemParaVelocidadePorMesPorMaquina[mesKey][maquina] += metragemConsumida;
               }
             }
           }
         }
       }
      } // Fim do IF (status === "Conclu√≠do"...)
        
    } // Fim do loop

    // --- C√ÅLCULOS FINAIS ---
    const kpi_refugoTotal = kpi_refugoProvocadoTotal + kpi_refugoObrigatorioTotal;
    const kpi_metragemConsumidaTotal = kpi_metragemBrutaTotal;
    const kpi_taxaRefugoTotal = (kpi_metragemConsumidaTotal > 0) ? (kpi_refugoTotal / kpi_metragemConsumidaTotal) * 100 : 0;
    const kpi_tempoMedioAtraso = (kpi_pedidosAtrasados > 0) ? (kpi_totalDiasAtraso / kpi_pedidosAtrasados) : 0;
    const kpi_totalPedidos = kpi_pedidosNoPrazo + kpi_pedidosAtrasados;
    const kpi_taxaMediaAtraso = (kpi_totalPedidos > 0) ? (kpi_pedidosAtrasados / kpi_totalPedidos) * 100 : 0;
    let maxTotalPedidosMensal = 0;
    Object.keys(pedidosNoPrazoPorMes).forEach(mesKey => {
      const valNoPrazo = pedidosNoPrazoPorMes[mesKey] || 0;
      const valAtrasados = pedidosAtrasadosPorMes[mesKey] || 0;
      const total = valNoPrazo + valAtrasados;
      if (total > maxTotalPedidosMensal) {
       maxTotalPedidosMensal = total;
      }
    });
    const kpis_velocidadeMediaPorMaquina = {};
    MAQUINAS_PARA_ANALISE.forEach(maquina => {
      const totalMetragem = totalMetragemParaVelocidadePorMaquina[maquina];
      const totalMinutos = totalMinutosProducaoPorMaquina[maquina];
      kpis_velocidadeMediaPorMaquina[maquina] = (totalMinutos > 0) ? (totalMetragem / totalMinutos) : 0;
    });
    const refugoTotalMensalData = formatarMesesParaGrafico(refugoTotalPorMes, "Refugo Total (m)");
    const taxaRefugoMensal = {};
    Object.keys(refugoTotalPorMes).forEach(mesKey => {
         const refugo = refugoTotalPorMes[mesKey] || 0;
         const consumida = metragemConsumidaPorMes[mesKey] || 0;
         taxaRefugoMensal[mesKey] = (consumida > 0) ? (refugo / consumida) * 100 : 0;
    });
    const taxaRefugoMensalData = formatarMesesParaGrafico(taxaRefugoMensal, "Taxa Refugo (%)", 'percent');
    const metragemBoaMensalData = formatarMesesParaGrafico(metragemBoaPorMes, "Metragem Boa (m)");
    const velocidadeMediaMensalPorMaquina = {};
    MAQUINAS_PARA_ANALISE.forEach(maquina => {
      const dadosMensaisDaMaquina = {};
      Object.keys(minutosProducaoPorMesPorMaquina).forEach(mesKey => {
         const metragemVel = metragemParaVelocidadePorMesPorMaquina[mesKey][maquina] || 0;
         const minutosProd = minutosProducaoPorMesPorMaquina[mesKey][maquina] || 0;
         dadosMensaisDaMaquina[mesKey] = (minutosProd > 0) ? (metragemVel / minutosProd) : 0;
      });
      velocidadeMediaMensalPorMaquina[maquina] = formatarMesesParaGrafico(dadosMensaisDaMaquina, "Velocidade (m/min)");
    });
    const ialMensalEmpilhadoData = formatarMesesEmpilhado(pedidosNoPrazoPorMes, pedidosAtrasadosPorMes);
    const taxaAtrasoMensal = {};
    const mesesIAL = Array.from(new Set([...Object.keys(pedidosNoPrazoPorMes), ...Object.keys(pedidosAtrasadosPorMes)]));
    mesesIAL.forEach(mesKey => {
         const noPrazo = pedidosNoPrazoPorMes[mesKey] || 0;
         const atrasados = pedidosAtrasadosPorMes[mesKey] || 0;
         const total = noPrazo + atrasados;
         taxaAtrasoMensal[mesKey] = (total > 0) ? (atrasados / total) * 100 : 0;
    });
    const taxaAtrasoMensalData = formatarMesesParaGrafico(taxaAtrasoMensal, "% Atrasados", 'percent');


    // --- Montagem do objeto dadosMensais ---
    const dadosMensais = {};
    Object.keys(refugoTotalPorMes).forEach(mesKey => {
         const refugoMes = refugoTotalPorMes[mesKey] || 0;
         const consumidaMes = metragemConsumidaPorMes[mesKey] || 0;
         const boaMes = metragemBoaPorMes[mesKey] || 0;
         
         dadosMensais[mesKey] = {
           kpis: {
             refugoTotal: refugoMes,
             taxaRefugo: (consumidaMes > 0) ? (refugoMes / consumidaMes) * 100 : 0,
             metragemTotal: {
               bruta: consumidaMes,
               liquida: boaMes
             }
           },
           graficos: {
             // Refugo
             refugoProvocadoPorMaquina: formatarParaGrafico(refugoProvocadoPorMaquinaPorMes[mesKey], "Refugo Provocado (m)"),
             refugoObrigatorioPorMaquina: formatarParaGrafico(refugoObrigatorioPorMaquinaPorMes[mesKey], "Refugo Obrigat√≥rio (m)"),
             refugoProvocadoPorImpressor: formatarParaGrafico(refugoProvocadoPorImpressorPorMes[mesKey], "Refugo Provocado (m)"),
             refugoObrigatorioPorImpressor: formatarParaGrafico(refugoObrigatorioPorImpressorPorMes[mesKey], "Refugo Obrigat√≥rio (m)"),
             // Produtividade
             produtividadeImpressor: {
               consumida: formatarParaGrafico(metragemConsumidaImpressorPorMes[mesKey], "Metragem Bruta (m)"),
               boa: formatarParaGrafico(metragemBoaImpressorPorMes[mesKey], "Metragem L√≠quida (m)")
             },
             // NOVO: Insumos
             metrosPorFaca: formatarParaGrafico(metrosPorFacaPorMes[mesKey], "Metros (m)"),
             rotulosPorProduto: formatarParaGrafico(rotulosPorProdutoPorMes[mesKey], "R√≥tulos (Qtd)")
           }
         };
    });

    // ===================================================
    // (NOVO) CHAMA A FUN√á√ÉO DE CLICH√äS E ANEXA OS DADOS
    // ===================================================
    let dadosCliches = {};
    try {
      // Assumindo que getDadosDashboard_Cliches() est√° no mesmo arquivo .gs
      dadosCliches = getDadosDashboard_Cliches(); 
    } catch (e) {
      Logger.log("Erro ao buscar dados de clich√™s: " + e.message);
      // N√£o lan√ßa erro, continua com o resto do dashboard
      // 'dadosCliches' ser√° apenas um objeto vazio {}
    }
    // ===================================================

    // --- Objeto de Retorno Final ---
    return {
      kpis: {
        ano: anoAtual, // <-- ADICIONADO
       refugoTotal: kpi_refugoTotal,
       taxaRefugo: kpi_taxaRefugoTotal,
       pedidosConcluidos: kpi_totalPedidos,
       tempoMedioAtraso: kpi_tempoMedioAtraso,
       taxaMediaAtraso: kpi_taxaMediaAtraso,
       velocidadeMediaPorMaquina: kpis_velocidadeMediaPorMaquina,
       maxPedidosMensal: maxTotalPedidosMensal,
       metragemTotal: {
         bruta: kpi_metragemBrutaTotal,
         liquida: kpi_metragemLiquidaTotal
       }
      },
      graficos: {
       ial: [
         ["Status", "Quantidade"],
         ["No Prazo", kpi_pedidosNoPrazo],
         ["Atrasados", kpi_pedidosAtrasados]
       ],
       ialMensalEmpilhado: ialMensalEmpilhadoData,
       ialTaxaAtrasoMensal: taxaAtrasoMensalData,

       refugoTotalMensal: refugoTotalMensalData,
       taxaRefugoMensal: taxaRefugoMensalData,
       
       refugoProvocadoPorMaquina: formatarParaGrafico(refugoProvocadoPorMaquina, "Refugo Provocado (m)"),
       refugoObrigatorioPorMaquina: formatarParaGrafico(refugoObrigatorioPorMaquina, "Refugo Obrigat√≥rio (m)"),
       refugoProvocadoPorImpressor: formatarParaGrafico(refugoProvocadoPorImpressor, "Refugo Provocado (m)"),
       refugoObrigatorioPorImpressor: formatarParaGrafico(refugoObrigatorioPorImpressor, "Refugo Obrigat√≥rio (m)"),

       metragemBoaMensal: metragemBoaMensalData,
       
       velocidadeMediaMensalPorMaquina: velocidadeMediaMensalPorMaquina,

       produtividadeImpressor: {
         consumida: formatarParaGrafico(metragemConsumidaImpressor, "Metragem Bruta (m)"),
         boa: formatarParaGrafico(metragemBoaImpressor, "Metragem L√≠quida (m)")
       },
       
       // Adiciona os dados de Insumos
       metrosPorFaca: formatarParaGrafico(metrosPorFaca, "Metros (m)"),
       rotulosPorProduto: formatarParaGrafico(rotulosPorProduto, "R√≥tulos (Qtd)"),
       // Placeholder para Clich√™
       metrosPorCliche: [
         ["Clich√™", "Metros (m)", { role: "annotation" }],
         ["(Aguardando Dados)", 0, "0"]
       ]
      },
      dadosMensais: dadosMensais,
      cliches: dadosCliches // <-- ADI√á√ÉO DA NOVA SE√á√ÉO
    };

  } catch (e) {
