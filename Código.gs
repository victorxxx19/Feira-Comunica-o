 * (Fun√ß√£o original que estava faltando)
 */
function _atualizarStatusAgregadoFaca(codFacaModelo) {
  try {
    const abaInventario = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_FACAS_INVENTARIO);
    const abaModelos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_FACAS_MODELOS);
    
    // 1. Verifica se existe ALGUMA faca "Dispon√≠vel" no invent√°rio
    let disponivel = false;
    const dadosInv = abaInventario.getRange(2, 2, abaInventario.getLastRow() - 1, 2).getValues(); // Col B (Modelo), Col C (Status)
    for (const item of dadosInv) {
      if (item[0] == codFacaModelo && item[1] == "Dispon√≠vel") {
        disponivel = true;
        break;
      }
    }
    const novoStatusModelo = disponivel ? "Dispon√≠vel" : "Indispon√≠vel";

    // 2. Atualiza o status na aba "Facas" (Modelos)
    const dadosModelos = abaModelos.getRange(2, 1, abaModelos.getLastRow() - 1, 11).getValues(); // A-K
    for(let i=0; i<dadosModelos.length; i++) {
      if(dadosModelos[i][0] == codFacaModelo) {
          const linhaAtualizar = i + 2;
          abaModelos.getRange(linhaAtualizar, 11).setValue(novoStatusModelo); // Col K (Status_Modelo)
          Logger.log(`Status do Modelo ${codFacaModelo} atualizado para ${novoStatusModelo}.`);
          return;
      }
    }
  } catch (e) {
