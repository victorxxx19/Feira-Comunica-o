/**
 * Fun√ß√£o para ser executada por um gatilho di√°rio (ex: 8h da manh√£).
 * Verifica pedidos com data de entrega para HOJE que ainda est√£o em produ√ß√£o.
 * ESTA FUN√á√ÉO USA AS SUAS CONSTANTES DE COLUNA.
 */
function verificarAlertasDePrazo() {
  
  const folha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS); 
  
  if (!folha) {
    Logger.log(`Erro: A aba "${NOME_ABA_PEDIDOS}" n√£o foi encontrada.`);
    return;
  }

  const dados = folha.getDataRange().getValues();

  // Pega a data de hoje e zera a hora
  const hoje = new Date();
  hoje.setHours(0, 0, 0, 0);

  // √çndices baseados em array (come√ßa em 0)
  const IDX_DATA_ENTREGA = COL_PEDIDO_DATA_ENTREGA - 1; // Col G -> √≠ndice 6
  const IDX_STATUS = COL_PEDIDO_STATUS - 1;           // Col I -> √≠ndice 8
  const IDX_NUM_PEDIDO = COL_PEDIDO_NUMERO - 1;         // Col A -> √≠ndice 0

  // Come√ßa em '1' para pular o cabe√ßalho
  for (let i = 1; i < dados.length; i++) {
    const linha = dados[i];
    
    // Pega os dados da linha baseado nos √≠ndices corretos
    const dataEntregaCell = linha[IDX_DATA_ENTREGA];
    const status = linha[IDX_STATUS];
    const numPedido = linha[IDX_NUM_PEDIDO];

    // Pula linha se n√£o tiver data de entrega ou status
    if (!dataEntregaCell || !status || !numPedido) {
      continue;
    }

    // Converte a data da c√©lula e zera a hora
    const dataEntrega = new Date(dataEntregaCell);
    dataEntrega.setHours(0, 0, 0, 0);

    // Compara se a data de entrega √© HOJE
    if (dataEntrega.getTime() === hoje.getTime()) {
      
      // Se a data for hoje, verifica se o status √© um dos que geram alerta
      if (STATUS_ALERTA.includes(status)) {
        
        // Envia o e-mail de alerta!
        const assunto = `ALERTA: Pedido ${numPedido} vence hoje!`;
        const titulo = "Alerta de Prazo de Entrega";
        const corpoHtml = `
          <p>A data de entrega do pedido <strong>${numPedido}</strong> √© hoje!</p>
          <p>Status atual: <strong>${status}</strong>.</p>
          <p style="font-weight: bold; font-size: 1.1em;">Vamos finalizar no prazo!</p>
        `;
        
        // Usa sua fun√ß√£o de envio existente
        enviarEmailSistema(assunto, titulo, corpoHtml);
      }
    }
  }
}

