/**
 * (SUBSTITUA SUA FUN√á√ÉO ANTIGA POR ESTA - VERS√ÉO OTIMIZADA)
 * Salva o corte de M√öLTIPLAS bobinas-m√£e de uma vez (Corte em Lote).
 * Esta vers√£o corrige o bug de timeout (net::ERR_FAILED / Uncaught Ku).
 */
function salvarCorteEmLote(listaIdsMaes, dadosCorte) {
  const lock = LockService.getScriptLock();
  if (!lock.tryLock(20000)) {
    throw new Error("O sistema est√° ocupado processando outro corte em lote. Tente novamente.");
  }

  try {
    const abaMae = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAMAE);
    const abaFilha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);

    const larguraCorte = parseFloat(String(dadosCorte.larguraCorte).replace(",", ".")) || 0;
    const numFilhas = parseInt(dadosCorte.numFilhas, 10);
    const larguraTotalConsumida = larguraCorte * numFilhas;
    const pedidoAlocado = dadosCorte.pedidoAlocado.trim(); // Pedido opcional

    if (larguraCorte <= 0 || numFilhas <= 0) {
      throw new Error("Largura do corte e N¬∫ de filhas devem ser maiores que zero.");
    }

    // 1. Ler todos os dados 1 vez (performance)
    const dadosMae = abaMae.getRange(2, 1, abaMae.getLastRow() - 1, 10).getValues(); // A at√© J
    const dadosFilha = (abaFilha.getLastRow() > 1) ? abaFilha.getRange("A2:B" + abaFilha.getLastRow()).getValues() : [];

    // 2. Construir mapas para busca r√°pida
    const mapaMaes = {}; // key: idMae, value: {rowIndex} (√≠ndice do array)
    dadosMae.forEach((linha, i) => {
      mapaMaes[linha[0]] = { data: linha, rowIndex: i }; // linha[0] = ID_BobinaMae
    });
    
    const mapaSeqFilhas = {}; // key: idMae, value: last_sequential
    dadosFilha.forEach(linha => {
      const idMae = linha[1]; // linha[1] = ID_BobinaMae
      const idFilha = linha[0]; // linha[0] = ID_BobinaFilha
      const partes = idFilha.split('/');
      const seq = parseInt(partes[partes.length - 1], 10);
      
      if (idMae && !isNaN(seq)) {
        if (!mapaSeqFilhas[idMae] || seq > mapaSeqFilhas[idMae]) {
          mapaSeqFilhas[idMae] = seq;
        }
      }
    });

    const novasLinhasFilhas = [];
    
    // 3. Processar cada Bobina-M√£e (MODIFICANDO O ARRAY 'dadosMae' EM MEM√ìRIA)
    for (const idMae of listaIdsMaes) {
      const maeInfo = mapaMaes[idMae];
      if (!maeInfo) continue; 

      const rowIndex = maeInfo.rowIndex;
      
      const larguraDispMae = parseFloat(String(dadosMae[rowIndex][7]).replace(",", ".")) || 0; // Col H (√≠ndice 7)
      const comprimento = parseFloat(String(dadosMae[rowIndex][6]).replace(",", ".")) || 0; // Col G (√≠ndice 6)
      const substrato = dadosMae[rowIndex][3]; // Col D (√≠ndice 3)
      const tipoDeCola = dadosMae[rowIndex][4]; // Col E (√≠ndice 4)
      const lote = dadosMae[rowIndex][1]; // Col B (√≠ndice 1)

      // Valida√ß√£o final de seguran√ßa
      if (larguraDispMae < larguraTotalConsumida) {
        continue; // Pula esta bobina se n√£o tiver largura
      }

      const novaLarguraMae = larguraDispMae - larguraTotalConsumida;
      const novoStatusMae = (novaLarguraMae < 0.01) ? "Finalizada" : "Parcial";
      
      // *** ATUALIZA O ARRAY 'dadosMae' em mem√≥ria ***
      dadosMae[rowIndex][7] = novaLarguraMae; // Atualiza Largura Dispon√≠vel (Col H)
      dadosMae[rowIndex][9] = novoStatusMae;  // Atualiza Status (Col J)
      
      let ultimoSeq = mapaSeqFilhas[idMae] || 0;

      // 4. Cria as novas Bobinas-Filhas (em mem√≥ria)
      for (let i = 1; i <= numFilhas; i++) {
        const seqAtual = ultimoSeq + i;
        const idFilha = `${idMae}/${seqAtual}`;
        novasLinhasFilhas.push([
          idFilha, idMae, lote, substrato, tipoDeCola, // 5 colunas
          larguraCorte, comprimento, comprimento,     // 3 colunas
          "Dispon√≠vel", pedidoAlocado                 // 2 colunas
        ]); // Total = 10 colunas
        
        mapaSeqFilhas[idMae] = seqAtual; 
      }
    }

    // 5. Salvar tudo de uma s√≥ vez (Muito R√°pido)
    if (novasLinhasFilhas.length > 0) {
      
      // =======================================================
      // --- CORRE√á√ÉO APLICADA AQUI (MUDADO DE 9 PARA 10) ---
      // =======================================================
      abaFilha.getRange(abaFilha.getLastRow() + 1, 1, novasLinhasFilhas.length, 10) 
              .setValues(novasLinhasFilhas);
    }
    
    // Salva as altera√ß√µes nas Bobinas-M√£e de uma s√≥ vez
    abaMae.getRange(2, 1, dadosMae.length, 10).setValues(dadosMae);
    
    SpreadsheetApp.flush();

    // ======================================================
    // --- NOVO BLOCO: ATUALIZAR PEDIDO (SE ALOCADO) ---
    // ======================================================
    if (pedidoAlocado && listaIdsMaes.length > 0) {
      try {
        const primeiraMaeId = listaIdsMaes[0];
        const primeiraMaeInfo = mapaMaes[primeiraMaeId];
        if (primeiraMaeInfo) {
          const lote = primeiraMaeInfo.data[1]; // Col B (Lote)
          const fornecedor = primeiraMaeInfo.data[2]; // Col C (Fornecedor)
          atualizarPedidoComLote(pedidoAlocado, lote, fornecedor);
        }
      } catch (e) {
        Logger.log(`Falha ao tentar chamar atualiza√ß√£o do pedido: ${e.message}`);
      }
    }
    // --- FIM DO NOVO BLOCO ---
    
    return { 
      sucesso: true, 
      mensagem: `${listaIdsMaes.length} bobina(s)-m√£e processada(s). ${novasLinhasFilhas.length} bobina(s)-filhas criadas.` 
    };

  } catch (e) {
    Logger.log(e);
    return { sucesso: false, mensagem: e.message, stack: e.stack };
  } finally {
    lock.releaseLock();
  }
}

