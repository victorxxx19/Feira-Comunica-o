/**
 * (SUBSTITUIR - V4.2) Salva pedido de REPOSI√á√ÉO E CHAMA A ATUALIZA√á√ÉO DE STATUS MESTRE
 */
function salvarPedidoReposicao(dados) {
  const lock = LockService.getScriptLock();
  lock.waitLock(15000); 
  
  let dadosProdutoParaEmail = {};
  let dataPrevistaCalculada; 
  let laminasAfetadasTexto = ""; // Para o e-mail
  
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const abaPedidos = ss.getSheetByName(NOME_ABA_CLICHE_PEDIDOS);
    const abaLaminas = ss.getSheetByName(NOME_ABA_CLICHE_LAMINAS);
    const abaMestre = ss.getSheetByName(NOME_ABA_CLICHE_MESTRE);
    const dataHoje = new Date();
    dataPrevistaCalculada = _calcularDataPrevistaCliche(dataHoje); 

    const ehJogoInteiro = (!dados.idsLaminas || dados.idsLaminas.length === 0);
    const idsLaminasParaSalvar = ehJogoInteiro ? [] : dados.idsLaminas;
    laminasAfetadasTexto = ehJogoInteiro ? "" : idsLaminasParaSalvar.join(', ');

    // 1. Salva o Cliche_Pedidos (UM PEDIDO)
    const idPedidoCliche = `PC-${String(abaPedidos.getLastRow() + 1).padStart(4, '0')}`;
    const novaLinhaPedido = [
      idPedidoCliche, dados.idCliche, 
      laminasAfetadasTexto, // <-- Coluna C (ID_Lamina) agora tem o texto
      dados.fornecedor, dataHoje,
      dados.arteFinalista, 
      dataPrevistaCalculada, 
      "", "", "", "", "", "", // H, I, J, K, L, M
      dados.tipoPedido,       // (N)
      dados.descricaoMotivo   // (O)
    ];
    abaPedidos.appendRow(novaLinhaPedido);
    
    // 2. Atualiza o status das L√ÇMINAS
    const dadosLaminasPlanilha = abaLaminas.getRange(2, 1, abaLaminas.getLastRow() - 1, 6).getValues();
    
    if (ehJogoInteiro) {
      // --- Caso 2a: Jogo Inteiro ---
      // (N√£o precisamos mais mexer no mestre aqui, a fun√ß√£o _verificar vai fazer isso)
      const rangesParaAtualizar = [];
      for (let i = 0; i < dadosLaminasPlanilha.length; i++) {
          if (dadosLaminasPlanilha[i][1] == dados.idCliche) {
            rangesParaAtualizar.push(abaLaminas.getRange(i + 2, 6)); // Coluna F
          }
      }
      for (const range of rangesParaAtualizar) {
        range.setValue("Em Fabrica√ß√£o");
      }
    } else {
      // --- Caso 2b: L√¢minas Avulsas (M√öLTIPLAS) ---
      const setLaminas = new Set(idsLaminasParaSalvar);
      for (let i = 0; i < dadosLaminasPlanilha.length; i++) {
          if (setLaminas.has(dadosLaminasPlanilha[i][0])) {
            const linhaLamina = i + 2;
            abaLaminas.getRange(linhaLamina, 6).setValue("Em Reposi√ß√£o"); // Coluna F
          }
      }
    }
    
    // ==========================================================
    // ==== GATILHO DE ATUALIZA√á√ÉO (ADICIONADO) ====
    // ==========================================================
    // Ap√≥s atualizar os status das l√¢minas, CHAMA A VERIFICA√á√ÉO MESTRE
    _verificarEAtualizarStatusMestre(dados.idCliche, abaMestre, abaLaminas);
    // ==========================================================

    dadosProdutoParaEmail = _getDadosProdutoParaEmail(dados.idProduto);
    SpreadsheetApp.flush();
    
  } catch (e) {
    Logger.log(`Erro DENTRO DO LOCK ao salvar Pedido de Reposi√ß√£o: ${e.message} ${e.stack}`);
    throw new Error(`Erro no servidor (lock): ${e.message}`);
  } finally {
    lock.releaseLock();
  }

  // --- E-MAIL (FORA DO LOCK) ---
  try {
    const { cliente, descricao } = dadosProdutoParaEmail;
    const assunto = ehJogoInteiro ? `Solicita√ß√£o de Jogo Inteiro: ${dados.idCliche}` : `Solicita√ß√£o de L√¢minas Avulsas: ${dados.idCliche}`;
    const titulo = ehJogoInteiro ? "Solicita√ß√£o de Jogo Inteiro" : "Solicita√ß√£o de L√¢minas Avulsas";
    const corpoHtml = `
      <p>Uma nova solicita√ß√£o foi registrada:</p>
      <p><strong>ID Clich√™:</strong> ${dados.idCliche}</p>
      <p><strong>Produto:</strong> ${dados.idProduto} (${descricao})</p>
      <p><strong>Cliente:</strong> ${cliente}</p>
      <p><strong>Item(s) Solicitado(s):</strong> ${ehJogoInteiro ? 'Jogo Inteiro' : laminasAfetadasTexto}</p>
      <p><strong>Tipo de Pedido:</strong> ${dados.tipoPedido}</p>
      <p><strong>Solicitante:</strong> ${dados.arteFinalista}</p>
      <p><strong>Fornecedor:</strong> ${dados.fornecedor}</p>
      <p><strong>Data Prevista:</strong> ${dataPrevistaCalculada.toLocaleDateString('pt-BR')}</p>
      ${dados.descricaoMotivo ? `<p><strong>Motivo:</strong> ${dados.descricaoMotivo}</p>` : ''}
    `;
    enviarEmailSistema(assunto, titulo, corpoHtml);
  } catch (e) {
    Logger.log(`Falha ao ENVIAR e-mail de Reposi√ß√£o: ${e.message}`);
  }
  
  try {
    return buscarDadosIniciaisCliche();
  } catch (e) {
    Logger.log(`Erro ao buscar dados FINAIS em salvarPedidoReposicao: ${e.message} ${e.stack}`);
    throw new Error(`Erro ao recarregar dados: ${e.message}`);
  }
}

function _registrarUsoDeFaca(numPedido, idUnicoFaca, metrosUsados) {
  try {
    // Se a faca for "Digital - Corte a Laser" ou n√£o tiver ID, n√£o registra.
    if (!idUnicoFaca || idUnicoFaca.toUpperCase().includes("DIGITAL")) {
      Logger.log(`Skipping Log_Uso_Facas for ${numPedido} (Faca: ${idUnicoFaca})`);
      return;
    }
    
    const metros = parseFloat(metrosUsados) || 0;
    if (metros <= 0) {
      Logger.log(`Skipping Log_Uso_Facas for ${numPedido} (Metros: ${metros})`);
      return;
    }

    const abaLogUso = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_LOG_USO_FACAS);
    abaLogUso.appendRow([
      new Date(),       // Data_Uso
      numPedido,      // Num_Pedido_Feira
      idUnicoFaca,    // ID_Unico_Faca
      metros          // Metros_Consumidos
    ]);
    
    Logger.log(`Uso de Faca Registrado: ${idUnicoFaca} usou ${metros}m no pedido ${numPedido}.`);

  } catch (e) {
    Logger.log(`ERRO ao registrar uso de faca (${idUnicoFaca}): ${e.message}`);
    // N√£o lan√ßa erro para n√£o parar o fluxo principal de finaliza√ß√£o.
  }
}

/**
 * [HELPER - ETAPA A] Busca itens de faca (ex: FX-123/1) que est√£o "Dispon√≠veis".
 * Chamado pelo FormProducao ao iniciar o setup.
 */
function getItensDeFacaDisponiveis(codFacaModelo) {
  try {
    const abaInventario = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_FACAS_INVENTARIO);
    if (abaInventario.getLastRow() < 2) return [];

    const modeloBusca = codFacaModelo.trim().toLowerCase();
    const dadosInv = abaInventario.getRange(2, 1, abaInventario.getLastRow() - 1, 3).getValues(); // Col A (ID_Unico), B (Modelo), C (Status)
    const facasDisponiveis = [];

    for (const item of dadosInv) {
      if (item[1].trim().toLowerCase() === modeloBusca && item[2].trim().toLowerCase() === "dispon√≠vel") {
        facasDisponiveis.push(item[0]); // Adiciona o ID √önico (ex: "FX-123/1")
      }
    }
    return facasDisponiveis;

  } catch (e) {
    Logger.log(`Erro ao buscar itens de faca para ${codFacaModelo}: ${e.message}`);
    return []; // Retorna vazio em caso de erro
  }
}

function _atualizarPedidosPendentesPorInsumo(idClicheRecebido, idFacaModeloRecebida) {
  Logger.log(`Iniciando verifica√ß√£o de pedidos pendentes... Faca: ${idFacaModeloRecebida}, Cliche: ${idClicheRecebido}`);
  const ss = SpreadsheetApp.getActiveSpreadsheet();
  const abaPedidos = ss.getSheetByName(NOME_ABA_PEDIDOS);
  const abaFacasInv = ss.getSheetByName(NOME_ABA_FACAS_INVENTARIO);
  const abaClicheMestre = ss.getSheetByName(NOME_ABA_CLICHE_MESTRE);
  const abaProdutos = ss.getSheetByName(NOME_ABA_PRODUTOS);
  
  const statusPendentes = ["Aguardando Faca", "Aguardando Clich√™", "Aguardando Faca e Clich√™"];

  try {
    // 1. Criar mapas de disponibilidade
    const facasDisponiveis = new Set();
    if (abaFacasInv.getLastRow() > 1) {
      const dadosFacas = abaFacasInv.getRange(2, 2, abaFacasInv.getLastRow() - 1, 2).getValues(); // Col B (Modelo), C (Status)
      dadosFacas.forEach(f => {
        if (f[1].trim().toLowerCase() === "dispon√≠vel") {
          facasDisponiveis.add(f[0].trim().toLowerCase());
        }
      });
    }
    Logger.log(`Mapa de Facas Dispon√≠veis criado com ${facasDisponiveis.size} itens.`);

    const clichesDisponiveis = new Set();
    const mapaClicheParaProduto = {}; // NOVO: Mapa para saber qual produto usa qual clich√™
    if (abaClicheMestre.getLastRow() > 1) {
      const dadosCliches = abaClicheMestre.getRange(2, 1, abaClicheMestre.getLastRow() - 1, 7).getValues(); // Col A (ID), B (Produto), G (Status)
      dadosCliches.forEach(c => {
        const idCliche = c[0].trim().toLowerCase();
        const idProduto = c[1].trim().toLowerCase();
        mapaClicheParaProduto[idCliche] = idProduto; // Mapeia "cl-100" -> "fc-500"

        if (c[6].trim().toLowerCase() === "dispon√≠vel") {
          clichesDisponiveis.add(idCliche);
        }
      });
    }
    Logger.log(`Mapa de Clich√™s Dispon√≠veis criado com ${clichesDisponiveis.size} itens.`);

    // 2. Criar mapa de Produto -> ID_Cliche_Vinculado (Necess√°rio para a l√≥gica)
    const mapaProdutoParaCliche = {};
    if (abaProdutos.getLastRow() > 1) {
      const mapaColProds = _criarMapaDeColunas(abaProdutos);
      const idxCod = mapaColProds["C√≥digo do Produto"];
      const idxCliche = mapaColProds["ID_Cliche_Vinculado"];
      if (idxCliche !== undefined) {
        const dadosProds = abaProdutos.getRange(2, 1, abaProdutos.getLastRow() - 1, abaProdutos.getLastColumn()).getValues();
        dadosProds.forEach(p => {
          mapaProdutoParaCliche[p[idxCod].trim().toLowerCase()] = p[idxCliche].trim().toLowerCase();
        });
      }
    }

    // 3. Ler e processar a aba Pedidos
    const rangePedidos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, TOTAL_COLUNAS_PEDIDOS);
    const dadosPedidos = rangePedidos.getValues();
    let mudouAlgo = false;
    let pedidosAtualizados = 0;

    for (let i = 0; i < dadosPedidos.length; i++) {
      const statusAtual = dadosPedidos[i][COL_PEDIDO_STATUS - 1]; // Col I

      if (statusPendentes.includes(statusAtual)) {
        const facaModelo = dadosPedidos[i][COL_PEDIDO_FACA - 1].trim().toLowerCase(); // Col M
        const codProduto = dadosPedidos[i][COL_PEDIDO_PRODUTO_CODIGO - 1].trim().toLowerCase(); // Col J
        let idCliche = dadosPedidos[i][COL_PEDIDO_ID_CLICHE - 1].trim().toLowerCase(); // Col AU
        const numPedido = dadosPedidos[i][COL_PEDIDO_NUMERO - 1];

        // Se o ID do Clich√™ no pedido for "N/A", busca o ID "correto" na aba Produtos
        if (idCliche === "n/a" || idCliche === "") {
            if (mapaProdutoParaCliche[codProduto]) {
                idCliche = mapaProdutoParaCliche[codProduto];
            }
        }
        
        // 4. Verifica se este pedido estava esperando o insumo que acabou de chegar
        const facaMatch = idFacaModeloRecebida && facaModelo === idFacaModeloRecebida.trim().toLowerCase();
        const clicheMatch = idClicheRecebido && idCliche === idClicheRecebido.trim().toLowerCase();

        if (facaMatch || clicheMatch) {
          Logger.log(`Pedido ${numPedido} (Status: ${statusAtual}) bate com o insumo recebido. Re-verificando...`);

          // 5. Re-verifica AMBOS os insumos usando os mapas completos
          const facaOK = facasDisponiveis.has(facaModelo) || facaModelo.includes("digital");
          // (Se o idCliche ainda for "n/a", clicheOK ser√° true, o que est√° correto)
          const clicheOK = clichesDisponiveis.has(idCliche) || idCliche === "n/a" || facaModelo.includes("digital");

          let statusNovo = statusAtual;

          if (facaOK && clicheOK) {
            statusNovo = "Dispon√≠vel";
          } else if (!facaOK && !clicheOK) {
            statusNovo = "Aguardando Faca e Clich√™";
          } else if (!facaOK) {
            statusNovo = "Aguardando Faca";
          } else if (!clicheOK) {
            statusNovo = "Aguardando Clich√™";
          }

          // 6. *** A CORRE√á√ÉO EST√Å AQUI ***
          // Se o clich√™ chegou E o pedido estava com "N/A", atualiza o ID do clich√™ no pedido
          if (clicheMatch && dadosPedidos[i][COL_PEDIDO_ID_CLICHE - 1] === "N/A" && idCliche !== "n/a") {
             dadosPedidos[i][COL_PEDIDO_ID_CLICHE - 1] = idClicheRecebido.toUpperCase(); // Salva o ID (ex: "CL-100")
             Logger.log(`   -> ATUALIZANDO Coluna ID_Cliche (AU) para: ${idClicheRecebido.toUpperCase()}`);
             mudouAlgo = true;
          }

          if (statusNovo !== statusAtual) {
            Logger.log(`   -> ATUALIZANDO Status (I) de "${statusAtual}" para "${statusNovo}"`);
            dadosPedidos[i][COL_PEDIDO_STATUS - 1] = statusNovo; // Atualiza o array
            mudouAlgo = true;
            pedidosAtualizados++;
          }
        }
      }
    }

    // 7. Se algo mudou, salva tudo de volta na planilha
    if (mudouAlgo) {
      rangePedidos.setValues(dadosPedidos);
      SpreadsheetApp.flush();
      Logger.log(`Sucesso! ${pedidosAtualizados} pedidos pendentes foram atualizados.`);
    } else {
      Logger.log(`Nenhum pedido pendente precisou de atualiza√ß√£o para este insumo.`);
    }

  } catch (e) {
    Logger.log(`ERRO GRAVE em _atualizarPedidosPendentesPorInsumo: ${e.message} ${e.stack}`);
  }
}

/**
 * (SUBSTITU√çDA - ETAPA B)
 * Recebe um Pedido de Clich√™ e atualiza status.
 * CHAMA A FUN√á√ÉO DE ATUALIZA√á√ÉO DE PEDIDOS PENDENTES.
 */
function receberPedidoCliche(idPedidoCliche, idCliche, idLamina, dadosRecebimento) {
  const lock = LockService.getScriptLock();
  lock.waitLock(10000);
  
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const abaPedidos = ss.getSheetByName(NOME_ABA_CLICHE_PEDIDOS);
    const abaMestre = ss.getSheetByName(NOME_ABA_CLICHE_MESTRE);
    const abaLaminas = ss.getSheetByName(NOME_ABA_CLICHE_LAMINAS);
    const dataRecebimento = new Date(dadosRecebimento.dataRecebimento.split('/').reverse().join('-')); 
    let dadosPedidoEmail = {}; 
    let dataPrevista = "";
    let laminasRecebidasTexto = idLamina; // Para o e-mail

    // 1. Atualiza o Cliche_Pedidos (Log de Compra)
    const dadosPedidos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, 15).getDisplayValues();
    for (let i = 0; i < dadosPedidos.length; i++) {
      if (dadosPedidos[i][0] == idPedidoCliche) {
        const linhaPedido = i + 2;
        dataPrevista = dadosPedidos[i][6];
        const statusReceb = _calcularStatusRecebimento(dataPrevista, dadosRecebimento.dataRecebimento);
        
        laminasRecebidasTexto = dadosPedidos[i][2]; // Coluna C (ID_Lamina)

        abaPedidos.getRange(linhaPedido, 8, 1, 6).setValues([
          [ dataRecebimento, statusReceb,
            parseFloat(String(dadosRecebimento.valorNF).replace(",", ".")) || 0,
            dadosRecebimento.numNF,
            dadosPedidos[i][11], // Obs (n√£o edita)
            dadosRecebimento.codLamina
          ]
        ]);
        
        dadosPedidoEmail = { 
            fornecedor: dadosPedidos[i][3], 
            tipoPedido: dadosPedidos[i][13],
            arteFinalista: dadosPedidos[i][5]
         };
        break;
      }
    }

    const ehJogoInteiro = (!laminasRecebidasTexto || laminasRecebidasTexto.trim() === "");
    
    Logger.log(`=== RECEBIMENTO CLICHE ===`);
    Logger.log(`ID Pedido: ${idPedidoCliche}, ID Clich√™: ${idCliche}, √â Jogo Inteiro? ${ehJogoInteiro}`);
    
    if (ehJogoInteiro) {
      // 3. JOGO COMPLETO (NOVO CLICH√ä)
      const dadosLaminas = abaLaminas.getRange(2, 1, abaLaminas.getLastRow() - 1, 6).getValues();
      let contadorLaminas = 0;
      
      for (let i = 0; i < dadosLaminas.length; i++) {
        if (String(dadosLaminas[i][1]).trim() == String(idCliche).trim()) { // Col B (ID_Cliche)
          const linhaLamina = i + 2;
          abaLaminas.getRange(linhaLamina, 6).setValue("Dispon√≠vel"); // Col F (Status_Lamina)
          contadorLaminas++;
        }
      }
      Logger.log(`‚úì Total de ${contadorLaminas} l√¢minas atualizadas para Dispon√≠vel`);

    } else {
      // 2. REPOSI√á√ÉO DE L√ÇMINAS AVULSAS
      const setLaminasRecebidas = new Set(laminasRecebidasTexto.split(',').map(id => id.trim()));
      const dadosLaminas = abaLaminas.getRange(2, 1, abaLaminas.getLastRow() - 1, 6).getValues();
      let contadorLaminas = 0;
      
      for (let i = 0; i < dadosLaminas.length; i++) {
        const idLaminaPlanilha = String(dadosLaminas[i][0]).trim();
        if (setLaminasRecebidas.has(idLaminaPlanilha)) {
          const linhaLamina = i + 2;
          abaLaminas.getRange(linhaLamina, 6).setValue("Dispon√≠vel");
          Logger.log(`‚úì L√¢mina ${idLaminaPlanilha} atualizada para Dispon√≠vel (linha ${linhaLamina})`);
          contadorLaminas++;
        }
      }
      Logger.log(`‚úì Total de ${contadorLaminas} l√¢minas atualizadas para Dispon√≠vel`);
    }

    // 4. ATUALIZA O STATUS MESTRE (SEMPRE CHAMA)
    _verificarEAtualizarStatusMestre(idCliche, abaMestre, abaLaminas);
    
    SpreadsheetApp.flush();
    
    // 5. [ETAPA B] GATILHO DE ATUALIZA√á√ÉO DE PEDIDOS
    // Dispara a verifica√ß√£o para este clich√™ que acabou de ficar "Dispon√≠vel"
    _atualizarPedidosPendentesPorInsumo(idCliche, null); 

    // --- E-MAIL DE RECEBIMENTO ---
    try {
      const { cliente, descricao } = _getDadosProdutoParaEmail(dadosRecebimento.idProduto);
      const assunto = `Recebimento de Clich√™: ${idCliche}`;
      const titulo = "Recebimento de Clich√™";
      const corpoHtml = `
        <p>Um pedido de clich√™ foi recebido e registrado no sistema:</p>
        <p><strong>ID Pedido de Compra:</strong> ${idPedidoCliche}</p>
        <p><strong>ID Clich√™:</strong> ${idCliche}</p>
        <p><strong>Produto:</strong> ${dadosRecebimento.idProduto} (${descricao})</p>
        <p><strong>Cliente:</strong> ${cliente}</p>
        <p><strong>Item(s) Recebido(s):</strong> ${ehJogoInteiro ? 'Jogo Completo' : laminasRecebidasTexto}</p>
        <p><strong>Fornecedor:</strong> ${dadosPedidoEmail.fornecedor || 'N/A'}</p>
        <p><strong>Data Recebimento:</strong> ${dadosRecebimento.dataRecebimento}</p>
        <p><strong>Valor:</strong> R$ ${dadosRecebimento.valorNF}</p>
        <p><strong>NF / Doc:</strong> ${dadosRecebimento.numNF}</p>
        <p><strong>C√≥d. Clicheria:</strong> ${dadosRecebimento.codLamina}</p>
      `;
