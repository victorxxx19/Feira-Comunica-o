 */
function funcaoUtilSanitizar(str) {
  if (!str) return "";
  return String(str)
    .replace(/</g, "&lt;") // Converte < para &lt;
    .replace(/>/g, "&gt;"); // Converte > para &gt;
}
function removerAcentos(str) {
¬† return str.normalize("NFD").replace(/[\u0300-\u036f]/g, "");
}

// C√ìDIGO.gs (Fun√ß√£o auxiliar)
function normalizarStatus(str) {
  if (!str) return "";
  // 1. Converte para min√∫sculas
  let lowerStr = str.toLowerCase();
  // 2. Remove acentos
  lowerStr = lowerStr.normalize("NFD").replace(/[\u0300-\u036f]/g, "");
  // 3. Remove todos os espa√ßos e tra√ßos para facilitar a compara√ß√£o
  return lowerStr.replace(/\s/g, "").replace(/-/g, "").trim(); 
}

/**
 * (NOVA FUN√á√ÉO AUXILIAR)
 * Cria um mapa de 'Nome da Coluna' para seu '√≠ndice' (posi√ß√£o no array).
 * Isso torna as fun√ß√µes de busca "√† prova de futuro".
 * @param {GoogleAppsScript.Spreadsheet.Sheet} aba A aba da planilha para mapear.
 * @returns {Object} Um objeto. Ex: {"C√≥digo do Produto": 1, "Cliente": 2}
 */
function _criarMapaDeColunas(aba) {
  if (!aba) return {};
  const cabecalho = aba.getRange(1, 1, 1, aba.getLastColumn()).getValues()[0];
  const mapa = {};
  cabecalho.forEach((coluna, index) => {
    mapa[coluna] = index;
  });
  return mapa;
}

/**
 * NOVO (V1.7): Comparador avan√ßado para a lista de Facas.
 * Ordena por: 1. Digital, 2. Facas "E-", 3. Facas "FX-" (numerica), 4. Outros.
 */
function comparadorDeFacasAvancado(a, b) {
  const aUpper = a.toUpperCase();
  const bUpper = b.toUpperCase();

  // 1. Prioridade M√°xima: "Digital - Corte a Laser"
  if (aUpper.includes("DIGITAL")) return -1;
  if (bUpper.includes("DIGITAL")) return 1;

  // 2. Prioridade 2: Facas "E-"
  const aIsE = aUpper.startsWith("E-");
  const bIsE = bUpper.startsWith("E-");
  if (aIsE && !bIsE) return -1;
  if (!aIsE && bIsE) return 1;
  if (aIsE && bIsE) {
    return a.localeCompare(b); // Ordena as "E-" entre si
  }

  // 3. Prioridade 3: Facas "FX" (Ordena√ß√£o Num√©rica)
  const aIsFX = aUpper.startsWith("FX");
  const bIsFX = bUpper.startsWith("FX");
  
  if (aIsFX && !bIsFX) return -1;
  if (!aIsFX && bIsFX) return 1;
  
  if (aIsFX && bIsFX) {
    // Extrai o n√∫mero de "FX 3 A" (-> 3) ou "FX 10" (-> 10)
    const numA = parseInt(aUpper.match(/FX[\s-]*(\d+)/)?.[1], 10);
    const numB = parseInt(bUpper.match(/FX[\s-]*(\d+)/)?.[1], 10);

    if (numA && numB) {
      if (numA !== numB) {
        return numA - numB; // Ordena pelo n√∫mero
      }
      // Se os n√∫meros s√£o iguais (ex: FX 3 A vs FX 3 B), ordena pelo texto
      return a.localeCompare(b);
    }
  }

  // 4. Prioridade 4: O resto (alfab√©tica padr√£o)
  return a.localeCompare(b);
}

