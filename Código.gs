function atualizarPedidoComLote(numPedido, lote, fornecedor) {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    if (abaPedidos.getLastRow() < 2) return; // Aba vazia
    
    const numBusca = String(numPedido).trim();
    const loteLimpo = String(lote).split('-')[0]; // Pega "P123" de "P123-1/1" ou "P123-1"

    const dados = abaPedidos.getRange(2, COL_PEDIDO_NUMERO, abaPedidos.getLastRow() - 1, 1).getValues();

    for (let i = 0; i < dados.length; i++) {
      if (dados[i][0].toString().trim() === numBusca) {
        const linhaParaAtualizar = i + 2;
        
        // Atualiza Fornecedor (Col AA/27) e Lote (Col AB/28)
        abaPedidos.getRange(linhaParaAtualizar, COL_PEDIDO_FORNECEDOR_SUBSTRATO).setValue(fornecedor);
        abaPedidos.getRange(linhaParaAtualizar, COL_PEDIDO_LOTE_SUBSTRATO).setValue(loteLimpo);
        
        Logger.log(`Pedido ${numBusca} atualizado com Lote: ${loteLimpo}, Fornecedor: ${fornecedor}`);
        return; // Encontrou e atualizou
      }
    }
    Logger.log(`Pedido ${numBusca} n√£o encontrado na aba Pedidos para atualiza√ß√£o de lote.`);
  } catch (e) {
    Logger.log(`Erro ao atualizar Pedido com Lote: ${e.message}`);
    // N√£o lan√ßa erro para n√£o parar o fluxo principal
  }
}

