/**
¬†* Calcula a metragem necess√°ria para um pedido
¬†* @param {number} puxada_mm - Puxada em mil√≠metros
¬†* @param {number} qtdCores - Quantidade de cores
¬†* @param {number} quantidadePedido - Quantidade de etiquetas
¬†* @param {boolean} semExcedente - Se TRUE n√£o adiciona 10%
¬†* @param {string} tipoRotulo - "√önico" ou "Frente e Verso Conjugados"
¬†* @param {number} carreiras - N√∫mero de carreiras da faca
¬†* @returns {number} Metragem final em metros
¬†*/
function calcularMetragem(puxada_mm, qtdCores, quantidadePedido, semExcedente, tipoRotulo, carreiras) {
¬† const GAP_M = 0.003;
¬† const METROS_POR_COR = 30;
¬† const METROS_POR_MIL = 10;

¬† try {
¬† ¬† // 1. Converter puxada para metros
¬† ¬† const puxada_m = parseFloat(String(puxada_mm).replace(",", ".")) / 1000;
¬† ¬† const numQtdCores = parseInt(qtdCores, 10) || 0;
¬† ¬† const numQuantidade = parseFloat(quantidadePedido);
¬† ¬† const numCarreiras = parseFloat(String(carreiras).replace(",", ".")) || 1;

¬† ¬† // 2. Ajustar quantidade (com ou sem excedente)
¬† ¬† let quantidadeAjustada = semExcedente ? numQuantidade : (numQuantidade * 1.10);

¬† ¬† // 3. Aplicar multiplicador de Tipo de R√≥tulo
¬† ¬† const multiplicadorTipo = (tipoRotulo === "Frente e Verso Conjugados") ? 2 : 1;
¬† ¬† quantidadeAjustada = quantidadeAjustada * multiplicadorTipo;

¬† ¬† // 4. C√°lculo base
¬† ¬† let base = (puxada_m + GAP_M) * quantidadeAjustada;
¬† ¬† let acrescimoCores = numQtdCores * METROS_POR_COR;
¬† ¬† let subtotal = base + acrescimoCores;

¬† ¬† // 5. Acr√©scimo por milhar
¬† ¬† let fatorMil = Math.ceil(subtotal / 1000);
¬† ¬† let acrescimoSetup = fatorMil * METROS_POR_MIL;

¬† ¬† // 6. Total antes de dividir pelas carreiras
¬† ¬† let totalAntesDivisao = subtotal + acrescimoSetup;

¬† ¬† // 7. Dividir pelas carreiras
¬† ¬† let metragemAntesSeguranca = totalAntesDivisao / numCarreiras;
    
    // 8. NOVO: Adicionar 5% de seguran√ßa do impressor (solicitado 06/11)
    let metragemFinal = metragemAntesSeguranca * 1.05;

¬† ¬† return Math.ceil(metragemFinal);

¬† } catch (e) {
¬† ¬† Logger.log("Erro ao calcular metragem: " + e);
¬† ¬† return 0;
¬† }
}

