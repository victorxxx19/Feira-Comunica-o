/**
 * Busca dados de um pedido para confirmar o cancelamento.
 * (Chamada pelo JS do 'FormCancelarPedido.html')
 * --- VERS√ÉO ATUALIZADA ---
 */
function buscarDadosPedidoParaCancelar(numPedido) {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const dados = abaPedidos.getDataRange().getDisplayValues(); // Pega valores como aparecem

    const numBusca = String(numPedido).trim();

    for (let i = 1; i < dados.length; i++) { // Come√ßa em 1 para pular cabe√ßalho
      const linha = dados[i];
      const numPlanilha = String(linha[COL_PEDIDO_NUMERO - 1]).trim(); // Col A (√≠ndice 0)

      if (numPlanilha === numBusca) {
        
        // --- CAMPOS REQUERIDOS ---
        const status = linha[COL_PEDIDO_STATUS - 1];          // Col I (√≠ndice 8)
        const cliente = linha[COL_PEDIDO_CLIENTE - 1];       // Col K (√≠ndice 10)
        const descricao = linha[COL_PEDIDO_DESCRICAO - 1];     // Col L (√≠ndice 11)
        const dataEntrega = linha[COL_PEDIDO_DATA_ENTREGA - 1]; // Col G (√≠ndice 6)
        // --- FIM DOS CAMPOS ---

        if (status === "Cancelado") {
          return { sucesso: false, mensagem: `Pedido ${numPedido} j√° est√° cancelado.` };
        }
        if (status === "Conclu√≠do") {
          return { sucesso: false, mensagem: `Pedido ${numPedido} j√° est√° conclu√≠do e n√£o pode ser cancelado.` };
        }

        // Pedido encontrado e apto para cancelamento
        return {
          sucesso: true,
          numPedido: numPlanilha,
          cliente: cliente,
          descricao: descricao,     // <-- NOVO
          dataEntrega: dataEntrega, // <-- NOVO
          status: status,         // <-- NOVO
          linhaPlanilha: i + 1 
        };
      }
    }

    return { sucesso: false, mensagem: `Pedido ${numPedido} n√£o encontrado.` };

  } catch (e) {
    Logger.log(`Erro ao buscar pedido para cancelar: ${e}`);
    return { sucesso: false, mensagem: `Erro no servidor: ${e.message}` };
  }
}

/**
 * Registra o cancelamento de um pedido na planilha.
 * (Chamada pelo JS do 'FormCancelarPedido.html')
 */
function registrarCancelamentoPedido(linhaPlanilha, numPedido, motivo) {
  if (!motivo || motivo.trim() === "") {
    throw new Error("O motivo do cancelamento √© obrigat√≥rio.");
  }

  const lock = LockService.getScriptLock();
  if (!lock.tryLock(5000)) {
    throw new Error("O sistema est√° ocupado. Tente novamente em alguns segundos.");
  }

  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const linha = parseInt(linhaPlanilha);

    // Pega dados para o e-mail ANTES de alterar
    const cliente = abaPedidos.getRange(linha, COL_PEDIDO_CLIENTE).getDisplayValue();
    const descricao = abaPedidos.getRange(linha, COL_PEDIDO_DESCRICAO).getDisplayValue();

    // 1. Atualiza o Status
    abaPedidos.getRange(linha, COL_PEDIDO_STATUS).setValue("Cancelado");
    // 2. Adiciona o Motivo
    abaPedidos.getRange(linha, COL_PEDIDO_MOTIVO_CANCELAMENTO).setValue(motivo.trim());
    // 3. Define a data de "conclus√£o" (cancelamento)
    abaPedidos.getRange(linha, COL_PEDIDO_DATA_CONCLUSAO).setValue(new Date());

    SpreadsheetApp.flush();

    // 4. Envia e-mail de notifica√ß√£o
    const assunto = `Pedido ${numPedido} - CANCELADO`;
    const titulo = "Pedido Cancelado";
    const corpoHtml = `
      <p>O seguinte pedido foi <strong>CANCELADO</strong> no sistema:</p>
      <p style="color: #D32F2F; font-weight: bold;">Status: CANCELADO</p>
      <p><strong>N¬∫ Pedido:</strong> ${numPedido}</p>
      <p><strong>Cliente:</strong> ${cliente}</p>
      <p><strong>Descri√ß√£o:</strong> ${descricao}</p>
      <br>
      <p><strong>Motivo do Cancelamento:</strong></p>
      <p>${motivo.trim()}</p>
    `;
    enviarEmailSistema(assunto, titulo, corpoHtml);

    return { sucesso: true, mensagem: `Pedido ${numPedido} cancelado com sucesso!` };

  } catch (e) {
    Logger.log(`Erro ao registrar cancelamento: ${e}`);
    throw new Error(`Erro ao salvar: ${e.message}`);
  } finally {
    lock.releaseLock();
  }
}

