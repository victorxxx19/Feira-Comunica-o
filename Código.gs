/**
 * (SUBSTITUA SUA FUN√á√ÉO 'buscarDadosIniciaisFacas' POR ESTA)
 * * MOTIVO: Corrigido o erro "is not defined" e adicionada a leitura
 * da nova coluna G (Observacao).
 */
function buscarDadosIniciaisFacas() {
  const ss = SpreadsheetApp.getActiveSpreadsheet();
  const abaModelos = ss.getSheetByName(NOME_ABA_FACAS_MODELOS);
  const abaInventario = ss.getSheetByName(NOME_ABA_FACAS_INVENTARIO);
  const abaPedidos = ss.getSheetByName(NOME_ABA_FACAS_PEDIDOS);
  const abaProblemas = ss.getSheetByName(NOME_ABA_FACAS_PROBLEMAS);
  const abaLogUso = ss.getSheetByName(NOME_ABA_LOG_USO_FACAS);
  
  // 1. Buscar Facas_Modelos (O Cat√°logo)
  const facasModelos = [];
  const mapaModelosAutocomplete = {};
  if (abaModelos.getLastRow() > 1) {
    const dadosModelos = abaModelos.getRange(2, 1, abaModelos.getLastRow() - 1, TOTAL_COLUNAS_FACAS).getValues();
    for (const linha of dadosModelos) {
      const codFaca = linha[COL_FACA_NOME - 1];
      facasModelos.push({
        codFaca: codFaca,
        largura: linha[COL_FACA_LARGURA - 1],
        altura: linha[COL_FACA_ALTURA - 1],
        puxada: linha[COL_FACA_PUXADA - 1],
        carreiras: linha[COL_FACA_CARREIRAS - 1],
        tipo: linha[COL_FACA_TIPO - 1],
        repeticoes: linha[COL_FACA_REPETICOES - 1],
        cilindro: linha[COL_FACA_CILINDRO - 1],
        entreRepeticoes: linha[COL_FACA_ENTRE_REPETICOES - 1],
        qtd: linha[9], // Col J
        statusModelo: linha[10] // Col K
      });
      mapaModelosAutocomplete[codFaca] = null; // Para o autocomplete
    }
  }

  // 2. Buscar Facas_Inventario (Os Itens F√≠sicos)
  const facasInventario = [];
  const mapaInventario = {}; // Mapa para agregar dados
  if (abaInventario.getLastRow() > 1) {
    // (MUDAN√áA) L√™ 7 colunas (A-G) para incluir a nova coluna Observacao
    const dadosInventario = abaInventario.getRange(2, 1, abaInventario.getLastRow() - 1, 7).getDisplayValues();
    for (const linha of dadosInventario) {
      const item = {
        idUnico: linha[0],
        codFacaModelo: linha[1],
        status: linha[2],
        localizacao: linha[3],
        dataEntrada: linha[4],
        idPedidoOrigem: linha[5],
        obs: linha[6] // (NOVO) Coluna G
      };
      facasInventario.push(item);
      mapaInventario[item.idUnico] = item; // Adiciona ao mapa
    }
  }
  
  // 3. Buscar Facas_Pedidos (Hist√≥rico de Compra)
  const facasPedidos = {};
  if (abaPedidos.getLastRow() > 1) {
    // (MUDAN√áA) Lendo 14 colunas (A-N) para incluir Tipo Pedido e Desc Motivo
    const dadosPedidos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, 14).getDisplayValues();
    for (const linha of dadosPedidos) {
      const idUnico = linha[1]; // Col B (ID_Unico_Faca)
      if (!idUnico) continue; 
      if (!facasPedidos[idUnico]) facasPedidos[idUnico] = [];
      
      facasPedidos[idUnico].push({
        idPedidoFaca: linha[0], idUnicoFaca: linha[1], codFacaModelo: linha[2],
        fornecedor: linha[3], dataSolicitacao: linha[4], solicitante: linha[5],
        dataPrevista: linha[6], dataRecebimento: linha[7], statusRecebimento: linha[8],
        valorNF: linha[9], numNF: linha[10], obs: linha[11],
        tipoPedido: linha[12], // (M)
        descricaoMotivo: linha[13] // (N)
      });
    }
  }

  // 4. Buscar Facas_Problemas (Hist√≥rico de Problemas)
  const facasProblemas = {};
  if (abaProblemas.getLastRow() > 1) {
    const dadosProblemas = abaProblemas.getRange(2, 1, abaProblemas.getLastRow() - 1, 4).getDisplayValues();
    for (const linha of dadosProblemas) {
      const idUnico = linha[1]; // Col B (ID_Unico_Faca)
      if (!idUnico) continue;
      if (!facasProblemas[idUnico]) facasProblemas[idUnico] = [];
      
      facasProblemas[idUnico].push({
        idProblema: linha[0], idUnicoFaca: linha[1],
        dataReporte: linha[2], descricaoMotivo: linha[3]
      });
    }
  }

  // 5. Buscar Log_Uso_Facas (Hist√≥rico de Uso)
  const facasLogUso = {};
  if (abaLogUso.getLastRow() > 1) {
    const dadosLog = abaLogUso.getRange(2, 1, abaLogUso.getLastRow() - 1, 4).getDisplayValues();
    for (const linha of dadosLog) {
      const idUnico = linha[2]; // Col C (ID_Unico_Faca)
      if (!idUnico) continue;
      if (!facasLogUso[idUnico]) facasLogUso[idUnico] = [];
      
      facasLogUso[idUnico].push({
        dataUso: linha[0],
        numPedidoFeira: linha[1],
        idUnicoFaca: linha[2],
        metrosConsumidos: parseFloat(linha[3]) || 0
      });
    }
  }
  
  facasModelos.sort((a, b) => comparadorDeFacasAvancado(a.codFaca, b.codFaca));
  
  return { 
    facasModelos, 
    facasInventario, 
    mapaInventario,
    facasPedidos, 
    facasProblemas, 
    facasLogUso,
    mapaModelosAutocomplete
  };
}


/**
 * 3. Busca todos os detalhes de UMA faca (para o Modal de Detalhes).
 * (Fun√ß√£o original que estava faltando)
 */
function getDetalhesFacaInventario(idUnicoFaca) {
  try {
    const todosDados = buscarDadosIniciaisFacas(); // Reutiliza a fun√ß√£o principal
    
    const itemInventario = todosDados.mapaInventario[idUnicoFaca];
    if (!itemInventario) {
      throw new Error("Item de invent√°rio n√£o encontrado.");
    }
    
    const modelo = todosDados.facasModelos.find(m => m.codFaca === itemInventario.codFacaModelo);
    
    return {
      sucesso: true,
      item: itemInventario,
      modelo: modelo || {}, // Retorna o modelo (ou objeto vazio se n√£o achar)
      pedidos: todosDados.facasPedidos[idUnicoFaca] || [],
      problemas: todosDados.facasProblemas[idUnicoFaca] || [],
      logUso: todosDados.facasLogUso[idUnicoFaca] || []
    };
    
  } catch(e) {
    Logger.log(e);
    return { sucesso: false, mensagem: e.message };
  }
}


/**
 * (SUBSTITUA SUA FUN√á√ÉO 'atualizarItemInventarioFaca' POR ESTA)
 * * MOTIVO: Adiciona o salvamento do campo "Observacao" na nova coluna G.
 */
function atualizarItemInventarioFaca(item) {
  const lock = LockService.getScriptLock();
  lock.waitLock(10000);
  
  try {
    const abaInventario = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_FACAS_INVENTARIO);
    // (MUDAN√áA) L√™ 7 colunas (A-G) para pegar o status antigo
    const dados = abaInventario.getRange(2, 1, abaInventario.getLastRow() - 1, 7).getValues(); 
    
    for (let i = 0; i < dados.length; i++) {
      if (dados[i][0] == item.idUnico) { // Col A (ID_Unico_Faca)
        const linhaAtualizar = i + 2;
        
        // (MUDAN√áA) Salva Status (C) e Localiza√ß√£o (D)
        abaInventario.getRange(linhaAtualizar, 3, 1, 2).setValues([
          [item.status, item.localizacao]
        ]);

        // (NOVO) Salva Observacao (G)
        abaInventario.getRange(linhaAtualizar, 7).setValue(item.obs);
        
        // Atualiza a Data de Entrada (Col E) se o status mudar para "Dispon√≠vel"
        const statusAntigo = dados[i][2]; // Col C
        if (item.status === "Dispon√≠vel" && statusAntigo !== "Dispon√≠vel") {
           abaInventario.getRange(linhaAtualizar, 5).setValue(new Date());
        }
        
        SpreadsheetApp.flush();
        _atualizarStatusAgregadoFaca(item.codFacaModelo); 
        
        return buscarDadosIniciaisFacas(); // Retorna os dados atualizados
      }
    }
    throw new Error("ID da faca n√£o encontrado para atualiza√ß√£o.");
    
  } catch (e) {
    Logger.log(`Erro ao atualizar item de invent√°rio de faca: ${e.message}`);
    throw new Error(`Erro no servidor: ${e.message}`);
  } finally {
    lock.releaseLock();
  }
}

