const TEMPLATE_ETIQUETA_ID = "1VKF4t9aoo2LIKoBLdAz_Nfoe7vWdwGB_vznGPi8-BGQ";

/**
 * (SUBSTITUA SUA FUN√á√ÉO ANTIGA POR ESTA - VERS√ÉO "PR√â-CORTADA")
 * Salva a bobina-m√£e e, opcionalmente, j√° cria as bobinas-filhas
 * se o checkbox "isPreCortada" estiver marcado.
 */
function salvarEntradaBobina(dados) {
  const lock = LockService.getScriptLock();
  if (!lock.tryLock(10000)) { 
    throw new Error("O sistema est√° ocupado gerando um PDF. Tente novamente.");
  }

  try {
    const abaMae = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAMAE);
    const abaFilha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);
    
    const loteLimpo = dados.lote.trim().toUpperCase();
    const quantidade = parseInt(dados.quantidade, 10);
    const isPreCortada = dados.isPreCortada; // <-- NOVO

    if (isNaN(quantidade) || quantidade <= 0) {
      throw new Error("A quantidade deve ser um n√∫mero maior que zero.");
    }
    
    // --- L√≥gica para achar o √∫ltimo sequencial ---
    let ultimoSequencial = 0;
    if (abaMae.getLastRow() > 1) {
      const dadosPlanilha = abaMae.getRange(2, 1, abaMae.getLastRow() - 1, 2).getValues();
      for (const linha of dadosPlanilha) {
        const lotePlanilha = linha[1].toString().trim().toUpperCase();
        if (lotePlanilha === loteLimpo) {
          const partesId = linha[0].toString().split('-');
          const sequencial = parseInt(partesId[partesId.length - 1], 10);
          if (!isNaN(sequencial) && sequencial > ultimoSequencial) {
            ultimoSequencial = sequencial;
          }
        }
      }
    }
    // --- Fim da l√≥gica ---

    const linhasMaeParaSalvar = [];
    const linhasFilhasParaSalvar = []; // <-- NOVO
    const dadosParaEtiqueta = []; 
    const dataEntrada = new Date();
    const larguraNum = parseFloat(dados.largura.replace(",", ".")) || 0;
    const comprimentoNum = parseFloat(dados.comprimento.replace(",", ".")) || 0;

    for (let i = 1; i <= quantidade; i++) {
      const sequencialAtual = ultimoSequencial + i;
      const idUnicoMae = `${loteLimpo}-${sequencialAtual}`;

      // --- L√≥gica do Atalho "Pr√©-Cortada" ---
      let statusMae = "Dispon√≠vel";
      if (isPreCortada) {
        // Se √© pr√©-cortada, a "M√£e" j√° entra "Finalizada"
        statusMae = "Finalizada"; 
        
        // Cria a "Filha" 1-para-1
        const idFilha = `${idUnicoMae}/1`; // Sempre /1 pois √© 1-para-1
        
        // [ID_Filha, ID_Mae, Lote, Substrato, Largura, Comp_Inicial, Comp_Disp, Status, Pedido_Alocado]
        linhasFilhasParaSalvar.push([
¬† ¬† ¬† ¬† ¬† idFilha, idUnicoMae, loteLimpo, dados.substrato, dados.tipoDeCola, // <-- AJUSTADO
¬† ¬† ¬† ¬† ¬† larguraNum, comprimentoNum, comprimentoNum,¬†
¬† ¬† ¬† ¬† ¬† "Dispon√≠vel", ""
¬† ¬† ¬† ¬† ]);
        
        // Manda o ID da FILHA para a etiqueta
        dadosParaEtiqueta.push({
          idBobina: idFilha, // <-- ID DA FILHA
          lote: loteLimpo,
          substrato: dados.substrato,
          fornecedor: dados.fornecedor
        });

      } else {
        // Fluxo normal: Manda o ID da M√ÉE para a etiqueta
        dadosParaEtiqueta.push({
          idBobina: idUnicoMae, // <-- ID DA M√ÉE
          lote: loteLimpo,
          substrato: dados.substrato,
          fornecedor: dados.fornecedor
        });
      }
      // --- Fim da L√≥gica ---

      const novaLinhaMae = [
        idUnicoMae,           // A: ID_BobinaUnica
        loteLimpo,            // B: Lote_Fornecedor
        dados.fornecedor,     // C: Fornecedor
        dados.substrato,      // D: Substrato
        dados.tipoDeCola,
        larguraNum,           // E: Largura_Inicial (m)
        comprimentoNum,       // F: Comprimento_Inicial (m)
        (isPreCortada ? 0 : larguraNum), // G: Largura_Disponivel (zera se for pr√©-cortada)
        dataEntrada,          // H: Data_Entrada
        statusMae,            // I: Status
        idUnicoMae            // J: C√≥d. de Barras
      ];
      
      linhasMaeParaSalvar.push(novaLinhaMae);
    }

    // 1. Salva na planilha M√ÉE
    if (linhasMaeParaSalvar.length > 0) {
      abaMae.getRange(abaMae.getLastRow() + 1, 1, linhasMaeParaSalvar.length, linhasMaeParaSalvar[0].length)
         .setValues(linhasMaeParaSalvar);
    }
    
    // 2. Salva na planilha FILHA (se houver)
    if (linhasFilhasParaSalvar.length > 0) {
      abaFilha.getRange(abaFilha.getLastRow() + 1, 1, linhasFilhasParaSalvar.length, linhasFilhasParaSalvar[0].length)
         .setValues(linhasFilhasParaSalvar);
    }
    
    // 3. Gera o PDF (com os IDs corretos)
    const pdfUrl = gerarEtiquetasPDF(dadosParaEtiqueta, TEMPLATE_ETIQUETA_ID);

    return { 
      sucesso: true, 
      mensagem: `${quantidade} bobina(s) registradas!`,
      pdfLink: pdfUrl
    };

  } catch (e) {
    Logger.log(e);
    return { sucesso: false, mensagem: `Erro ao salvar bobina: ${e.message}`, stack: e.stack };
  } finally {
    lock.releaseLock();
  }
}


/**
 * (VERS√ÉO FINAL - FOR√áANDO A FORMATA√á√ÉO)
 * Corrige o bug de espa√ßamento nas p√°ginas 2, 3, etc.
 * @param {Array<Object>} dadosBobinas - Array de objetos (ex: [{idBobina, lote, substrato, fornecedor}, ...])
 * @param {string} templateId - O ID do template do Google Docs.
 * @returns {string} A URL do PDF gerado.
 */
function gerarEtiquetasPDF(dadosBobinas, templateId) {
  if (!dadosBobinas || dadosBobinas.length === 0) return null;

  try {
    const templateDocFile = DriveApp.getFileById(templateId);
    
    const nomePasta = "Etiquetas_Bobinas_PDF";
    let pastaDestino;
    const pastas = DriveApp.getFoldersByName(nomePasta);
    if (pastas.hasNext()) {
      pastaDestino = pastas.next();
    } else {
      pastaDestino = DriveApp.createFolder(nomePasta);
    }

    const nomeNovoDoc = `Etiquetas Lote ${dadosBobinas[0].lote} (${Utilities.formatDate(new Date(), Session.getScriptTimeZone(), "yyyy-MM-dd HH:mm")})`;
    const copiaFile = templateDocFile.makeCopy(nomeNovoDoc, pastaDestino);
    const doc = DocumentApp.openById(copiaFile.getId());
    const body = doc.getBody();
    
    const templateElements = [];
    for (let i = 0; i < body.getNumChildren(); i++) {
      templateElements.push(body.getChild(i).copy());
    }

    // --- FUN√á√ÉO HELPER PARA SUBSTITUIR TEXTO POR QR CODE ---
    function substituirPlaceholderPorQRCode(secaoBody, idBobina) {
      
      const qrUrl = "https://api.qrserver.com/v1/create-qr-code/?size=150x150&data=" + 
                    encodeURIComponent(idBobina) + "&format=png&qzone=1";
      
      try {
        const qrBlob = UrlFetchApp.fetch(qrUrl).getBlob().setName(idBobina + ".png");

        const placeholder = secaoBody.findText("{{QR_CODE}}");
        if (placeholder) {
          const element = placeholder.getElement();
          const parent = element.getParent();
          const para = parent.asParagraph(); 

          para.clear(); // Limpa o texto '{{QR_CODE}}'
          
          const img = para.appendInlineImage(qrBlob);

          // Usa o tamanho menor
          img.setWidth(90); 
          img.setHeight(90);
          
          // ======================================================
          // --- CORRE√á√ÉO FINAL ---
          // Formata o par√°grafo DEPOIS de inserir a imagem
          para.setAlignment(DocumentApp.HorizontalAlignment.CENTER);
          para.setPageBreakBefore(false);
          para.setSpacingBefore(0);
          para.setSpacingAfter(0);
          // ======================================================
        }
      } catch (e) {
        Logger.log("Falha ao buscar QR code da API: " + e.message);
        secaoBody.replaceText("{{QR_CODE}}", "[Erro ao gerar QR Code]");
      }
    }
    // --- FIM DA FUN√á√ÉO HELPER ---

    // 2. Popula a primeira etiqueta (que j√° est√° no doc)
    const primeiraBobina = dadosBobinas[0];
    body.replaceText("{{SUBSTRATO}}", primeiraBobina.substrato || "");
    body.replaceText("{{LOTE}}", primeiraBobina.lote || "");
    body.replaceText("{{ID_BOBINA}}", primeiraBobina.idBobina || "");
    body.replaceText("{{FORNECEDOR}}", primeiraBobina.fornecedor || "");
    substituirPlaceholderPorQRCode(body, primeiraBobina.idBobina); 

    // 3. Para as bobinas restantes...
    for (let i = 1; i < dadosBobinas.length; i++) {
      const bobina = dadosBobinas[i];
      
      body.appendPageBreak(); // Adiciona a quebra de p√°gina
      
      // ======================================================
      // --- CORRE√á√ÉO FINAL (FOR√áAR FORMATA√á√ÉO DA C√ìPIA) ---
      // ======================================================
      templateElements.forEach(el => {
        let newElement;
        if (el.getType() === DocumentApp.ElementType.PARAGRAPH) {
          newElement = body.appendParagraph(el.asParagraph().copy());
          // FOR√áA a remo√ß√£o de espa√ßamento fantasma
          newElement.setSpacingBefore(0);
          newElement.setSpacingAfter(0);
        } else if (el.getType() === DocumentApp.ElementType.TABLE) {
          newElement = body.appendTable(el.asTable().copy());
          // Tenta for√ßar o espa√ßamento da primeira linha
          newElement.getRow(0).getCell(0).setPaddingTop(0);
        }
      });
      // ======================================================
      
      // Substitui os placeholders na nova p√°gina
      body.replaceText("{{SUBSTRATO}}", bobina.substrato || "");
      body.replaceText("{{LOTE}}", bobina.lote || "");
      body.replaceText("{{ID_BOBINA}}", bobina.idBobina || "");
      body.replaceText("{{FORNECEDOR}}", bobina.fornecedor || "");
      substituirPlaceholderPorQRCode(body, bobina.idBobina); 
    }

    // 4. Salva e fecha o Doc
    doc.saveAndClose();
    
    // 5. Gera o PDF e retorna a URL
    const pdfBlob = copiaFile.getAs(MimeType.PDF);
    const pdfFile = pastaDestino.createFile(pdfBlob).setName(nomeNovoDoc + ".pdf");
    
    pdfFile.setSharing(DriveApp.Access.ANYONE_WITH_LINK, DriveApp.Permission.VIEW);
    
    copiaFile.setTrashed(true);
    
    return pdfFile.getUrl();

  } catch (e) {
    Logger.log(`Erro ao gerar PDF com QR Code: ${e.message} ${e.stack}`);
    throw new Error(`Falha ao gerar PDF: ${e.message}`);
  }
}
