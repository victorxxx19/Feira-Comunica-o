/**
 * NOVO: Busca a lista de Cilindros Anilox para o formul√°rio.
 * Combina Lineatura (Coluna C) e BCM (Coluna D).
 */
function getListaAnilox() {
  try {
    const aba = SpreadsheetApp.getActiveSpreadsheet().getSheetByName("Anilox");
    if (!aba || aba.getLastRow() <= 1) return [];
    
    // Assume Col C = Lineatura, Col D = BCM
    const range = aba.getRange(2, 3, aba.getLastRow() - 1, 2);
    const valores = range.getValues();
    const lista = [];
    const unicos = new Set(); // Para evitar duplicatas (ex: S (3,2 BCM))

    for (const linha of valores) {
      const lineatura = linha[0]; // Col C
      const bcm = linha[1]; // Col D
      
      if (lineatura && String(lineatura).trim() !== "") {
        const item = `${lineatura} (${bcm} BCM)`;
        if (!unicos.has(item)) {
          unicos.add(item);
          lista.push(item);
        }
      }
    }
    return lista.sort();
    
  } catch (e) {
    Logger.log(`Erro ao buscar lista de Anilox: ${e}`);
    return []; // Retorna vazio em caso de erro
  }
}
/**
 * (VERS√ÉO MELHORADA)
 * Busca o MAIOR c√≥digo de produto cadastrado (FC-XXXX) e retorna o pr√≥ximo.
 * Se n√£o houver nenhum, retorna FC-0001.
 */
function getProximoCodigoProduto() {
  const abaProdutos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PRODUTOS);
  const ultimaLinha = abaProdutos.getLastRow();
  
  if (ultimaLinha < 2) {
    return "FC-0001"; // Se n√£o h√° produtos, come√ßa do 1
  }

  // Pega TODOS os c√≥digos da Coluna B
  const dados = abaProdutos.getRange(2, COL_PRODUTO_CODIGO, ultimaLinha - 1, 1).getValues();
  
  let maiorNumero = 0;
  
  // Itera por todos os c√≥digos para achar o maior
  for (let i = 0; i < dados.length; i++) {
    const codigo = dados[i][0];
    if (codigo && typeof codigo === 'string') {
      // Usa regex para extrair o n√∫mero (ex: de "FC-0007")
      const match = codigo.match(/^FC-(\d{4})$/);
      if (match) {
        const numeroAtual = parseInt(match[1], 10);
        if (numeroAtual > maiorNumero) {
          maiorNumero = numeroAtual;
        }
      }
    }
  }

  const proximoNumero = maiorNumero + 1;
  const proximoCodigo = "FC-" + String(proximoNumero).padStart(4, '0'); // Formata para FC-0008
  
  return proximoCodigo;
}
