/**
 * NOVO (V4.5): PASSO 2 (Digital) - Inicia a Impress√£o e Calcula o Setup
 * Salva tempo de setup, data fim setup, data inicio produ√ß√£o.
 */
function iniciarImpressaoDigital(linhaPlanilha) {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const linha = parseInt(linhaPlanilha);
    
    // Colunas novas: AR=44, AS=45, AT=46
    
    // 1. Busca a data de in√≠cio do setup (Coluna AS)
    const dataInicioSetup = abaPedidos.getRange(linha, 45).getValue(); // Col AS
    if (!dataInicioSetup) {
      throw new Error("Setup n√£o foi iniciado! (Etapa 1 falhou)");
    }
    
    const dataInicioProd = new Date(); // Este √© o Fim do Setup E In√≠cio da Produ√ß√£o
    
    // 2. Calcula o tempo de setup em minutos
    const diffMs = dataInicioProd.getTime() - dataInicioSetup.getTime();
    const setupMinutos = Math.round(diffMs / 60000); // Converte para minutos

    // 3. Salva os dados
    abaPedidos.getRange(linha, 46).setValue(dataInicioProd); // Col AT (Data Fim Setup)
    abaPedidos.getRange(linha, 44).setValue(setupMinutos); // Col AR (Tempo de Setup (min))
    
    // Salva o "Tempo Total" (AL) - IN√çCIO
    abaPedidos.getRange(linha, COL_PEDIDO_DATA_INICIO_PROD).setValue(dataInicioProd); // Col AL
    
    abaPedidos.getRange(linha, COL_PEDIDO_STATUS).setValue("Imprimindo (Digital)"); // Col I
    
    SpreadsheetApp.flush();
    return `Impress√£o Digital iniciada! (Setup: ${setupMinutos} min)`;
    
  } catch (e) {
    Logger.log(`Erro ao iniciar impress√£o digital (Linha ${linhaPlanilha}): ${e}`);
    throw new Error(`Erro no servidor: ${e.message}`);
  }
}

