/**
 * (SUBSTITUIR - ETAPA B CORRIGIDA)
 * Atualiza pedidos pendentes quando insumos chegam.
 * CORRE√á√ÉO: Agora tamb√©m atualiza a COLUNA ID_CLICHE (AU) nos pedidos.
 */
function _atualizarPedidosPendentesPorInsumo(idClicheRecebido, idFacaModeloRecebida) {
  Logger.log(`Iniciando verifica√ß√£o de pedidos pendentes... Faca: ${idFacaModeloRecebida}, Cliche: ${idClicheRecebido}`);
  const ss = SpreadsheetApp.getActiveSpreadsheet();
  const abaPedidos = ss.getSheetByName(NOME_ABA_PEDIDOS);
  const abaFacasInv = ss.getSheetByName(NOME_ABA_FACAS_INVENTARIO);
  const abaClicheMestre = ss.getSheetByName(NOME_ABA_CLICHE_MESTRE);
  
  const statusPendentes = ["Aguardando Faca", "Aguardando Clich√™", "Aguardando Faca e Clich√™"];

  try {
    // 1. Criar mapas de disponibilidade
    const facasDisponiveis = new Set();
    if (abaFacasInv.getLastRow() > 1) {
      const dadosFacas = abaFacasInv.getRange(2, 2, abaFacasInv.getLastRow() - 1, 2).getValues(); // Col B (Modelo), C (Status)
      dadosFacas.forEach(f => {
        if (f[1].trim().toLowerCase() === "dispon√≠vel") {
          facasDisponiveis.add(f[0].trim().toLowerCase());
        }
      });
    }
    Logger.log(`Mapa de Facas Dispon√≠veis criado com ${facasDisponiveis.size} itens.`);

    const clichesDisponiveis = new Set();
    // NOVO: Mapa para saber qual produto usa qual clich√™
    const mapaClicheToProduto = {}; 
    if (abaClicheMestre.getLastRow() > 1) {
      const dadosCliches = abaClicheMestre.getRange(2, 1, abaClicheMestre.getLastRow() - 1, 7).getValues(); // Col A (ID), B (Produto), G (Status)
      dadosCliches.forEach(c => {
        const idCliche = c[0].trim().toLowerCase();
        const idProduto = c[1].trim().toLowerCase();
        mapaClicheToProduto[idCliche] = idProduto; // Mapeia "cl-100" -> "fc-500"

        if (c[6].trim().toLowerCase() === "dispon√≠vel") {
          clichesDisponiveis.add(idCliche);
        }
      });
    }
    Logger.log(`Mapa de Clich√™s Dispon√≠veis criado com ${clichesDisponiveis.size} itens.`);
    
    // 2. Determina qual PRODUTO ou FACA teve o insumo recebido
    const idProdutoQueChegou = mapaClicheToProduto[idClicheRecebido ? idClicheRecebido.toLowerCase() : ''];
    const idFacaQueChegou = idFacaModeloRecebida ? idFacaModeloRecebida.toLowerCase() : null;

    // 3. Ler e processar a aba Pedidos
    const rangePedidos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, TOTAL_COLUNAS_PEDIDOS);
    const dadosPedidos = rangePedidos.getValues();
    let mudouAlgo = false;
    let pedidosAtualizados = 0;

    for (let i = 0; i < dadosPedidos.length; i++) {
      const statusAtual = dadosPedidos[i][COL_PEDIDO_STATUS - 1]; // Col I

      // 4. Se o pedido est√° pendente
      if (statusPendentes.includes(statusAtual)) {
        const facaModelo = dadosPedidos[i][COL_PEDIDO_FACA - 1].trim().toLowerCase(); // Col M
        const codProduto = dadosPedidos[i][COL_PEDIDO_PRODUTO_CODIGO - 1].trim().toLowerCase(); // Col J
        const idClichePlanilha = dadosPedidos[i][COL_PEDIDO_ID_CLICHE - 1].trim().toLowerCase(); // Col AU
        const numPedido = dadosPedidos[i][COL_PEDIDO_NUMERO - 1];

        // 5. Verifica se este pedido estava esperando o insumo que acabou de chegar
        const facaMatch = idFacaQueChegou && facaModelo === idFacaQueChegou;
        const clicheMatch = idProdutoQueChegou && codProduto === idProdutoQueChegou; // O clich√™ recebido √© para o produto deste pedido?

        if (facaMatch || clicheMatch) {
          Logger.log(`Pedido ${numPedido} (Status: ${statusAtual}) bate com o insumo recebido. Re-verificando...`);

          // 6. Re-verifica AMBOS os insumos usando os mapas completos
          const facaOK = facasDisponiveis.has(facaModelo) || facaModelo.includes("digital");

          // Precisamos do ID correto do clich√™ para verificar
          let idClicheCorreto = idClichePlanilha;
          if (idClicheCorreto === "n/a" || idClicheCorreto === "") {
             // Tenta achar o ID do clich√™ pelo C√≥d. Produto no mapa que criamos
             const clichesDoProduto = Object.keys(mapaClicheToProduto).filter(key => mapaClicheToProduto[key] === codProduto);
             if (clichesDoProduto.length > 0) {
               idClicheCorreto = clichesDoProduto[0]; // Pega o primeiro ID de clich√™ associado a este produto
             }
          }
          
          const clicheOK = clichesDisponiveis.has(idClicheCorreto) || idClicheCorreto === "n/a" || facaModelo.includes("digital");

          let statusNovo = statusAtual;

          if (facaOK && clicheOK) {
            statusNovo = "Dispon√≠vel";
          } else if (!facaOK && !clicheOK) {
            statusNovo = "Aguardando Faca e Clich√™";
          } else if (!facaOK) {
            statusNovo = "Aguardando Faca";
          } else { // (clicheOK === false)
            statusNovo = "Aguardando Clich√™";
          }

          // 7. *** A CORRE√á√ÉO EST√Å AQUI ***
          // Se o clich√™ bateu (clicheMatch) E o pedido estava com "N/A" E temos um ID de clich√™ recebido...
          if (clicheMatch && (idClichePlanilha === "n/a" || idClichePlanilha === "") && idClicheRecebido) {
             // ...ent√£o, atualiza a Coluna AU (ID_Cliche) do pedido!
             dadosPedidos[i][COL_PEDIDO_ID_CLICHE - 1] = idClicheRecebido.toUpperCase(); // Salva o ID (ex: "CL-100")
             Logger.log(`   -> ATUALIZANDO Coluna ID_Cliche (AU) para: ${idClicheRecebido.toUpperCase()}`);
             mudouAlgo = true;
          }

          // 8. Atualiza o status se ele mudou
          if (statusNovo !== statusAtual) {
            Logger.log(`   -> ATUALIZANDO Status (I) de "${statusAtual}" para "${statusNovo}"`);
            dadosPedidos[i][COL_PEDIDO_STATUS - 1] = statusNovo; // Atualiza o array
            mudouAlgo = true;
            pedidosAtualizados++;
          }
        }
      }
    }

    // 9. Se algo mudou, salva tudo de volta na planilha
    if (mudouAlgo) {
      rangePedidos.setValues(dadosPedidos);
      SpreadsheetApp.flush();
      Logger.log(`Sucesso! ${pedidosAtualizados} pedidos pendentes foram atualizados.`);
    } else {
      Logger.log(`Nenhum pedido pendente precisou de atualiza√ß√£o para este insumo.`);
    }

  } catch (e) {
    Logger.log(`ERRO GRAVE em _atualizarPedidosPendentesPorInsumo: ${e.message} ${e.stack}`);
  }
}
