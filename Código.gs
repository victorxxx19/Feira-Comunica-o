function buscarDadosAgrupadosOverview() {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    if (abaPedidos.getLastRow() < 2) {
      return { pcp: [], producao: {}, cq: {}, expedicao: [] };
    }
    
    const statusExcluidos = ["Conclu√≠do", "Cancelado"];
    
    const dados = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, TOTAL_COLUNAS_PEDIDOS).getDisplayValues();

    const setores = {
      pcp: [],
      producao: {}, 
      cq: {},       
      expedicao: []
    };

    for (let i = 0; i < dados.length; i++) {
      const linha = dados[i];
      const statusAtual = linha[COL_PEDIDO_STATUS - 1]; // Col 9
      
      if (!statusExcluidos.includes(statusAtual)) {
        
        const pedido = {
          linhaPlanilha: i + 2,
          numPedido: linha[COL_PEDIDO_NUMERO - 1],       // Col 1
          dataEntrega: linha[COL_PEDIDO_DATA_ENTREGA - 1], // Col 7
          cliente: linha[COL_PEDIDO_CLIENTE - 1],          // Col 11
          descricao: linha[COL_PEDIDO_DESCRICAO - 1],      // Col 12
          quantidade: linha[COL_PEDIDO_QUANTIDADE - 1],    // Col 4
          metragem: linha[COL_PEDIDO_METRAGEM - 1],        // Col 29 (AC) <-- ADICIONADO
          status: statusAtual,                            // Col 9
          maquina: linha[COL_PEDIDO_MAQUINA - 1],         // Col 23 (W)
          fila: parseInt(linha[COL_PEDIDO_FILA - 1]) || 999, // Col 24 (X)
          unidadeCQ: linha[COL_PEDIDO_UNIDADE_CQ - 1]     // Col 32 (AF)
        };
        
        // Separa por status
        const statusNorm = normalizarStatus(statusAtual); 
        
        if (statusNorm === "aguardandoinsumos" || statusNorm === "disponivel") {
          setores.pcp.push(pedido);
        } 
        else if (statusNorm === "liberadoparaproducao" || statusNorm === "programado" || statusNorm === "emproducao") {
          // A Fila 999 era de pedidos sem fila salva, agora com o bug corrigido, ela deve sumir
          if(pedido.fila === 999) pedido.fila = "-"; 

          const maquina = pedido.maquina || "Sem M√°quina";
          if (!setores.producao[maquina]) {
            setores.producao[maquina] = [];
          }
          setores.producao[maquina].push(pedido);
        }
        else if (statusNorm === "producaofinalizada" || statusNorm === "controlando") {
           if(pedido.fila === 999) pedido.fila = "-";
           const unidade = pedido.unidadeCQ || "Sem Unidade";
           if (!setores.cq[unidade]) {
             setores.cq[unidade] = [];
           }
           setores.cq[unidade].push(pedido);
        }
        else if (statusNorm === "naexpedicao") {
          setores.expedicao.push(pedido);
        }
      }
    }
    
    // Ordena as filas internas
    for (const maquina in setores.producao) {
      setores.producao[maquina].sort((a, b) => a.fila - b.fila);
    }
    for (const unidade in setores.cq) {
      setores.cq[unidade].sort((a, b) => a.fila - b.fila);
    }
    
    return setores;
    
  } catch (e) {
    Logger.log(`Erro ao buscar dados agrupados para Overview: ${e}`);
    return { erro: e.message };
  }
}
