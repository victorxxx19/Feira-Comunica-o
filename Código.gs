// SUBSTITUA A FUN√á√ÉO getResumoDeEstoqueAgrupado PELA VERS√ÉO ABAIXO:

/**
 * (VERS√ÉO CORRIGIDA)
 * Busca os dados de estoque e retorna um RESUMO AGRUPADO.
 */
function getResumoDeEstoqueAgrupado() {
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const abaMae = ss.getSheetByName(NOME_ABA_ALMOX_BOBINAMAE);
    const abaFilha = ss.getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);
    
    // --- CORRE√á√ÉO 1: Usar a constante correta ---
    const abaLog = ss.getSheetByName(NOME_ABA_LOG_CONSUMO); 

    // 1. Mapear Consumo Real (do Log)
    const consumoPorBobina = {};
    if (abaLog && abaLog.getLastRow() > 1) { // Verifica se abaLog existe
      // Col B = ID_BobinaFilha, Col D = Comp_Consumido
      const dadosLog = abaLog.getRange(2, 2, abaLog.getLastRow() - 1, 3).getValues();
      for (const linha of dadosLog) {
        const idFilha = linha[0].toString().trim();
        const consumo = parseFloat(String(linha[2]).replace(",", ".")) || 0;
        if (idFilha) {
          consumoPorBobina[idFilha] = (consumoPorBobina[idFilha] || 0) + consumo;
        }
      }
    }

    // 2. Mapear Lotes para Fornecedores (usando a Bobina-M√£e)
    const loteParaFornecedorMap = {};
    if (abaMae && abaMae.getLastRow() > 1) {
      // Col B = Lote_Fornecedor, Col C = Fornecedor
      const dadosMaeLotes = abaMae.getRange(2, 2, abaMae.getLastRow() - 1, 2).getValues();
      for (const linha of dadosMaeLotes) {
        const lote = linha[0].toString().trim();
        const fornecedor = linha[1].toString().trim();
        if (lote && !loteParaFornecedorMap[lote]) {
          loteParaFornecedorMap[lote] = fornecedor;
        }
      }
    }

    // --- CORRE√á√ÉO 2: Processar Estoque Bruto (Bobinas-M√£e) ---
    const resumoMae = {};
    if (abaMae && abaMae.getLastRow() > 1) {
      const dadosMae = abaMae.getRange("A2:J" + abaMae.getLastRow()).getValues(); // A=ID, B=Lote, C=Forn, D=Sub, E=Cola, H=LargDisp, J=Status
      for (const linha of dadosMae) {
        const status = linha[9].toString().trim(); // Col J
        
        if (status !== "Finalizada") {
          const fornecedor = linha[2].toString().trim() || "N/D"; // Col C
          const substrato = linha[3].toString().trim() || "N/D"; // Col D
          const tipoDeCola = linha[4].toString().trim() || "N/D"; // Col E
          const larguraDisp = parseFloat(String(linha[7]).replace(",", ".")) || 0; // Col H
          
          const key = `${substrato}|${tipoDeCola}|${fornecedor}`;
          
          if (!resumoMae[key]) {
            resumoMae[key] = { substrato, tipoDeCola, fornecedor, qtd: 0, larguraTotal: 0 };
          }
          resumoMae[key].qtd += 1;
          resumoMae[key].larguraTotal += larguraDisp;
        }
      }
    }
    // --- FIM DA CORRE√á√ÉO 2 ---

    // 4. Processar Estoque Pronto (Bobinas-Filhas)
    const resumoFilha = {};
    if (abaFilha && abaFilha.getLastRow() > 1) {
      const dadosFilha = abaFilha.getRange("A2:J" + abaFilha.getLastRow()).getValues(); // A=ID, C=Lote, D=Sub, E=Cola, F=Largura, G=CompInicial, I=Status
      for (const linha of dadosFilha) {
        const status = linha[8].toString().trim(); // Col I
        const idFilha = linha[0].toString().trim(); // Col A
        const compInicial = parseFloat(String(linha[6]).replace(",", ".")) || 0; // Col G

        const consumo = consumoPorBobina[idFilha] || 0;
        const saldoReal = compInicial - consumo;

        if (status !== "Finalizada" && saldoReal >= 1) {
          const lote = linha[2].toString().trim(); // Col C
          const fornecedor = loteParaFornecedorMap[lote] || "N/D";
          const substrato = linha[3].toString().trim() || "N/D"; // Col D
          const tipoDeCola = linha[4].toString().trim() || "N/D"; // Col E
          const largura = parseFloat(String(linha[5]).replace(",", ".")) || 0; // Col F
          const larguraStr = largura.toFixed(3); 
          const key = `${substrato}|${tipoDeCola}|${fornecedor}|${larguraStr}`;
          
          if (!resumoFilha[key]) {
            resumoFilha[key] = { substrato, tipoDeCola, fornecedor, largura: larguraStr, qtd: 0, metragemTotal: 0 };
          }
          resumoFilha[key].qtd += 1;
          resumoFilha[key].metragemTotal += saldoReal;
        }
      }
    }

    // 5. Formata os dados para o frontend
    const arrayMae = Object.values(resumoMae).sort((a, b) => a.substrato.localeCompare(b.substrato) || a.fornecedor.localeCompare(b.fornecedor));
    const arrayFilha = Object.values(resumoFilha).sort((a, b) => a.substrato.localeCompare(b.substrato) || a.fornecedor.localeCompare(b.fornecedor) || a.largura.localeCompare(b.largura));
    
    // --- CORRE√á√ÉO 3: Retornar o objeto esperado pelo frontend ---
    return {
      sucesso: true,
      bobinasMae: arrayMae,
      bobinasFilhas: arrayFilha
    };
    
  } catch (e) {
    Logger.log(e);
    return { sucesso: false, mensagem: e.message, stack: e.stack };
  }
}

