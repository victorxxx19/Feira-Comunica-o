function _getProximoIdUnicoFaca(codFacaModelo, abaInventario) {
  try {
    const ultimoLinha = abaInventario.getLastRow();
    let maiorSequencial = 0;
    
    if (ultimoLinha >= 2) {
      const dadosIds = abaInventario.getRange(2, 1, ultimoLinha - 1, 1).getValues(); // Col A (ID_Unico_Faca)
      
      for (const id of dadosIds) {
        const idStr = id[0].toString();
        // Verifica se o ID come√ßa com o modelo da faca (ex: "FX 278")
        if (idStr.startsWith(codFacaModelo)) {
          const partes = idStr.split('/');
          if (partes.length === 2) {
            const num = parseInt(partes[1], 10);
            if (!isNaN(num) && num > maiorSequencial) {
              maiorSequencial = num;
            }
          }
        }
      }
    }
    // Retorna o pr√≥ximo n√∫mero no formato "FX 278/2"
    return `${codFacaModelo}/${maiorSequencial + 1}`;

  } catch (e) {
    Logger.log(`Erro ao buscar proximo ID de Faca para ${codFacaModelo}: ${e}`);
    // Fallback de seguran√ßa
    return `${codFacaModelo}/1`;
  }
}

