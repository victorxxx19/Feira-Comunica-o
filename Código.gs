 * limpa a programa√ß√£o e atualiza o status do pedido.
 * (Chamada pelo FormProducao.html)
 */
function registrarProblemaProducao(dadosDoProblema) {
  const lock = LockService.getScriptLock();
  if (!lock.tryLock(5000)) {
    throw new Error("O sistema est√° ocupado. Tente novamente em alguns segundos.");
  }

  try {
    const { numPedido, tipo, descricao } = dadosDoProblema;

    // 1. Valida√ß√£o simples
    if (!numPedido || !tipo || !descricao) {
      throw new Error("Todos os campos (Pedido, Tipo e Descri√ß√£o) s√£o obrigat√≥rios.");
    }
    if (descricao.trim().length < 10) {
      throw new Error("A descri√ß√£o do problema precisa ser mais detalhada (m√≠nimo 10 caracteres).");
    }

    // 2. Acha a linha do pedido (usa a fun√ß√£o auxiliar existente)
    const linhaPlanilha = getLinhaPedidoPeloNumero(numPedido);

    // 3. Determina o novo status
    let novoStatus = "";
    if (tipo.startsWith("Clich√™")) {
      novoStatus = "Aguardando Clich√™";
    } else if (tipo.startsWith("Faca")) {
      novoStatus = "Aguardando Faca";
    } else {
      // Voc√™ pode adicionar mais tipos aqui se precisar
      novoStatus = "Aguardando Insumos"; // Um status gen√©rico
    }

    // 4. Formata o texto do problema
    const textoProblema = `${tipo}: ${descricao.trim()}`;

    // 5. Atualiza a planilha
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);

    // Limpa a programa√ß√£o de produ√ß√£o
    abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_MAQUINA).clearContent();     // Col W
    abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_IMPRESSOR).clearContent();  // Col AD
    abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_FILA).clearContent();
    abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_DATA_INICIO_PROD).clearContent();        // Col X

    // Registra o problema e o novo status
    abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_STATUS).setValue(novoStatus);              // Col I
    abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_PROBLEMAS_PRODUCAO).setValue(textoProblema); // Col AI

    SpreadsheetApp.flush();

    // ===============================================
    // ==== IN√çCIO DA ADI√á√ÉO DE E-MAIL (NOVO) ====
    // ===============================================
    try {
      // Busca dados para o e-mail (Cliente e Descri√ß√£o do Produto)
      const dadosEmail = abaPedidos.getRange(linhaPlanilha, COL_PEDIDO_CLIENTE, 1, 2).getDisplayValues()[0];
      const cliente = dadosEmail[0]; // Col K
      const descricaoProduto = dadosEmail[1]; // Col L

      const assunto = `ALERTA PRODU√á√ÉO: Pedido ${numPedido} com Problema`;
      const titulo = "Problema Reportado na Produ√ß√£o";
      const corpoHtml = `
        <p>Um problema foi reportado na produ√ß√£o, e o pedido foi <strong>removido da fila</strong> e teve seu status alterado.</p>
        <p><strong>N¬∫ Pedido:</strong> ${numPedido}</p>
        <p><strong>Cliente:</strong> ${cliente}</p>
        <p><strong>Produto:</strong> ${descricaoProduto}</p>
        <br>
        <p><strong>Novo Status:</strong> ${novoStatus}</p>
        <p><strong>Problema Reportado (Tipo):</strong> ${tipo}</p>
        <p><strong>Descri√ß√£o do Problema:</strong></p>
        <p style="font-style: italic; background-color: #f5f5f5; padding: 10px; border-radius: 4px;">
          ${descricao.trim()}
        </p>
      `;
      
      // Envia o e-mail usando sua fun√ß√£o centralizada
      enviarEmailSistema(assunto, titulo, corpoHtml);
      Logger.log(`E-mail de problema de produ√ß√£o enviado para o pedido ${numPedido}.`);

    } catch (e) {
