            loteParaAtualizar = dados[i][2].toString().trim(); // Col C (Lote)
        }
        // -----------------------------

        linhasAtualizadas++;
      }
    }

    if (linhasAtualizadas === 0) {
      throw new Error("Nenhuma das bobinas foi encontrada ou atualizada.");
    }

    // --- L√ìGICA (PROBLEMA 3) ---
    // Se encontramos um lote, vamos buscar o fornecedor e atualizar o pedido
    if (loteParaAtualizar) {
        const abaMae = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAMAE);
        if (abaMae && abaMae.getLastRow() > 1) {
            const dadosMaeLotes = abaMae.getRange(2, 2, abaMae.getLastRow() - 1, 2).getValues();
            for (const linha of dadosMaeLotes) {
                if (linha[0].toString().trim() === loteParaAtualizar) {
                    const fornecedor = linha[1].toString().trim();
                    // Chama a fun√ß√£o helper que j√° existe no seu c√≥digo
                    atualizarPedidoComLote(numPedidoLimpo, loteParaAtualizar, fornecedor); 
                    break;
                }
            }
        }
    }
    // -----------------------------

    SpreadsheetApp.flush();
    return { 
      sucesso: true, 
      mensagem: `${linhasAtualizadas} bobina(s) alocada(s) com sucesso ao Pedido ${numPedidoLimpo}!`
    };

  } catch (e) {
    Logger.log(e);
    return { sucesso: false, mensagem: e.message, stack: e.stack };
  } finally {
    lock.releaseLock();
  }
}

