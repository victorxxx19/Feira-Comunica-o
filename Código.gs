/**
 * Converte um objeto {chave: valor} em um array de gr√°fico [['Categoria', 'Valor'], ['A', 10], ['B', 20]]
 * Ordena do maior para o menor.
 */
function formatarParaGrafico(obj, nomeColunaValor, tipoDado = 'number') {
  const dataArray = [[ "Categoria", nomeColunaValor, { role: "annotation" } ]];
  
  const entries = Object.entries(obj);
  
  // Ordena do maior para o menor valor
  entries.sort((a, b) => b[1] - a[1]);

  for (const [key, value] of entries) {
    let annotationValue;
    if (tipoDado === 'percent') {
      annotationValue = (value || 0).toLocaleString('pt-BR', { minimumFractionDigits: 2, maximumFractionDigits: 2 }) + '%';
    } else {
      annotationValue = (value || 0).toLocaleString('pt-BR', { maximumFractionDigits: 0 });
    }
    dataArray.push([key, value, annotationValue]);
  }
  return dataArray;
}

/**
 * Converte um objeto {mesKey: valor} em um array de gr√°fico, formatando a chave do m√™s.
 * Ex: {"2025-01": 10} vira [["Jan/25", 10, "10"]]
 */
function formatarMesesParaGrafico(obj, nomeColunaValor, tipoDado = 'number') {
  const dataArray = [[ "M√™s", nomeColunaValor, { role: "annotation" } ]];
  
  // Garante a ordem correta dos meses
  const mesesOrdenados = Object.keys(obj).sort();

  for (const mesKey of mesesOrdenados) {
    const value = obj[mesKey] || 0;
    const nomeMes = formatMesKeyParaGrafico(mesKey);
    
    let annotationValue;
    if (tipoDado === 'percent') {
      annotationValue = (value || 0).toLocaleString('pt-BR', { minimumFractionDigits: 2, maximumFractionDigits: 2 }) + '%';
    } else {
      annotationValue = (value || 0).toLocaleString('pt-BR', { maximumFractionDigits: 0 });
    }

    dataArray.push([nomeMes, value, annotationValue]);
  }
  return dataArray;
}

/**
 * Converte dois objetos de m√™s (ex: noPrazo, atrasados) em um array para gr√°fico empilhado.
 */
function formatarMesesEmpilhado(objNoPrazo, objAtrasados) {
  const dataArray = [["M√™s", "No Prazo", { role: "annotation" }, "Atrasados", { role: "annotation" }]];
  
  const mesesOrdenados = Object.keys(objNoPrazo).sort();

  for (const mesKey of mesesOrdenados) {
    const valNoPrazo = objNoPrazo[mesKey] || 0;
    const valAtrasados = objAtrasados[mesKey] || 0;
    const nomeMes = formatMesKeyParaGrafico(mesKey);
    
    const annNoPrazo = (valNoPrazo > 0) ? valNoPrazo.toLocaleString('pt-BR', { maximumFractionDigits: 0 }) : null;
    const annAtrasados = (valAtrasados > 0) ? valAtrasados.toLocaleString('pt-BR', { maximumFractionDigits: 0 }) : null;

    dataArray.push([
      nomeMes, 
      valNoPrazo, annNoPrazo,
      valAtrasados, annAtrasados
    ]);
  }
  return dataArray;
}

/**
 * Fun√ß√£o auxiliar para converter "2025-01" em "Jan/25".
 */
function formatMesKeyParaGrafico(mesKey) {
  try {
    const [ano, mes] = mesKey.split('-');
    const dataObj = new Date(ano, parseInt(mes) - 1, 1);
    const nomeMes = dataObj.toLocaleString('pt-BR', { month: 'short' });
    return `${nomeMes.charAt(0).toUpperCase() + nomeMes.slice(1).replace('.', '')}/${ano.slice(2)}`;
  } catch (e) {
    return mesKey; // Retorna a chave original em caso de erro
  }
}

/**
* Fun√ß√£o auxiliar para converter datas de string (dd/MM/yyyy ou yyyy-MM-dd'T'...) para Date.
*/
function parseDate(dateString) {
  if (!dateString) return new Date(0); // Retorna data 'epoch' se for nulo/vazio
  
  // Tenta formato ISO (ex: 2025-10-20T10:00:00.000Z)
  if (String(dateString).includes('T')) {
    const date = new Date(dateString);
    if (!isNaN(date.getTime())) return date;
  }

  // Tenta formato dd/MM/yyyy (comum em planilhas)
  if (String(dateString).includes('/')) {
    const parts = String(dateString).split(' ')[0].split('/'); // Ignora a hora se houver
    if (parts.length === 3) {
      // Formato: dia, m√™s, ano
      const date = new Date(parts[2], parts[1] - 1, parts[0]);
      if (!isNaN(date.getTime())) return date;
    }
  }
  
  // Se for um objeto Date nativo do Apps Script (vindo de .getValue())
  if (typeof dateString.getMonth === 'function') {
    return dateString;
  }

  // Se falhar, retorna data 'epoch'
  Logger.log("N√£o foi poss√≠vel parsear a data: " + dateString);
  return new Date(0); 
}

