function validarConsumoMetragem(dadosConsumo) {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const abaFilha = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_ALMOX_BOBINAFILHA);
    const numPedido = dadosConsumo.pedidoConsumido;

    // --- PASSO 0: PEGAR O MAPA DE CONSUMO (OTIMIZADO) ---
    const consumptionMap = getConsumptionMap(); // Chama a nova helper

    // --- PASSO 1: VERIFICAR SE H√Å BOBINAS ALOCADAS (CORRE√á√ÉO PROBLEMA 2) ---
    const dadosFilhas = abaFilha.getRange("A2:J" + abaFilha.getLastRow()).getValues();
    let bobinasAlocadasComSaldo = 0;
    
    for (let i = 0; i < dadosFilhas.length; i++) {
      const linha = dadosFilhas[i];
      const idFilha = linha[0].toString();        // Col A (ID)
      const compInicial = parseFloat(linha[6]);   // Col G (Comp_Inicial)
      const status = linha[8].toString();       // Col I (Status)
      const pedidoAlocado = linha[9].toString();  // Col J (Pedido_Alocado)

      if (pedidoAlocado === numPedido && (status === "Dispon√≠vel" || status === "Em Uso")) {
        const consumo = consumptionMap[idFilha] || 0;
        const saldoReal = compInicial - consumo;
        if (saldoReal > 0) {
          bobinasAlocadasComSaldo++;
        }
      }
    }

    if (bobinasAlocadasComSaldo === 0) {
      throw new Error(`Nenhuma bobina-filha (com saldo > 0) foi encontrada alocada para o pedido ${numPedido}.`);
    }
    
    // --- PASSO 2: VALIDAR METRAGEM (L√ìGICA ANTIGA) ---
    const metragemConsumida = parseFloat(String(dadosConsumo.metragemConsumida).replace(",", ".")) || 0;
    const dadosPedidos = abaPedidos.getRange("A2:AC" + abaPedidos.getLastRow()).getValues();
    let metragemCalculada = 0;

    for (let i = 0; i < dadosPedidos.length; i++) {
      if (String(dadosPedidos[i][COL_PEDIDO_NUMERO - 1]).trim() === numPedido) {
        metragemCalculada = parseFloat(String(dadosPedidos[i][COL_PEDIDO_METRAGEM - 1]).replace(",", ".")) || 0;
        break;
      }
    }
    if (metragemCalculada === 0) {
        throw new Error("N√£o foi poss√≠vel encontrar a metragem calculada do pedido.");
    }

    const limiteMaximo = metragemCalculada * 1.10;
    if (metragemConsumida > limiteMaximo) {
      return { sucesso: true, necessitaJustificativa: true, dadosConsumo: dadosConsumo };
    }
    
    return { sucesso: true, necessitaJustificativa: false, dadosConsumo: dadosConsumo };

  } catch (e) {
    Logger.log(`Erro em validarConsumoMetragem: ${e.message}`);
    return { sucesso: false, mensagem: `Erro de servidor: ${e.message}` };
  }
}

function getFichaTecnicaParaSetup(codigoProduto, tipoPedido) {
  try {
    const abaProdutos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PRODUTOS);
    
    if (!abaProdutos) {
      throw new Error(`Aba "${NOME_ABA_PRODUTOS}" n√£o encontrada.`);
    }
    
    const ultimaLinha = abaProdutos.getLastRow();
    
    if (ultimaLinha < 2) {
      throw new Error("Nenhum produto cadastrado.");
    }
    
    // 1. Busca manual (mais compat√≠vel)
    const codigos = abaProdutos.getRange(2, COL_PRODUTO_CODIGO, ultimaLinha - 1, 1).getValues();
    
    let linhaEncontrada = -1;
    
    for (let i = 0; i < codigos.length; i++) {
      if (String(codigos[i][0]).trim() === String(codigoProduto).trim()) {
        linhaEncontrada = i + 2; // +2 porque: √≠ndice 0 + header na linha 1
        break;
      }
    }

    if (linhaEncontrada === -1) {
      throw new Error(`Produto "${codigoProduto}" n√£o encontrado.`);
    }

    // 2. Pega o mapa de colunas
    const mapa = _criarMapaDeColunas(abaProdutos);
    const idxFicha = mapa["FichaTecnicaCores"];

    if (idxFicha === undefined) {
      throw new Error('Coluna "FichaTecnicaCores" n√£o encontrada.');
    }
    
    // 3. L√™ a ficha t√©cnica
    const ficha = abaProdutos.getRange(linhaEncontrada, idxFicha + 1).getValue();

    Logger.log(`‚úì Ficha t√©cnica encontrada para ${codigoProduto}`);

    return { 
        fichaTecnicaCores: ficha || "[]",
        tipoPedido: tipoPedido
    };

  } catch (e) {
     Logger.log("‚úó Erro em getFichaTecnicaParaSetup: " + e.message);
     throw new Error(e.message);
  }
}

function salvarAniloxDaProducao(codigoProduto, fichaTecnicaCoresJSON) {
  try {
    const abaProdutos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PRODUTOS);
    const mapa = _criarMapaDeColunas(abaProdutos);
    const idxCodigo = mapa["C√≥digo do Produto"];
    const idxFicha = mapa["FichaTecnicaCores"];
    
    if (idxFicha === undefined) {
      Logger.log("N√£o foi poss√≠vel salvar Anilox: Coluna 'FichaTecnicaCores' n√£o encontrada.");
      return; // Sai da fun√ß√£o
    }

    const dados = abaProdutos.getRange(2, 1, abaProdutos.getLastRow() - 1, abaProdutos.getLastColumn()).getValues();

    for (let i = 0; i < dados.length; i++) {
      if (dados[i][idxCodigo] == codigoProduto) {
        const linhaPlanilha = i + 2;
        // Salva o JSON na coluna "FichaTecnicaCores" (S, √≠ndice + 1)
        abaProdutos.getRange(linhaPlanilha, idxFicha + 1).setValue(fichaTecnicaCoresJSON);
        Logger.log(`Anilox do Produto ${codigoProduto} atualizados na linha ${linhaPlanilha}.`);
        return; // Encontrou e salvou
      }
    }
    Logger.log(`Produto ${codigoProduto} n√£o encontrado para salvar anilox.`);
  } catch (e) {
    Logger.log(`Erro ao salvar Anilox: ${e.message}`);
    // N√£o lan√ßa erro para n√£o parar o fluxo principal
  }
}

