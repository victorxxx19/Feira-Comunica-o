/**
 * NOVO (V1.5 - CORRIGIDO): Salva imagem/pdf no Drive e retorna um link/c√≥digo.
 * @param {string} base64Data A string de dados (ex: "data:image/png;base64,iVBOR...")
 * @param {string} codigoProduto O c√≥digo do produto (ex: "FC-0001") para nomear o arquivo.
 * @returns {string} Uma URL direta (se imagem) ou um c√≥digo (se PDF).
 */
function salvarLayoutDrive(base64Data, codigoProduto) {
  try {
    const NOME_PASTA = "Layouts de Produtos"; 
    
    // Decodifica a string Base64
    const splitData = base64Data.split(',');
    const contentType = splitData[0].match(/:(.*?);/)[1]; // Ex: "image/png" ou "application/pdf"
    const bytes = Utilities.base64Decode(splitData[1]);
    const blob = Utilities.newBlob(bytes, contentType);
    
    const extensao = contentType.split('/')[1]; // Ex: "png" ou "pdf"
    const nomeArquivo = `${codigoProduto}_Layout_${new Date().getTime()}.${extensao}`;
    blob.setName(nomeArquivo);

    // Encontra ou cria a pasta de destino
    let pasta;
    const pastas = DriveApp.getFoldersByName(NOME_PASTA);
    if (pastas.hasNext()) {
      pasta = pastas.next();
    } else {
      pasta = DriveApp.createFolder(NOME_PASTA);
    }

    // Salva o arquivo e o torna p√∫blico
    const arquivo = pasta.createFile(blob);
    arquivo.setSharing(DriveApp.Access.ANYONE_WITH_LINK, DriveApp.Permission.VIEW);
    
    // ======================================================
    // === A CORRE√á√ÉO V1.5 EST√Å AQUI ===
    // ======================================================
    
    if (contentType.startsWith("image/")) {
      // Se for IMAGEM, retorna a URL da THUMBNAIL (miniatura) em tamanho grande.
      // Esta URL √© est√°vel para <img> tags.
      return `https://drive.google.com/thumbnail?id=${arquivo.getId()}&sz=w800`; // w800 = 800px de largura
      
    } else if (contentType === "application/pdf") {
      // Se for PDF, retorna o C√ìDIGO ESPECIAL com o nome do arquivo
      return `PDF_FILE::${nomeArquivo}`;
      
    } else {
      // Para outros tipos (pouco prov√°vel)
      return arquivo.getUrl(); // Retorna o link padr√£o do Drive
    }

  } catch (e) {
    Logger.log(`Erro ao salvar layout no Drive: ${e}`);
    return ""; // Retorna vazio em caso de erro
  }
}

