/**
 * 10. Busca um MODELO de faca pelo nome (FormFaca)
 * (Fun√ß√£o original que estava faltando)
 */
function buscarFacaPeloNome(nomeFaca) {
  try {
    const abaFacas = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_FACAS_MODELOS);
    
    if (!abaFacas) {
      Logger.log("Aba de facas n√£o encontrada!");
      throw new Error("Aba de facas n√£o encontrada. Verifique o nome da aba.");
    }
    
    const ultimaLinha = abaFacas.getLastRow();
    
    if (ultimaLinha < 2) {
      throw new Error("N√£o h√° facas cadastradas.");
    }
    
    // Busca 9 colunas
    const dados = abaFacas.getRange(2, 1, ultimaLinha - 1, 9).getDisplayValues();
    
    const nomeBusca = nomeFaca.trim().toLowerCase();
    
    for (let i = 0; i < dados.length; i++) {
      const linha = dados[i];
      const nomePlanilha = linha[COL_FACA_NOME - 1].toString().trim().toLowerCase();
      
      if (nomePlanilha === nomeBusca) {
        
        return {
          linha: i + 2, 
          nome: linha[COL_FACA_NOME - 1],
          largura: linha[COL_FACA_LARGURA - 1], 
          altura: linha[COL_FACA_ALTURA - 1], 
          puxada: linha[COL_FACA_PUXADA - 1], 
          carreiras: linha[COL_FACA_CARREIRAS - 1], 
          tipo: linha[COL_FACA_TIPO - 1],
          repeticoes: linha[COL_FACA_REPETICOES - 1], 
          cilindro: linha[COL_FACA_CILINDRO - 1],
          entreRepeticoes: linha[COL_FACA_ENTRE_REPETICOES - 1]
        };
      }
    }
    
    Logger.log("Faca n√£o encontrada ap√≥s percorrer todas as linhas.");
    throw new Error(`Faca "${nomeFaca}" n√£o encontrada.`);
    
  } catch (e) {
    Logger.log(`Erro ao buscar faca ${nomeFaca}: ${e}`);
    throw e; // Relan√ßa o erro para o frontend capturar
  }
}

