function buscarPedidosParaQualidade() {
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    if (abaPedidos.getLastRow() < 2) {
      Logger.log("Aba Pedidos est√° vazia.");
      return { pedidos: [], inspetores: getListaDeAba(NOME_ABA_INSPETORES, 1) }; 
    }

    const dadosCompletos = abaPedidos.getRange(2, 1, abaPedidos.getLastRow() - 1, TOTAL_COLUNAS_PEDIDOS).getDisplayValues(); 
    const pedidosParaCQ = [];
    
    for (let i = 0; i < dadosCompletos.length; i++) {
      const linha = dadosCompletos[i];
      
      const statusOriginal = String(linha[COL_PEDIDO_STATUS - 1] || "").trim();
      if (statusOriginal === "") continue;

      // Normaliza o status (ex: "Produ√ß√£o Finalizada" -> "producaofinalizada")
      const statusNormalizado = normalizarStatus(statusOriginal); 
      
      // --- L√ìGICA DE COMPARA√á√ÉO DIRETA (A PROVA DE FALHAS) ---
      // Em vez de usar .includes(), checamos cada status diretamente,
      // igual √† fun√ß√£o do FormOverview que est√° funcionando.
      if (statusNormalizado === "emproducao" || 
          statusNormalizado === "producaofinalizada" || 
          statusNormalizado === "controlando" || 
          statusNormalizado === "naexpedicao") 
      {
        pedidosParaCQ.push({
          linhaPlanilha: i + 2, 
          numPedido: linha[COL_PEDIDO_NUMERO - 1], 
          dataEntrega: linha[COL_PEDIDO_DATA_ENTREGA - 1], 
          cliente: linha[COL_PEDIDO_CLIENTE - 1], 
          descricao: linha[COL_PEDIDO_DESCRICAO - 1], 
          quantidade: linha[COL_PEDIDO_QUANTIDADE - 1], 
          status: statusOriginal, // Envia o status original (ex: "Produ√ß√£o Finalizada")
          maquinaProd: linha[COL_PEDIDO_MAQUINA - 1], 
          impressor: linha[COL_PEDIDO_IMPRESSOR - 1], 
          fila: linha[COL_PEDIDO_FILA - 1], 
          unidadeCQ: linha[COL_PEDIDO_UNIDADE_CQ - 1], 
          inspetor: linha[COL_PEDIDO_INSPETOR - 1], 
          obsCQ: linha[COL_PEDIDO_OBS_ESPECIAIS - 1], // AH (Observa√ß√µes CQ/Especiais)
          qtdAprovada: linha[COL_PEDIDO_QTD_APROVADA_CQ - 1], // AO
          
          // *** CORRE√á√ÉO: ADICIONA O CAMPO FALTANTE ***
          semExcedente: linha[COL_PEDIDO_SEM_EXCEDENTE - 1] // AE (Col 31)
          // *** FIM DA CORRE√á√ÉO ***
        });
      }
      // --- FIM DA L√ìGICA DE COMPARA√á√ÉO ---
    }

    return {
      pedidos: pedidosParaCQ,
      inspetores: getListaDeAba(NOME_ABA_INSPETORES, 1)
    };
  } catch (e) {
    Logger.log(`Erro ao buscar pedidos para Qualidade: ${e}`);
    return { pedidos: [], inspetores: [], erro: e.message }; 
  }
}
// C√ìDIGO.gs (Fun√ß√£o salvarMudancasQualidade MODIFICADA)

function salvarMudancasQualidade(pedidosAtualizados) {
  if (!pedidosAtualizados || pedidosAtualizados.length === 0) {
    return "Nenhuma altera√ß√£o para salvar.";
  }
  const lock = LockService.getScriptLock();
  if (!lock.tryLock(10000)) {
    throw new Error("O sistema est√° ocupado salvando dados. Tente novamente.");
  }
  try {
    const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
    const ultimoLinha = abaPedidos.getLastRow();
    if (ultimoLinha < 2) throw new Error("Nenhum dado para atualizar.");
    
    const ultimaColuna = TOTAL_COLUNAS_PEDIDOS; // 43 colunas
    const range = abaPedidos.getRange(2, 1, ultimoLinha - 1, ultimaColuna);
    const dados = range.getValues(); // <-- DADOS ANTIGOS DA PLANILHA

    let alteracoes = 0;
    let mensagemLimpeza = ""; 
    
    for (const pedido of pedidosAtualizados) { // <-- MUDAN√áAS NOVAS DO FORMUL√ÅRIO
      const indiceArray = pedido.linhaPlanilha - 2; 

      if (dados[indiceArray]) { 
        
        const statusNovo = pedido.status; 
        const statusAntigo = dados[indiceArray][COL_PEDIDO_STATUS - 1];
        const numPedido = dados[indiceArray][COL_PEDIDO_NUMERO - 1];
        
        // L√≥gica de E-mail/Limpeza/VALIDA√á√ÉO (s√≥ se o status mudar para Expedi√ß√£o ou Conclu√≠do)
        if (statusNovo && statusNovo !== statusAntigo && (statusNovo === "Na Expedi√ß√£o" || statusNovo === "Conclu√≠do")) {
          
          // ========================================================
          // --- BLOQUEIOS DE FINALIZA√á√ÉO DE CQ ---
          // ========================================================

          // 1. Bloqueio: Produ√ß√£o Finalizada (Checa a Coluna AM / 39)
          const dataFimProducao = dados[indiceArray][COL_PEDIDO_DATA_FIM_PROD - 1]; 
          if (!dataFimProducao || String(dataFimProducao).trim() === "") {
            throw new Error(`Pedido ${numPedido}: N√£o pode ser finalizado. A Produ√ß√£o ainda n√£o foi conclu√≠da.`);
          }

          // 2. Bloqueio: Inspetor (Checa o dado que veio do formul√°rio)
          if (!pedido.inspetor || pedido.inspetor.trim() === "") {
            throw new Error(`Pedido ${numPedido}: N√£o pode ser finalizado sem um Inspetor selecionado.`);
          }

          // 3. Bloqueio: Quantidade Aprovada (deve ser > 0)
          const qtdNum = parseFloat(String(pedido.qtdAprovada).replace(',', '.')) || 0;
          if (qtdNum <= 0) {
            throw new Error(`Pedido ${numPedido}: N√£o pode ser finalizado sem uma "Qtd. Aprovada" v√°lida (maior que zero).`);
          }

          // 4. NOVO BLOQUEIO: Pedido "Sem Excedente" (Checa Col AE=31 vs Col D=4)
          // Pega o valor da flag "Sem Excedente" da planilha (Col AE)
          const semExcedenteFlag = String(dados[indiceArray][COL_PEDIDO_SEM_EXCEDENTE - 1]).trim().toUpperCase();
          const isSemExcedente = (semExcedenteFlag === "TRUE" || semExcedenteFlag === "VERDADEIRO" || semExcedenteFlag === "SIM");

          if (isSemExcedente) {
            // √â "Sem Excedente". A Qtd. Aprovada (qtdNum) deve ser IGUAL √† Qtd. Pedido (Col D).
            
            // Pega a Qtd. Pedido original (pode estar formatada como "1.000")
            const qtdPedidoOriginalStr = String(dados[indiceArray][COL_PEDIDO_QUANTIDADE - 1]);
            // Limpa formata√ß√£o (ex: "1.000,00" -> 1000)
            const qtdPedidoOriginalNum = parseFloat(qtdPedidoOriginalStr.replace(/\./g, '').replace(',', '.')) || 0;

            if (qtdNum !== qtdPedidoOriginalNum) {
              // Bloqueia se a quantidade aprovada for diferente da original
              throw new Error(`Pedido ${numPedido}: √© sem excedente. A "Qtd. Aprovada" (${qtdNum}) deve ser EXATAMENTE igual √† "Qtd. do Pedido" (${qtdPedidoOriginalNum}).`);
            }
          }
          
          // ========================================================
          // --- FIM DOS BLOQUEIOS ---
          // ========================================================

          // Se passou nas valida√ß√µes, continua o processo...
          const cliente = dados[indiceArray][COL_PEDIDO_CLIENTE - 1]; 
          const descricao = dados[indiceArray][COL_PEDIDO_DESCRICAO - 1]; 
          const dataEntrega = dados[indiceArray][COL_PEDIDO_DATA_ENTREGA - 1]; 
          
          enviarNotificacaoStatus(statusNovo, numPedido, cliente, descricao, dataEntrega, pedido.motivoAtraso); 
          mensagemLimpeza += limparBobinasSobra(numPedido);
        }
        
        // ATUALIZA√á√ÉO DOS DADOS (O c√≥digo original continua aqui)
        if (pedido.status !== undefined) dados[indiceArray][COL_PEDIDO_STATUS - 1] = pedido.status;   
        if (pedido.unidadeCQ !== undefined) dados[indiceArray][COL_PEDIDO_UNIDADE_CQ - 1] = pedido.unidadeCQ;   
        if (pedido.fila !== undefined) dados[indiceArray][COL_PEDIDO_FILA - 1] = pedido.fila;        
        if (pedido.inspetor !== undefined) dados[indiceArray][COL_PEDIDO_INSPETOR - 1] = pedido.inspetor;
        if (pedido.obsCQ !== undefined) dados[indiceArray][COL_PEDIDO_OBS_ESPECIAIS - 1] = pedido.obsCQ; 

        if (pedido.qtdAprovada !== undefined) {
            const qtdNum = parseFloat(String(pedido.qtdAprovada).replace(',', '.')) || null; 
            dados[indiceArray][COL_PEDIDO_QTD_APROVADA_CQ - 1] = qtdNum; 
        }
        
        if (pedido.status === "Conclu√≠do") {
          dados[indiceArray][COL_PEDIDO_DATA_CONCLUSAO - 1] = new Date(); // Col 8
          if (pedido.motivoAtraso) {
            dados[indiceArray][COL_PEDIDO_MOTIVO_ATRASO - 1] = pedido.motivoAtraso; // Col 26
          }
        }
        alteracoes++;
      }
    }
    
    if (alteracoes > 0) {
      range.setValues(dados);
      SpreadsheetApp.flush();
    }

    return `${alteracoes} altera√ß√µes de pedido foram salvas com sucesso! ${mensagemLimpeza}`;
  } catch (e) {
    Logger.log(`ERRO FATAL ao salvar mudan√ßas da Qualidade: ${e.message} ${e.stack}`);
    throw new Error(`${e.message}`);
  } finally {
    lock.releaseLock();
  }
}

/**
 * ==========================================================
 * FUN√á√ÉO UNIFICADA DE E-MAIL (AGORA USANDO O NOVO TEMPLATE)
 * ==========================================================
 */
function enviarNotificacaoStatus(statusNovo, numPedido, cliente, descricao, dataEntrega, motivoAtraso) {
  let assunto = "";
  let titulo = "";
  let introTexto = "";
  let statusFormatado = statusNovo.toUpperCase(); // "CONCLU√çDO", "NA EXPEDI√á√ÉO"

  // 1. Define o texto de acordo com o status
  switch (statusNovo) {
    case "Liberado para produ√ß√£o":
      assunto = `Pedido ${numPedido} - Liberado para Produ√ß√£o`;
      titulo = "Pedido Liberado para Produ√ß√£o";
      introTexto = "O PCP liberou o seguinte pedido:";
      break;
    
    case "Na Expedi√ß√£o":
      assunto = `Pedido ${numPedido} - Na Expedi√ß√£o`;
      titulo = "Pedido Pronto para Faturamento";
      introTexto = "O seguinte pedido concluiu o controle de qualidade e est√° aguardando faturamento e envio:";
      break;
    
    case "Conclu√≠do":
      assunto = `Pedido ${numPedido} - Conclu√≠do`;
      titulo = "Pedido Conclu√≠do";
      introTexto = "O pedido abaixo foi finalizado no sistema:";
      break;
    
    default:
      return; // N√£o envia e-mail para outros status
  }

  // ---- IN√çCIO DA NOVA L√ìGICA DE ATRASO ----
  
  let motivoHtml = ""; // Esta vari√°vel vai guardar o HTML do motivo
  
  // S√≥ executa esta l√≥gica se o status for "Conclu√≠do" E se uma data de entrega foi fornecida
  if (statusNovo === "Conclu√≠do" && dataEntrega) {
    const hoje = new Date();
    const dataEntregaPedido = new Date(dataEntrega);

    // Zera o hor√°rio das datas para comparar apenas o dia
    hoje.setHours(0, 0, 0, 0);
    dataEntregaPedido.setHours(0, 0, 0, 0);

    // Compara se HOJE √© DEPOIS da data de entrega
    if (hoje > dataEntregaPedido) {
      
      // Se estiver atrasado, verifica se um motivo foi preenchido
      if (motivoAtraso && motivoAtraso.trim() !== "") {
        // Se SIM, formata o motivo em vermelho
        motivoHtml = `<p style="color: #D32F2F; font-weight: bold;">Motivo do Atraso: ${motivoAtraso}</p>`;
      } else {
        // Se N√ÉO, informa que est√° atrasado mas sem motivo
        motivoHtml = `<p style="color: #D32F2F; font-weight: bold;">ATEN√á√ÉO: Pedido conclu√≠do com atraso, mas o motivo n√£o foi informado.</p>`;
      }
    }
  }
  // ---- FIM DA NOVA L√ìGICA DE ATRASO ----

  // 2. Monta o corpo interno (layout da imagem)
  const corpoInternoHtml = `
¬† ¬† <p>${introTexto}</p>
¬† ¬† <p><strong>Status:</strong> ${statusFormatado}</p>
    ${motivoHtml}
¬† ¬† <p><strong>N¬∫ Pedido:</strong> ${numPedido}</p>
¬† ¬† <p><strong>Cliente:</strong> ${cliente}</p>
¬† ¬† <p><strong>Descri√ß√£o:</strong> ${descricao}</p>
¬† `;
  
  // 3. Envia usando a nova fun√ß√£o central
  enviarEmailSistema(assunto, titulo, corpoInternoHtml);
}

