/**
 * NOVO (V1.3): Fun√ß√£o Auxiliar para encontrar uma forma (caixa) no Slide pela tag.
 */
function _findShapeWithText(slide, text) {
  const shapes = slide.getShapes();
  for (const shape of shapes) {
    if (shape.getText().asString().includes(text)) {
      return shape;
    }
  }
  // Se n√£o achar em Formas, procura em Tabelas
  const tables = slide.getTables();
  for (const table of tables) {
    for (let r = 0; r < table.getNumRows(); r++) {
      for (let c = 0; c < table.getNumColumns(); c++) {
        const cell = table.getCell(r, c);
        if (cell.getText().asString().includes(text)) {
          return cell; // Retorna a C√©lula como se fosse uma Forma
        }
      }
    }
  }
  return null;
}

/**
 * NOVO (V1.3): Gera a Ficha T√©cnica em PDF a partir de um produto.
 * (Chamado pelo 'FormProduto.html')
 */
function gerarFichaTecnicaPDF(produto) {
  try {
    const NOME_PASTA_PDF = "Fichas T√©cnicas Geradas"; // Pasta de sa√≠da
    const template = DriveApp.getFileById(TEMPLATE_FICHA_TECNICA_ID);
    
    // --- 1. Encontrar ou Criar Pasta de Sa√≠da ---
    let pastaDestino;
    const pastas = DriveApp.getFoldersByName(NOME_PASTA_PDF);
    if (pastas.hasNext()) {
      pastaDestino = pastas.next();
    } else {
      pastaDestino = DriveApp.createFolder(NOME_PASTA_PDF);
    }

    // --- 2. Criar C√≥pia Tempor√°ria do Template ---
    const nomeArquivoTemp = `TEMP_FT_${produto.codigo}`;
    const copiaTemp = template.makeCopy(nomeArquivoTemp, pastaDestino);
    const apresentacao = SlidesApp.openById(copiaTemp.getId());
    const slide = apresentacao.getSlides()[0]; // Pega o primeiro slide

    // --- 3. Substituir Tags de Texto Simples ---
    slide.replaceAllText("{{CLIENTE}}", produto.cliente || "N/A");
    slide.replaceAllText("{{CODIGO_PRODUTO}}", produto.codigo || "N/A");
    slide.replaceAllText("{{DESCRICAO}}", produto.descricao || "N/A");
    slide.replaceAllText("{{FACA}}", produto.faca || "N/A");
    slide.replaceAllText("{{LARGURA}}", produto.largura || "N/A");
    slide.replaceAllText("{{CARREIRAS}}", produto.carreiras || "N/A");
    slide.replaceAllText("{{SUBSTRATO}}", produto.substrato || "N/A");
    slide.replaceAllText("{{TIPO_COLA}}", produto.tipoCola || "N/A");
    slide.replaceAllText("{{PUXADA}}", produto.puxada || "N/A");
    slide.replaceAllText("{{ALTURA}}", produto.altura || "N/A");
    slide.replaceAllText("{{DIAMETRO_TUBETE}}", produto.diametroTubete || "N/A");
    slide.replaceAllText("{{QTD_POR_ROLO}}", produto.qtdPorRolo || "N/A");
    
    // Substitui tags de acabamento (que n√£o est√£o na tabela de cores)
    slide.replaceAllText("{{ACABAMENTO}}", produto.acabamento || "N/A");
    slide.replaceAllText("{{COLD}}", produto.cold || "N/A");

    // --- 4. Preencher a Tabela de Cores (JSON) ---
    const fichaCores = JSON.parse(produto.fichaTecnicaCores || "[]");
    for (let i = 0; i < 7; i++) { // Loop 7 vezes (para {{COR_1...}} at√© {{COR_7...}})
      const estacao = fichaCores[i]; // Pega o dado (ex: {cor: "Ciano", ...})
      const estacaoNum = i + 1;

      if (estacao && estacao.cor) {
        // Se houver dados, preenche
        slide.replaceAllText(`{{COR_${estacaoNum}_TIPO}}`, estacao.cor);
        slide.replaceAllText(`{{COR_${estacaoNum}_PANTONE}}`, estacao.pantone || "---");
        slide.replaceAllText(`{{COR_${estacaoNum}_ANILOX}}`, estacao.anilox || "N/A");
      } else {
        // Se n√£o houver dados (ex: s√≥ 5 cores), limpa as tags restantes
        slide.replaceAllText(`{{COR_${estacaoNum}_TIPO}}`, "---");
        slide.replaceAllText(`{{COR_${estacaoNum}_PANTONE}}`, "---");
        slide.replaceAllText(`{{COR_${estacaoNum}_ANILOX}}`, "---");
      }
    }

    // --- 5. Substituir Imagem do LAYOUT DE ARTE ---
    const shapeLayout = _findShapeWithText(slide, "{{LAYOUT_ARTE}}");
    if (shapeLayout) {
      if (produto.layoutArte && produto.layoutArte.startsWith("http")) {
        const blob = UrlFetchApp.fetch(produto.layoutArte).getBlob();
        shapeLayout.replaceWithImage(blob);
      } else if (produto.layoutArte && produto.layoutArte.startsWith("PDF_FILE::")) {
        shapeLayout.getText().setText("Arte em PDF (anexada separadamente)");
      } else {
        shapeLayout.getText().setText("Layout n√£o fornecido");
      }
    }

    // --- 6. Substituir Imagem do SENTIDO DE IMPRESS√ÉO ---
    const shapeSentido = _findShapeWithText(slide, "{{SENTIDO_IMPRESSAO}}");
    if (shapeSentido) {
      const sentidoId = produto.sentidoImpressao; // Ex: "1", "2", "3" ou "4"
      if (sentidoId && SENTIDO_IMPRESSAO_IMAGENS[sentidoId]) {
        // Decodifica o SVG Base64
        const base64Data = SENTIDO_IMPRESSAO_IMAGENS[sentidoId].split(',')[1];
        const bytes = Utilities.base64Decode(base64Data);
        const blob = Utilities.newBlob(bytes, "image/svg+xml");
        shapeSentido.replaceWithImage(blob);
      } else {
        shapeSentido.getText().setText("Sentido n√£o definido");
      }
    }

    // --- 7. Salvar, Gerar PDF e Limpar ---
    apresentacao.saveAndClose();
    
    const pdfBlob = copiaTemp.getAs(MimeType.PDF);
    const nomePdf = `FT - ${produto.codigo} - ${produto.descricao}.pdf`;
    const pdfFile = pastaDestino.createFile(pdfBlob).setName(nomePdf);
    
    pdfFile.setSharing(DriveApp.Access.ANYONE_WITH_LINK, DriveApp.Permission.VIEW);
    
    copiaTemp.setTrashed(true); // Exclui a c√≥pia tempor√°ria do slide
    
    return pdfFile.getUrl(); // Retorna o link do PDF para o frontend

  } catch (e) {
    Logger.log(`Erro fatal ao gerar PDF: ${e.message}\nStack: ${e.stack}`);
    throw new Error(`Falha ao gerar PDF: ${e.message}`);
  }
}

