/**
 * FUN√á√ÉO AUXILIAR: Busca um pedido pelo n√∫mero e retorna sua linha.
 * @param {string} numPedido O n√∫mero do pedido.
 * @returns {number} A linha da planilha (base 1).
 * @throws {Error} Se o pedido n√£o for encontrado, ou se j√° estiver Conclu√≠do/Cancelado.
 */
function getLinhaPedidoPeloNumero(numPedido) {
  const abaPedidos = SpreadsheetApp.getActiveSpreadsheet().getSheetByName(NOME_ABA_PEDIDOS);
  // L√™ da Col A (N¬∫ Pedido) at√© a Col I (Status)
  const dados = abaPedidos.getRange(2, COL_PEDIDO_NUMERO, abaPedidos.getLastRow() - 1, COL_PEDIDO_STATUS).getValues(); 
  const numBusca = String(numPedido).trim();

  for (let i = 0; i < dados.length; i++) {
    const numPlanilha = String(dados[i][COL_PEDIDO_NUMERO - 1]).trim(); // Col A (√≠ndice 0)

    if (numPlanilha === numBusca) {
      const status = dados[i][COL_PEDIDO_STATUS - 1]; // Col I (√≠ndice 8)
      
      if (status === "Conclu√≠do" || status === "Cancelado") {
        throw new Error(`O Pedido ${numPedido} j√° est√° ${status} e n√£o pode ter problemas reportados.`);
      }
      
      return i + 2; // Retorna a linha real da planilha (base 1)
    }
  }
  
  // Se saiu do loop, n√£o encontrou
  throw new Error(`Pedido ${numPedido} n√£o encontrado na planilha.`);
}

