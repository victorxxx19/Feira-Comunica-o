   <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script>
    // --- FUNÇÕES UTILITÁRIAS GLOBAIS ---
    const formatNumber = (num, decimals = 0) => {
        const safeNum = num || 0;
        return safeNum.toLocaleString('pt-BR', {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals
        });
    };
    const formatPercent = (num, decimals = 2) => {
        const safeNum = num || 0;
        return safeNum.toLocaleString('pt-BR', {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals
        }) + ' <span class="unit">%</span>';
    };


    // --- OPÇÕES DE GRÁFICO (ATUALIZADAS) ---
    const baseChartOptions = {
        width: 680,
        height: 400,
        chartArea: { width: '78%', height: '70%', left: '10%', top: '10%' },
        legend: { position: 'none' },
        hAxis: {
            textStyle: { color: '#333333', fontSize: 12, bold: true },
            titleTextStyle: { color: '#333333', fontSize: 14, bold: true, italic: false },
            baselineColor: '#ccc',
            gridlines: { color: 'transparent' }
        },
        vAxis: {
            textStyle: { color: '#666', fontSize: 12 },
            minValue: 0,
            format: '#.###',
            gridlines: { color: 'transparent' },
            baselineColor: '#ccc',
            titleTextStyle: { color: '#333333', fontSize: 14, bold: true, italic: false }
        },
        animation: { duration: 1000, easing: 'out', startup: true },
        bar: { groupWidth: '85%' },
        tooltip: { textStyle: { fontSize: 13 } },
        dataOpacity: 1.0,
        annotations: {
            alwaysOutside: true,
            stem: { length: 10, color: 'transparent' },
            textStyle: {
                fontSize: 14,
                bold: true,
     	 	 	color: '#000000',
                auraColor: 'none',
                opacity: 1.0
            }
        }
    };

    // --- OPÇÕES DO GRÁFICO HORIZONTAL (CORRIGIDO) ---
    // --- CORREÇÃO 1: OPÇÕES DO GRÁFICO HORIZONTAL ---
    const hBarChartOptions = {
      // O gráfico deve ter 100% de largura para caber no 'col m6'
      width: '100%', 
      // A altura será definida dinamicamente pela função drawHBarChart
      
      chartArea: { width: '60%', height: '85%', left: '25%', top: '5%' }, 
      legend: { position: 'none' }, 
      
      hAxis: { 
     	 textStyle: { color: '#666', fontSize: 12 },
     	 minValue: 0,
     	 format: '#,###', 
     	 baselineColor: '#ccc',
     	 titleTextStyle: { color: '#333333', fontSize: 14, bold: true, italic: false },
     	 gridlines: { color: 'transparent' } // Remove as linhas de grade
      },
      vAxis: { 
     	 textStyle: { color: '#333333', fontSize: 11, bold: true }, 
     	 titleTextStyle: { color: '#333333', fontSize: 14, bold: true, italic: false }
      },
      animation: { duration: 1000, easing: 'out', startup: true },
      tooltip: { textStyle: { fontSize: 13 } },
      dataOpacity: 1.0,
      annotations: {
        alwaysOutside: true,
        stem: { length: 10, color: 'transparent' },
        textStyle: {
          fontSize: 12,
          bold: true,
          color: '#000000',
          auraColor: 'none'
        }
      },
      colors: ['#0047BB']
    };
    // ----------------------------------------------------

    const productivityChartOptions = { ...baseChartOptions, colors: ['#37417c'] };
    const refugoChartOptions = { ...baseChartOptions, colors: ['#f44336'] };
    const refugoPercentageChartOptions = {
        ...refugoChartOptions,
        vAxis: { ...refugoChartOptions.vAxis, format: '#,##0.00\'%\'', viewWindow: { min: 0, max: 100 } },
    };
    const goodMeterChartOptions = { ...baseChartOptions, colors: ['#4CAF50'] };
    
     const pieOptions = {
        width: 650,
        height: 400,
        chartArea: { width: '85%', height: '80%', left: '7.5%', top: '10%' },
        legend: { position: 'none' },
        animation: { duration: 1000, easing: 'out', startup: true },
        colors: ['#4CAF50', '#D32F2F'],
        is3D: false,
        pieSliceText: 'percentage', 
        pieSliceTextStyle: { color: 'black', fontSize: 14, bold: true },
        tooltip: { textStyle: { fontSize: 13 } }
     };

     const stackedBarChartOptions = {
        ...baseChartOptions,
        height: 400,
        width: 1400,
        isStacked: true,
        colors: ['#4CAF50', '#f44336'],
        legend: { position: 'none' },
        chartArea: { width: '85%', height: '70%', left: '10%', top: '10%' }
     };


     // --- INICIALIZAÇÃO E CARREGAMENTO ---
     google.charts.load('current', { 'packages': ['corechart'] });
     google.charts.setOnLoadCallback(carregarDados);

     // VARIÁVEL GLOBAL
     let dashboardData = {};

     function carregarDados() {
        google.script.run
          .withSuccessHandler(onDataLoaded)
          .withFailureHandler(onDataError)
          .getDadosDashboard();
     }

     function formatMesKey(mesKey) {
        if (mesKey === "TOTAL") return "Total Anual";
        const [ano, mes] = mesKey.split('-');
        const dataObj = new Date(ano, mes - 1, 1);
        const nomeMes = dataObj.toLocaleString('pt-BR', { month: 'short' });
        return `${nomeMes.charAt(0).toUpperCase() + nomeMes.slice(1).replace('.', '')}/${ano.slice(2)}`;
     }

     // --- LÓGICA DE FILTROS ---

     function handleFilterChange(mesKey) {
        updateRefugoData(mesKey);
        updateProdutividadeData(mesKey);
        updateInsumosCharts(mesKey);
        updateClichesData(mesKey); // <-- ADICIONADO
     }

     function updateRefugoData(mesKey) {
        // ... (código sem mudança) ...
        let kpiData, chartData, labelPrefix, labelSuffix;
        if (mesKey === "TOTAL") {
          kpiData = dashboardData.kpis;
          chartData = dashboardData.graficos;
          labelPrefix = "Refugo Total"; labelSuffix = "Anual";
        } else {
          if (!dashboardData.dadosMensais || !dashboardData.dadosMensais[mesKey]) return;
          kpiData = dashboardData.dadosMensais[mesKey].kpis;
          chartData = dashboardData.dadosMensais[mesKey].graficos;
          labelPrefix = "Refugo Total"; labelSuffix = formatMesKey(mesKey);
        }
        document.querySelector('#kpi_refugoTotal').previousElementSibling.innerHTML = `${labelPrefix} ${labelSuffix}`;
        document.querySelector('#kpi_taxaRefugo').previousElementSibling.innerHTML = `Taxa de Refugo ${labelSuffix}`;
        document.getElementById('kpi_refugoTotal').innerHTML = `${formatNumber(kpiData.refugoTotal, 0)} <span class="unit">m</span>`;
        document.getElementById('kpi_taxaRefugo').innerHTML = formatPercent(kpiData.taxaRefugo, 2);
        const calcularMax = (arr) => (!arr || arr.length <= 1) ? null : Math.max(...arr.slice(1).map(r => r[1])) * 1.2;
        drawBarChart('chart_refugo_provocado_maquina', chartData.refugoProvocadoPorMaquina, refugoChartOptions, 'Máquina', 'Refugo Provocado (m)', '#,###', calcularMax(chartData.refugoProvocadoPorMaquina));
        drawBarChart('chart_refugo_obrigatorio_maquina', chartData.refugoObrigatorioPorMaquina, refugoChartOptions, 'Máquina', 'Refugo Obrigatório (m)', '#,###', calcularMax(chartData.refugoObrigatorioPorMaquina));
        drawBarChart('chart_refugo_provocado_impressor', chartData.refugoProvocadoPorImpressor, refugoChartOptions, 'Impressor', 'Refugo Provocado (m)', '#,###', calcularMax(chartData.refugoProvocadoPorImpressor));
        drawBarChart('chart_refugo_obrigatorio_impressor', chartData.refugoObrigatorioPorImpressor, refugoChartOptions, 'Impressor', 'Refugo Obrigatório (m)', '#,###', calcularMax(chartData.refugoObrigatorioImpressor));
     }

     function updateProdutividadeData(mesKey) {
        // ... (código sem mudança) ...
        let kpiData, chartData, labelSuffix;
        if (mesKey === "TOTAL") {
          kpiData = dashboardData.kpis.metragemTotal;
          chartData = dashboardData.graficos.produtividadeImpressor;
          labelSuffix = "Anual";
        } else {
          if (!dashboardData.dadosMensais || !dashboardData.dadosMensais[mesKey]) return;
          kpiData = dashboardData.dadosMensais[mesKey].kpis.metragemTotal;
          chartData = dashboardData.dadosMensais[mesKey].graficos.produtividadeImpressor;
          labelSuffix = formatMesKey(mesKey);
        }
        document.querySelector('#kpi_metragemBrutaTotal').previousElementSibling.innerHTML = `Metragem Bruta Total ${labelSuffix}`;
        document.querySelector('#kpi_metragemLiquidaTotal').previousElementSibling.innerHTML = `Metragem Líquida Total ${labelSuffix}`;
        document.getElementById('kpi_metragemBrutaTotal').innerHTML = `${formatNumber(kpiData.bruta, 0)} <span class="unit">m</span>`;
        document.getElementById('kpi_metragemLiquidaTotal').innerHTML = `${formatNumber(kpiData.liquida, 0)} <span class="unit">m</span>`;
        const calcularMax = (arr) => (!arr || arr.length <= 1) ? null : Math.max(...arr.slice(1).map(r => r[1])) * 1.2;
        drawBarChart('chart_imp_consumida', chartData.consumida, productivityChartOptions, 'Impressor', 'Metragem Bruta (m)', '#,###', calcularMax(chartData.consumida));
        drawBarChart('chart_imp_boa', chartData.boa, goodMeterChartOptions, 'Impressor', 'Metragem Líquida (m)', '#,###', calcularMax(chartData.boa));
     }

     // --- CORREÇÃO 1: FUNÇÃO DE FILTRO DE INSUMOS (Bug "Não há dados") ---
     function updateInsumosCharts(mesKeyParam = null) {
       
       const mesKey = mesKeyParam || document.getElementById('filtroMes').value;
       const selectedFaca = document.getElementById('filtroFaca').value;
       const selectedProduto = document.getElementById('filtroProduto').value;

       let dataFaca, dataProduto;
       
       // Criamos uma CÓPIA profunda dos dados para não "contaminar" a variável global
       if (mesKey === "TOTAL") {
         // Usamos JSON.parse(JSON.stringify()) para criar uma cópia e quebrar a referência
         dataFaca = JSON.parse(JSON.stringify(dashboardData.graficos.metrosPorFaca));
         dataProduto = JSON.parse(JSON.stringify(dashboardData.graficos.rotulosPorProduto));
       } else {
         if (!dashboardData.dadosMensais || !dashboardData.dadosMensais[mesKey] || !dashboardData.dadosMensais[mesKey].graficos.metrosPorFaca) {
           console.warn("Dados mensais de insumos não encontrados para:", mesKey);
           dataFaca = [["Faca", "Metros (m)", { role: "annotation" }]];
           dataProduto = [["Produto", "Rótulos (Qtd)", { role: "annotation" }]];
         } else {
           // Copia os dados mensais
           dataFaca = JSON.parse(JSON.stringify(dashboardData.dadosMensais[mesKey].graficos.metrosPorFaca));
           dataProduto = JSON.parse(JSON.stringify(dashboardData.dadosMensais[mesKey].graficos.rotulosPorProduto));
         }
       }
       
       // Agora filtramos a CÓPIA, não o original
       if (selectedFaca !== "TODAS") {
         const header = dataFaca[0];
         const filteredData = dataFaca.slice(1).filter(row => row[0] === selectedFaca);
         dataFaca = [header, ...filteredData];
       }
       
       if (selectedProduto !== "TODOS") {
         const header = dataProduto[0];
         const filteredData = dataProduto.slice(1).filter(row => row[0] === selectedProduto);
         dataProduto = [header, ...filteredData];
       }
       
       // === ALTERAÇÃO JS ===
       // Desenha o gráfico de Facas com os dados copiados e filtrados
       drawHBarChart('chart_metros_por_faca', dataFaca, hBarChartOptions, 'Faca', 'Metros (m)', '#,###');
       
       // A linha abaixo foi removida pois o gráfico 'chart_rotulos_por_produto' não existe mais
       // drawHBarChart('chart_rotulos_por_produto', dataProduto, hBarChartOptions, 'Produto', 'Rótulos (Qtd)', '#,###');
       // === FIM DA ALTERAÇÃO ===
     }
     // --- FIM DA CORREÇÃO 1 ---

     // --- FUNÇÃO PRINCIPAL ---
     function onDataLoaded(data) {
        console.log("Dados recebidos:", data);
        if (data.erro) {
          onDataError({ message: data.erro });
          return;
        }
        dashboardData = data;

        try {
             // (NOVO) Preenche o ano em todos os KPIs
             const ano = (data.kpis.ano) ? data.kpis.ano : new Date().getFullYear();
             document.querySelectorAll('.kpi-ano').forEach(span => {
                 span.textContent = ano;
             });

          // 1. PREENCHER KPIs (ANUAIS)
          // ... (código sem mudança) ...
          const velMaquinas = data.kpis.velocidadeMediaPorMaquina || {};
          const metragem = data.kpis.metragemTotal || {};
          document.getElementById('kpi_metragemBrutaTotal').innerHTML = `${formatNumber(metragem.bruta, 0)} <span class="unit">m</span>`;
          document.getElementById('kpi_metragemLiquidaTotal').innerHTML = `${formatNumber(metragem.liquida, 0)} <span class="unit">m</span>`;
          document.getElementById('kpi_refugoTotal').innerHTML = `${formatNumber(data.kpis.refugoTotal, 0)} <span class="unit">m</span>`;
          document.getElementById('kpi_taxaRefugo').innerHTML = formatPercent(data.kpis.taxaRefugo, 2);
          document.getElementById('kpi_pedidosConcluidos').innerHTML = formatNumber(data.kpis.pedidosConcluidos, 0);
          document.getElementById('kpi_tempoMedioAtraso').innerHTML = `${formatNumber(data.kpis.tempoMedioAtraso, 1)} <span class="unit">dias</span>`;
          document.getElementById('kpi_taxaMediaAtraso').innerHTML = formatPercent(data.kpis.taxaMediaAtraso, 2);
          document.getElementById('kpi_velocidade_superprint').innerHTML = `${formatNumber(velMaquinas['Superprint 250'], 2)} <span class="unit">m/min</span>`;
          document.getElementById('kpi_velocidade_etirama').innerHTML = `${formatNumber(velMaquinas['Etirama Fit'], 2)} <span class="unit">m/min</span>`;
          document.getElementById('kpi_velocidade_kromia1').innerHTML = `${formatNumber(velMaquinas['Kromia I'], 2)} <span class="unit">m/min</span>`;
          document.getElementById('kpi_velocidade_kromia2').innerHTML = `${formatNumber(velMaquinas['Kromia II'], 2)} <span class="unit">m/min</span>`;

          // 2. CALCULAR MÁXIMOS (ANUAIS)
          // ... (código sem mudança) ...
          const calcularMax = (arr) => (!arr || arr.length <= 1) ? null : Math.max(...arr.slice(1).map(r => r[1])) * 1.20;
          const viewWindowMaxIAL = data.kpis.maxPedidosMensal ? data.kpis.maxPedidosMensal + 3 : null;
          const maxRefugoTotalMensal = calcularMax(data.graficos.refugoTotalMensal);
          const maxRefugoProvocadoMaquina = calcularMax(data.graficos.refugoProvocadoPorMaquina);
          const maxRefugoObrigatorioMaquina = calcularMax(data.graficos.refugoObrigatorioPorMaquina);
          const maxRefugoProvocadoImpressor = calcularMax(data.graficos.refugoProvocadoPorImpressor);
          const maxRefugoObrigatorioImpressor = calcularMax(data.graficos.refugoObrigatorioPorImpressor);
          const maxMetragemBoaMensal = calcularMax(data.graficos.metragemBoaMensal);
          const maxMetragemImpConsumida = calcularMax(data.graficos.produtividadeImpressor.consumida);
          const maxMetragemImpBoa = calcularMax(data.graficos.produtividadeImpressor.boa);
          const maxVelSuperprint = calcularMax(data.graficos.velocidadeMediaMensalPorMaquina['Superprint 250']);
          const maxVelEtirama = calcularMax(data.graficos.velocidadeMediaMensalPorMaquina['Etirama Fit']);
          const maxVelKromia1 = calcularMax(data.graficos.velocidadeMediaMensalPorMaquina['Kromia I']);
          const maxVelKromia2 = calcularMax(data.graficos.velocidadeMediaMensalPorMaquina['Kromia II']);

          // 3. DESENHAR GRÁFICOS (ANUAIS)
          // --- SEÇÃO IAL ---
          drawPieChart('chart_ial', data.graficos.ial, pieOptions);
          drawBarChart('chart_ial_mensal_empilhado', data.graficos.ialMensalEmpilhado, stackedBarChartOptions, 'Mês', 'Qtd. Pedidos', '#,###', viewWindowMaxIAL);
          drawBarChart('chart_ial_taxa_atraso_mensal', data.graficos.ialTaxaAtrasoMensal, refugoPercentageChartOptions, 'Mês', '% Pedidos Atrasados', '#,##0.00\'%\'');
          // --- SEÇÃO REFUGO ---
          drawBarChart('chart_refugo_total_mensal', data.graficos.refugoTotalMensal, refugoChartOptions, 'Mês', 'Refugo Total (m)', '#,###', maxRefugoTotalMensal);
          drawBarChart('chart_taxa_refugo_mensal', data.graficos.taxaRefugoMensal, refugoPercentageChartOptions, 'Mês', 'Taxa Refugo (%)', '#,##0.00\'%\'');
          drawBarChart('chart_refugo_provocado_maquina', data.graficos.refugoProvocadoPorMaquina, refugoChartOptions, 'Máquina', 'Refugo Provocado (m)', '#,###', maxRefugoProvocadoMaquina);
          drawBarChart('chart_refugo_obrigatorio_maquina', data.graficos.refugoObrigatorioPorMaquina, refugoChartOptions, 'Máquina', 'Refugo Obrigatório (m)', '#,###', maxRefugoObrigatorioMaquina);
          drawBarChart('chart_refugo_provocado_impressor', data.graficos.refugoProvocadoPorImpressor, refugoChartOptions, 'Impressor', 'Refugo Provocado (m)', '#,###', maxRefugoProvocadoImpressor);
          drawBarChart('chart_refugo_obrigatorio_impressor', data.graficos.refugoObrigatorioPorImpressor, refugoChartOptions, 'Impressor', 'Refugo Obrigatório (m)', '#,###', maxRefugoObrigatorioImpressor);
          // --- SEÇÃO PRODUTIVIDADE ---
          drawBarChart('chart_metragem_boa_mensal', data.graficos.metragemBoaMensal, goodMeterChartOptions, 'Mês', 'Metragem Boa (m)', '#,###', maxMetragemBoaMensal);
          drawBarChart('chart_imp_consumida', data.graficos.produtividadeImpressor.consumida, productivityChartOptions, 'Impressor', 'Metragem Bruta (m)', '#,###', maxMetragemImpConsumida);
          drawBarChart('chart_imp_boa', data.graficos.produtividadeImpressor.boa, goodMeterChartOptions, 'Impressor', 'Metragem Líquida (m)', '#,###', maxMetragemImpBoa);
    	 	 	drawBarChart('chart_velocidade_superprint', data.graficos.velocidadeMediaMensalPorMaquina['Superprint 250'], productivityChartOptions, 'Mês', 'Velocidade Média (m/min)', '#,##0.0', maxVelSuperprint);
          drawBarChart('chart_velocidade_etirama', data.graficos.velocidadeMediaMensalPorMaquina['Etirama Fit'], productivityChartOptions, 'Mês', 'Velocidade Média (m/min)', '#,##0.0', maxVelEtirama);
          drawBarChart('chart_velocidade_kromia1', data.graficos.velocidadeMediaMensalPorMaquina['Kromia I'], productivityChartOptions, 'Mês', 'Velocidade Média (m/min)', '#,##0.0', maxVelKromia1);
          drawBarChart('chart_velocidade_kromia2', data.graficos.velocidadeMediaMensalPorMaquina['Kromia II'], productivityChartOptions, 'Mês', 'Velocidade Média (m/min)', '#,##0.0', maxVelKromia2);

          // === ALTERAÇÃO JS ===
          // --- SEÇÃO INSUMOS E PRODUTOS (CHAMADA INICIAL) ---
          
          // Esta linha agora desenha no DIV 'chart_metros_por_faca' que está na seção de Produtividade
          drawHBarChart('chart_metros_por_faca', data.graficos.metrosPorFaca, hBarChartOptions, 'Faca', 'Metros (m)', '#,###');
          
          // A linha 'chart_rotulos_por_produto' foi removida
          // A linha 'chart_metros_por_cliche' foi removida
          // -------------------------------------------------------------
          // === FIM DA ALTERAÇÃO ===

        } catch (e) {
          onDataError({ message: "Erro ao desenhar gráficos: " + e.message });
          return;
        }

        // 4. INICIALIZAR COMPONENTES E LISTENERS
        
        document.getElementById('filtroMes').addEventListener('change', (e) => {
          handleFilterChange(e.target.value); 
        });
        
        document.getElementById('filtroFaca').addEventListener('change', updateInsumosCharts);
        document.getElementById('filtroProduto').addEventListener('change', updateInsumosCharts);
        // ----------------------------------------

        // --- (NOVO) CHAMA A FUNÇÃO DE DESENHO DE CLICHÊS ---
        if (data.cliches) {
          desenharGraficosCliches(data.cliches);
        }
        // --- FIM DA ADIÇÃO ---

        // 5. ESCONDER O LOADING
        document.getElementById('loading-overlay').style.display = 'none';

     } // <-- FIM DA FUNÇÃO onDataLoaded

     function onDataError(error) {
        console.error("Erro ao carregar dados do Dashboard:", error);
        const loadingEl = document.getElementById('loading-overlay');
        loadingEl.innerHTML = `<h5 style="color: red; padding: 20px;">Falha ao carregar dados: ${error.message}</h5>`;
     }


// --- FUNÇÕES DE DESENHO DE GRÁFICO ---

     function drawBarChart(elementId, dataArray, options, hAxisTitle, vAxisTitle, annotationFormat = '#,###', vAxisMax = null) {
        const el = document.getElementById(elementId);
        if (!el) { console.error("Elemento não encontrado: " + elementId); return; }
        if (!dataArray || dataArray.length <= 1) {
          el.innerHTML = "<p style='text-align:center; padding-top: 80px; color: #888;'>Não há dados para exibir.</p>";
          el.className = "chart-container-no-data";
          return;
        }

        let data;
        
        // --- INÍCIO DA CORREÇÃO (Gráficos de Velocidade) ---
        
        // 1. Verifica se é um gráfico de velocidade (que precisa de formatação decimal)
        const isVelocityChart = (annotationFormat === '#,##0.0');

        if (isVelocityChart && dataArray.length > 1 && dataArray[0].length === 3) {
          // SE FOR GRÁFICO DE VELOCIDADE:
          // O backend está enviando a anotação arredondada (ex: 50.5 -> "51").
          // Vamos corrigir a anotação (coluna 2) manualmente.

          const formatter = new google.visualization.NumberFormat({
            pattern: annotationFormat, // '#,##0.0'
            decimalSymbol: ',',
            groupingSymbol: '.'
          });

          // Loop começa em 1 para pular o cabeçalho
          for (let i = 1; i < dataArray.length; i++) {
            const value = dataArray[i][1]; // O valor numérico (ex: 50.5)
            if (value === 0 || value === null) {
              dataArray[i][2] = null; // Zera a anotação
            } else {
              // Formata o valor numérico e o coloca como a string de anotação
              dataArray[i][2] = formatter.formatValue(value); // ex: "50,5"
            }
          }
          
          // Agora o dataArray está: [["Mês", ...], ["Jan/25", 50.5, "50,5"]]
          data = google.visualization.arrayToDataTable(dataArray);
          formatter.format(data, 1); // Formata a coluna 1 para o tooltip/eixo

        } else {
          // SE FOR QUALQUER OUTRO GRÁFICO (Refugo, IAL, etc.):
          // Usa a lógica padrão que já funcionava.
          
          for (let i = 1; i < dataArray.length; i++) {
            const value = dataArray[i][1];
            if (value === 0 || value === null || typeof value === 'undefined') { 
              if(dataArray[i].length > 2) dataArray[i][2] = null; // Esconde anotação da col 2
              if(dataArray[i].length > 4) dataArray[i][4] = null; // Esconde anotação da col 4 (stacked)
            }
          }
          
          data = google.visualization.arrayToDataTable(dataArray);

          // Formata as colunas de dados (para tooltips e eixos)
          if (annotationFormat) {
            const pattern = annotationFormat.includes('%') ? '#,##0.00' : '#,###';
            const formatter = new google.visualization.NumberFormat({
              pattern: pattern, 
              decimalSymbol: ',',
              groupingSymbol: '.'
            });
            formatter.format(data, 1); // Formata a coluna 1 (ex: "No Prazo")
            if(data.getNumberOfColumns() > 3) {
             formatter.format(data, 3); // Formata a coluna 3 (ex: "Atrasados" no stacked)
            }
          }
        }
        // --- FIM DA CORREÇÃO ---


        let customOptions = { ...options };
        customOptions.hAxis = { ...customOptions.hAxis, title: hAxisTitle };
        customOptions.vAxis = { ...customOptions.vAxis, title: vAxisTitle };

        if (vAxisMax !== null) {
          customOptions.vAxis.viewWindow = { max: vAxisMax, min: 0 };
        } else if (!annotationFormat.includes('%')) { 
          customOptions.vAxis.viewWindow = { min: 0 };
        }

        // Aplica formatação ao Eixo Y
        if (annotationFormat.includes('%')) {
          customOptions.vAxis.format = '#,##0.0\'%\''; 
          customOptions.annotations.textStyle.fontSize = 12;
        } else if (annotationFormat === '#,##0.0') {
          customOptions.vAxis.format = '#,##0.0'; // Garante eixo Y decimal para velocidade
        }
         else {
          customOptions.vAxis.format = '#,###';
        }
        
        const chart = new google.visualization.ColumnChart(el);
        
        // Desenha usando 'data' (que agora está sempre correto)
        chart.draw(data, customOptions);
     }

     // --- FUNÇÃO DO GRÁFICO HORIZONTAL (CORRIGIDA) ---
     function drawHBarChart(elementId, dataArray, options, vAxisTitle, hAxisTitle, annotationFormat = '#,###') {
        const el = document.getElementById(elementId);
        if (!el) { console.error("Elemento não encontrado: " + elementId); return; }

        if (!dataArray || dataArray.length <= 1) {
          el.innerHTML = "<p style='text-align:center; padding-top: 80px; color: #888;'>Não há dados para exibir.</p>";
          el.className = "chart-container-no-data";
          el.style.height = '400px'; 
          return;
        }
        
        // --- CORREÇÃO DE ORDENAÇÃO ---
        // O backend já envia [Maior, Medio, Menor].
        // O BarChart desenha o item 0 (Maior) no TOPO.
        // A linha .reverse() que eu adicionei antes estava errada e foi removida.
        
        // Apenas criamos o dataTable com os dados originais
        const data = google.visualization.arrayToDataTable(dataArray);
        // --- FIM DA CORREÇÃO ---
        
        
        if (annotationFormat) {
          const formatter = new google.visualization.NumberFormat({ pattern: annotationFormat, decimalSymbol: ',', groupingSymbol: '.' });
          formatter.format(data, 1);
        }

        let customOptions = { ...options };
        customOptions.hAxis = { ...customOptions.hAxis, title: hAxisTitle };
        customOptions.vAxis = { ...customOptions.vAxis, title: vAxisTitle };

        // Cálculo de Altura Dinâmica
        // A altura do gráfico DEVE crescer (a barra de rolagem no CSS vai limitá-la)
        const chartHeight = (data.getNumberOfRows() * 40) + 60;
        customOptions.height = Math.max(chartHeight, 400); // Mínimo de 400px

        const chart = new google.visualization.BarChart(el);
        chart.draw(data, customOptions);
     }

     function drawPieChart(elementId, dataArray, options) {
        // ... (função sem mudança) ...
        const el = document.getElementById(elementId);
        if (!el) { console.error("Elemento não encontrado: " + elementId); return; }
        if (!dataArray || dataArray.length <= 1) {
          el.innerHTML = "<p style='text-align:center; padding-top: 80px; color: #888;'>Não há dados para exibir.</p>";
          el.className = "chart-container-no-data";
          return;
        }
        const data = google.visualization.arrayToDataTable(dataArray);
        const chart = new google.visualization.PieChart(el);
        chart.draw(data, options);
     }

     // --- INICIALIZAÇÃO DOS COMPONENTES MATERIALIZE ---
     document.addEventListener('DOMContentLoaded', function() {
        var elems = document.querySelectorAll('.collapsible');
        M.Collapsible.init(elems, { accordion: false });

        // Chama o backend para popular TODOS os filtros
        google.script.run.withSuccessHandler(populateMesSelector).getMesesDisponiveis();
        google.script.run.withSuccessHandler(populateFacaSelector).getListaFacas();
        google.script.run.withSuccessHandler(populateProdutoSelector).getListaProdutos();
     });

     function populateMesSelector(meses) {
        // ... (função sem mudança) ...
        const selector = document.getElementById('filtroMes');
        if (!selector) return;
        meses.sort().forEach(mesKey => {
          const option = document.createElement('option');
          option.value = mesKey;
          option.textContent = formatMesKey(mesKey);
          selector.appendChild(option);
        });
        M.FormSelect.init(selector);
     }

     function populateFacaSelector(facas) {
        // ... (função sem mudança) ...
        const selector = document.getElementById('filtroFaca');
        if (!selector) return;
        facas.forEach(faca => {
          const option = document.createElement('option');
          option.value = faca;
          option.textContent = faca;
          selector.appendChild(option);
        });
        M.FormSelect.init(selector);
     }

     function populateProdutoSelector(produtos) {
        // ... (função sem mudança) ...
        const selector = document.getElementById('filtroProduto');
        if (!selector) return;
        produtos.forEach(produto => {
          const option = document.createElement('option');
          option.value = produto;
          option.textContent = (produto.length > 30) ? produto.substring(0, 27) + '...' : produto;
          selector.appendChild(option);
        });
        M.FormSelect.init(selector);
     }

// ===================================================
// (NOVO) LÓGICA DO DASHBOARD DE CLICHÊS (V1.2 - CORRIGIDA)
// ===================================================

// Formatação de Moeda
const formatCurrency = (num) => {
    return (num || 0).toLocaleString('pt-BR', {
      style: 'currency',
      currency: 'BRL',
      minimumFractionDigits: 2,
      maximumFractionDigits: 2
    });
};

// Variável global para guardar os dados
let dashboardData_Cliches = {};

// Função auxiliar para zerar KPIs no filtro
function _initClicheStats() {
  return {
    costTotal: 0, 
    costNovo: 0, // <-- Adicionado
    avgCostNovo: 0, 
    avgLeadTime: 0, 
    countErroTotal: 0, 
    costErroInterno: 0, 
    costErroClicheria: 0
  };
}

// Função principal de desenho (chamada 1x no onDataLoaded)
function desenharGraficosCliches(data) {
    if (!data || !data.kpis) {
      console.warn("Dados de Clichês não encontrados para desenhar.");
      return;
    }
    dashboardData_Cliches = data; // Salva os dados globalmente
    
    // --- MUDANÇA (V1.1): Opções de Gráficos Corrigidas ---
    
    // Opções de Gráficos
    const pieOptionsCliche = { 
        ...pieOptions, // Herda as opções base de pizza
        width: '100%',
        height: 450,
        chartArea: { width: '85%', height: '80%', left: '7.5%', top: '10%' },
        colors: ['#37417c', '#0047BB', '#5c6bc0', '#f44336', '#D32F2F'],
        legend: { position: 'right', textStyle: { fontSize: 13 } },
        pieSliceText: 'percentage', // Coloca a % DENTRO da fatia
        pieSliceTextStyle: { color: 'white', fontSize: 14, bold: true } // Estilo da %
    };

    const barOptionsCliche = { 
        ...productivityChartOptions, // Herda as opções de barra
        width: '100%', // Garante que caiba no card
        height: 450, 	// Mais altura para os rótulos
        chartArea: { width: '80%', height: '70%', left: '10%', top: '12%' }, // Dá 12% de espaço em cima
        annotations: { 
          alwaysOutside: true, // Força os rótulos para fora
          textStyle: { fontSize: 14, bold: true, color: '#000' } // Rótulos maiores
        }
    };
    
    const errorBarOptions = { 
        ...refugoChartOptions, // Herda as opções de barra de erro
        width: '100%',
        height: 450,
        chartArea: { width: '80%', height: '70%', left: '10%', top: '12%' },
        annotations: { 
          alwaysOutside: true,
          textStyle: { fontSize: 14, bold: true, color: '#000' }
        }
    };
    
    const stackedOptionsCliche = { 
        ...stackedBarChartOptions, // Herda as opções de barra empilhada
        width: '100%', // CORRIGE o estouro (tira o width: 1400)
        height: 450, 	// Mais altura
        chartArea: { width: '80%', height: '70%', left: '10%', top: '12%' }, // Espaço para rótulos
        annotations: { 
          alwaysOutside: false, // Em empilhados, 'false' centraliza o rótulo
          textStyle: { fontSize: 14, bold: true, color: 'white' } // Rótulos brancos
        },
        colors: ['#4CAF50', '#f44336'], // No Prazo, Atrasado
        legend: { position: 'bottom' } 
    };
    
    // --- FIM DAS MUDANÇAS NAS OPÇÕES ---

    
    // Atualiza os KPIs e Gráficos com os dados TOTAIS (anuais)
    updateClichesData("TOTAL");
    
    // Desenha os gráficos que SÓ dependem do total (não mudam com o filtro de mês)
    drawBarChart('chart_cliche_custo_mensal', data.graficos.custoMensal, barOptionsCliche, 'Mês', 'Custo Total (R$)', '#,###');
    drawBarChart('chart_cliche_contagem_mensal', data.graficos.contagemMensal, barOptionsCliche, 'Mês', 'Qtd. Clichês Novos', '#,###');
    drawPieChart('chart_cliche_custo_tipo', data.graficos.custoPorTipo, pieOptionsCliche);
    drawBarChart('chart_cliche_erros_internos_mensal', data.graficos.errosInternosMensal, errorBarOptions, 'Mês', 'Qtd. Erros Internos', '#,###');
    drawBarChart('chart_cliche_prazos_mensal', data.graficos.prazosMensal, stackedOptionsCliche, 'Mês', 'Qtd. Pedidos', '#,###');
    
    // (NOVO) Desenha os gráficos de CUSTO de erro
    drawBarChart('chart_cliche_custo_erro_interno_mensal', data.graficos.custoErroInternoMensal, errorBarOptions, 'Mês', 'Custo Erro Interno (R$)', '#,###');
    drawBarChart('chart_cliche_custo_erro_clicheria_mensal', data.graficos.custoErroClicheriaMensal, errorBarOptions, 'Mês', 'Custo Erro Clicheria (R$)', '#,###');
}

// Função de FILTRO (chamada pelo handleFilterChange)
function updateClichesData(mesKey) {
    if (!dashboardData_Cliches || !dashboardData_Cliches.kpis) return;
    
    let kpis, graficos;
    
    if (mesKey === "TOTAL") {
        kpis = dashboardData_Cliches.kpis;
        graficos = dashboardData_Cliches.graficos;
    } else {
        if (!dashboardData_Cliches.dadosMensais || !dashboardData_Cliches.dadosMensais[mesKey]) {
          // Se não há dados para o mês, zera
          kpis = _initClicheStats(); // Usa a função de zerar
          graficos = { custoPorClicheria: [], errosPorClicheria: [], prazosPorClicheria: [] };
        } else {
          kpis = dashboardData_Cliches.dadosMensais[mesKey].kpis;
          graficos = dashboardData_Cliches.dadosMensais[mesKey].graficos;
        }
    }
    
    // 1. Atualiza os KPIs
    document.getElementById('kpi_cliche_custo_total').innerHTML = formatCurrency(kpis.costTotal);
    // (MUDANÇA V1.1) Troca Custo Médio por Custo Total
    document.getElementById('kpi_cliche_custo_novo').innerHTML = formatCurrency(kpis.costNovo); 
    document.getElementById('kpi_cliche_avg_lead_time').innerHTML = `${formatNumber(kpis.avgLeadTime, 1)} <span class="unit">dias</span>`;
    document.getElementById('kpi_cliche_count_erros').innerHTML = formatNumber(kpis.countErroTotal, 0);

    // (MUDANÇA V1.1): Popula os novos KPIs de Custo de Erro
    document.getElementById('kpi_cliche_custo_erro_interno').innerHTML = formatCurrency(kpis.costErroInterno);
    document.getElementById('kpi_cliche_custo_erro_clicheria').innerHTML = formatCurrency(kpis.costErroClicheria);

    
    // 2. Redesenha os gráficos que mudam com o mês
    
    // (MUDANÇA V1.1): Usa as novas opções corrigidas
    const barOptionsCliche = { 
        ...productivityChartOptions, 
        width: '100%', height: 450, 
        chartArea: { width: '80%', height: '70%', left: '10%', top: '12%' },
        annotations: { alwaysOutside: true, textStyle: { fontSize: 14, bold: true, color: '#000' } }
    };
    const errorBarOptions = { 
        ...refugoChartOptions, 
        width: '100%', height: 450, 
        chartArea: { width: '80%', height: '70%', left: '10%', top: '12%' },
        annotations: { alwaysOutside: true, textStyle: { fontSize: 14, bold: true, color: '#000' } }
    };
    const stackedOptionsCliche = { 
        ...stackedBarChartOptions, 
        width: '100%', height: 450, 
        chartArea: { width: '80%', height: '70%', left: '10%', top: '12%' },
        annotations: { alwaysOutside: false, textStyle: { fontSize: 14, bold: true, color: 'white' } },
        colors: ['#4CAF50', '#f44336'], 
        legend: { position: 'bottom' } 
    };
    // --- FIM DA MUDANÇA ---


    drawBarChart('chart_cliche_custo_clicheria', graficos.custoPorClicheria, barOptionsCliche, 'Clicheria', 'Custo Total (R$)', '#,###');
    drawBarChart('chart_cliche_erros_clicheria', graficos.errosPorClicheria, errorBarOptions, 'Clicheria', 'Qtd. Erros', '#,###');
    drawBarChart('chart_cliche_prazos_clicheria', graficos.prazosPorClicheria, stackedOptionsCliche, 'Clicheria', 'Qtd. Pedidos', '#,###');
}

</script>
</body>
</html>
